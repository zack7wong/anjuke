<script src="public/js/myCommon.js"></script>

<script src="public/js/slick.min.js"></script>
<script>
    $(function(){
        //合作伙伴切换
        $('.ban-item,.i-newsitem').css('display','block');
        $('.banner,.i-news').slick({
            arrows:false,
            dots:true,
            infinite: true,
            speed: 500,
            fade: true,
            cssEase: 'linear',
            slidesToShow: 1,
            slidesToScroll: 1,
            autoplay: true,
            autoplaySpeed: 2000
			
        });
       //动态新闻切换
        $('.i-tab li').each(function(i,v){
            $(this).hover(function(){
                $(this).addClass('active').siblings().removeClass('active');
                $('.i-dynamicCont .i-dynamic').eq(i).show().siblings().hide();
            })
        })
        var auto = 1;  //等于1则自动切换，其他任意数字则不自动切换
        if(auto ==1){
            var number = 0;
            var maxNumber = $('.i-tab li').length;
            function autotab(){
                number++;
                number == maxNumber? number = 0 : number;
                $('.i-tab li:eq('+number+')').addClass('active').siblings().removeClass('active');
                $('.i-dynamicCont .i-dynamic:eq('+number+')').show().siblings().hide();
            }
            var tabChange = setInterval(autotab,3000);
            //鼠标悬停暂停切换
            $('.i-dynamicwrap').mouseover(function(){
                clearInterval(tabChange);
            });
            $('.i-dynamicwrap').mouseout(function(){
                tabChange = setInterval(autotab,3000);
            });
        }
        //专题网站
       $('.i-themetab').slick({
		    arrows:true,
		    slidesToShow: 4,
		    slidesToScroll: 1,
		    autoplay: true,
		    autoplaySpeed: 3000,
		});
    })
</script>
</body>
</html>