<script type="text/javascript">
    setTimeout(function(){
        scrollHeight();
        $(window).scroll(function(){
            scrollHeight();
        })
    },100)
    function scrollHeight(){
        var winH = $(window).height();
        var scrollTop = $(window).scrollTop();
        if(scrollTop >= winH){
            $('.backTop').fadeIn();
        }else{
            $('.backTop').fadeOut();
        }
    }
    function backTop(){
        $('body,html').animate({'scrollTop':'0px'},1000);
    }
</script>
<script>
    setTimeout(function(){
        scrollHeight();
        $(window).scroll(function(){
            scrollHeight();
        })
    },100)
    function scrollHeight(){
        var winH = $(window).height();
        var scrollTop = $(window).scrollTop();
        if(scrollTop >= winH){
            $('.backTop').fadeIn();
        }else{
            $('.backTop').fadeOut();
        }
    }
    function backTop(){
        $('body,html').animate({'scrollTop':'0px'},1000);
    }
</script>
