var week=["星期日","星期一","星期二","星期三","星期四","星期五","星期六"];
var testDate = new Date(); 
function getNowFormatDate() 
{ 
var day = new Date(); 
var Year = 0; 
var Month = 0; 
var Day = 0; 
var CurrentDate = ""; 
//初始化时间 
//Year= day.getYear();//有火狐下2008年显示108的bug 
Year= day.getFullYear();//ie火狐下都可以 
Month= day.getMonth()+1; 
Day = day.getDate(); 
//Hour = day.getHours(); 
// Minute = day.getMinutes(); 
// Second = day.getSeconds(); 
CurrentDate += Year + "年"; 
if (Month >= 10 ) 
{ 
CurrentDate += Month + "月"; 
} 
else 
{ 
CurrentDate += "0" + Month + "月"; 
} 
if (Day >= 10 ) 
{ 
CurrentDate += Day ; 
} 
else 
{ 
CurrentDate += "0" + Day ; 
} 
return CurrentDate+"日"; 
}
document.write(getNowFormatDate()+" "+week[testDate .getDay()]);
