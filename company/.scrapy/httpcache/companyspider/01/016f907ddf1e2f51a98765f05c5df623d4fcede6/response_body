                        <script>
                            var showNum1 = document.getElementById("num1");
                            //首屏焦点图
                            var m1 = showNum1.getElementsByTagName("li").length; //图片个数
                            var n1 = 0;
                            var showNum1 = document.getElementById("num1");
                            function Mea1(value) {
                                n1 = value;
                                setBg1(value);
                                plays1(value);
                                cons1(value);
                            }
                            function setBg1(value) {
                                for (var i = 0; i < m1; i++) if (value == i) {
                                    showNum1.getElementsByTagName("li")[i].className = 'on';
                                } else {
                                    showNum1.getElementsByTagName("li")[i].className = '';
                                }
                            }
                            function plays1(value) {
                                try {
                                    with(foucs1) {
                                        filters[0].Apply();
                                        for (i = 0; i < m1; i++) {
                                            i == value ? children[i].style.display = "block": children[i].style.display = "none";
                                        }
                                        filters[0].play();
                                    }
                                } catch(e) {
                                    var divlist1 = document.getElementById("foucs1").getElementsByTagName("li");
                                    for (i = 0; i < m1; i++) {
                                        i == value ? divlist1[i].style.display = "block": divlist1[i].style.display = "none";
                                    }
                                }
                            }
                            function cons1(value) {
                                try {
                                    with(con1) {
                                        for (i = 0; i < m1; i++) i == value ? children[i].style.display = "block": children[i].style.display = "none";
                                    }
                                } catch(e) {
                                    var titList1 = document.getElementById("con1").getElementsByTagName("li");
                                    for (i = 0; i < m1; i++) {
                                        i == value ? titList1[i].style.display = "block": titList1[i].style.display = "none";
                                    }
                                }
                            }
                            function clearAuto1() {
                                clearInterval(autoStart1);
                            }
                            function setAuto1() {
                                autoStart1 = setInterval("auto1(n1)", 3000);
                            }
                            function auto1() {
                                n1++;
                                if (n1 > m1 - 1) n1 = 0;
                                Mea1(n1);
                            }
                            setAuto1();
                        </script>
                    </div>
                </div>
