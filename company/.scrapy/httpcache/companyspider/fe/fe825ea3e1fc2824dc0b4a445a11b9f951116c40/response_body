            <script>
     $(function(){
      $(".li_div").each(function(i, target){
        $(target).mouseenter(function(e){
            //stop current animation
            $(target).stop();
            $(target).find(".con_imgname").stop(false, true);
            $(target).parent().addClass("cur");
            $(".li_div").not($(target)).addClass("not_cur");
            $(target).find(".con_imgname").animate({bottom:"1px"}, "normal");
            $(target).animate({width:"156px", height:"91px",top:"-17px",left:"-25px"},"normal");
        });
        $(target).mouseleave(function(e){
            $(target).stop();
            $(target).find(".con_imgname").stop(false, true);
            $(target).parent().removeClass("cur");
            $(".li_div").not($(target)).removeClass("not_cur");
            $(target).find(".con_imgname").animate({bottom:"-27px"}, "normal");
            $(target).animate({ width:"106px", height:"62px", top:"0", left:"2px"}, "normal");
        });
    });
});
</script>
