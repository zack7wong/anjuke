	function StronClickProGa(sid)
	{
		for (var i=1;i<6;i++)
		{
			if(parseInt(sid)==parseInt(i))
			{
				document.getElementById("SlMc"+i).style.display="block";
				document.getElementById("Sl"+i).className="Out";
			}
			else
			{
				document.getElementById("SlMc"+i).style.display="none";
				document.getElementById("Sl"+i).className="Over";
			}
		}
	}
