        	String.prototype.trim = function(){
				    return this.replace(/(^\s*)|(\s*$)/g, "");    
				  }  
			function mySearch(){
				var key =document.getElementById("keyword").value.trim();
				if(key.length <=0){
					alert("关键字必填");
				}else{
					/* var url = appPath + "lwgl/common/result!allSearch.action?keyword="+key+"&lwSiteId=402881e65964160a0159641b08770002"; */
					var url = appPath + "lwgl/common/result!allSearch.action?keyword="+key; 
					url = encodeURI(encodeURI(url));
					window.open(url);
				}
			}
	 	</script>

