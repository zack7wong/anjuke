<script type="text/JavaScript">
	var baseUrl;
	function writeBase() {
		var theUrl = location.href;
		baseUrl = theUrl;
		var siteStr = "/www2/www.fjsdxy.com";
 		var indexNum = theUrl.indexOf(siteStr);
	 	if (indexNum > 0) { // 如果从后台预览进入
	 	baseUrl = theUrl.substring(0, indexNum + siteStr.length) + "/"; // 最后一个斜杠很重要
		 }else {
	 	indexNum = theUrl.indexOf("/", 7);  	// 7 = "http://".length
	 	baseUrl = theUrl.substring(0, indexNum) + "/"; // 最后一个斜杠很重要
	 }
 		document.writeln("<base href=\"" + baseUrl + "\"></base>");
	}
 	writeBase();
</script>
