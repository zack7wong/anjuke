	<div style="display:none;" id="logoutContainer" class="nsy-login-islogin">
		<p>
			您好,<span id="pusername" class="nsy-login-loginname"></span>,登录成功!
		</p>
<a href="http://218.75.105.28:8888/office" target="_blank">办公中心</a> <a onclick="ppllogout()">安全退出</a> 
	</div>
<script type="text/javascript" src="../Js/passport_v130609.js"></script>
<script type="text/javascript" src="../Js/md5.js"></script>
<script type="text/javascript" src="../Js/sha1.js"></script>
<script language="javascript" type="text/javascript">
var p_passporturl="http://218.75.105.28:8888/passport";
var p_officeurl="http://218.75.105.28:8888/office";
var p_memberurl="http://218.75.105.28:8888/member";
var p_serverid="153301";
var p_weburl="http://www.hzyhzhx.com";
var p_root="1";
var p_username="";
function ppllogin()
{
     var username = ppltrim(pplgettextvalue('passportname', null)); // 用户名
     var password = ppltrim(pplgettextvalue('passportpwd', ''));// 密码
     var verifycode = ppltrim(pplgettextvalue('verifyCode', ''));// 验证码
     var auto_login = pplgetcheckvalue('auto_login', false);// 自动登录
     if(username == '' || password == '')
     {
        processError('请输入用户名和密码!');                    
     }
     else
     {
        passportset(username, password, verifycode, auto_login);
     }
}
//发起登录请求
function passportset(username/*用户名*/, password/*密码*/, verifycode/*验证码*/, auto_login/*是否自动登录*/)
{
   var src = p_passporturl + "/scriptLogin?action=login" +
                    "&username=" + encodeURIComponent(username) +
                    "&server=" + p_serverid +
                    "&root=" + p_root +
                    "&multiAccount=1" +
                    "&cookieSaveType=" + (auto_login ? "3" : "2") +
                    "&password=" + hex_md5(password) + hex_sha1(password) +
                    "&verifyCode=" + verifycode +
                    "&charset=utf-8" +
                    "&dynamicVerifyURL=0" +
                    "&url=" + encodeURIComponent(p_weburl) +
                    "&v=" + new Date().getTime();
    var script = document.createElement("script");
    script.type = "text/javascript";
    script.language = "javascript";
    script.src = src;
    pplsetcookie("__redirect", "true");
    //如果选择了自动登录,则保存相应值和用户名密码
    if (auto_login) {
        pplsetcookie('__userName', username, new Date("June 3, 2020"));
        pplsetcookie('__password', password, new Date("June 3, 2020"));
    }
    document.body.appendChild(script);
}
//登录成功
function pp_init() {  
    var __redirect = pplgetcookie("__redirect");   
    if (__redirect == "true") {   
        pplclearcookie("__redirect");   
        if (account.fixedType == 1 || account.fixedType == 3 || (account.fixedType == 2 && !hasCheckServerType["5"])) 
        {   
            //登录是否自动跳转
            //parent.location.href = p_officeurl;
        }   
        else if (hasCheckServerType["5"]) {   
            //登录是否自动跳转
            //parent.location.href = p_officeurl;
        }
    }
    var logincontainer = document.getElementById("loginContainer"); 
    var logoutcontainer = document.getElementById("logoutContainer"); 
    logincontainer.style.display = "none";
    logoutcontainer.style.display = ""; 
    var pplusername = document.getElementById("pusername");
    pplusername.innerHTML=""+p_username+"";
}
//安全退出
function ppllogout() {
    var auto_login = pplgetcookie('__redirect');
    if (auto_login) {
        pplclearcookie('__redirect');
        pplclearcookie('__userName');
        pplclearcookie('__password');
    }
    location.href = "http://218.75.105.28:8888/member/user/logout.htm?url=" + encodeURIComponent(location.href);
}
    //提示错误信息
    function processError(loginError) {  
        if (loginError) {   
            loginError = loginError.toString();   
            var index = loginError.indexOf(':');   
            var errorCode = loginError.substr(0, index);   
            // 用户名不存在   
            if (errorCode == '301') {   
            //debugger;  
            } 
            loginError = loginError.substr(index + 1, loginError.length - index - 1);   
            alert(loginError);
        }  
    }
</script>
<script language="javascript" type="text/javascript">
pplcheck();
</script>
<script language="javascript" type="text/javascript">
ppllogininit();
</script>
</div>
