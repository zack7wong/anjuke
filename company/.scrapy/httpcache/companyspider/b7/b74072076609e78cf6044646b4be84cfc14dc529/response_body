    function byId(id){return document.getElementById(id)};
	/*********箭头控制滚动方向、加速及鼠标拖动***************/
	var MarqueeDiv2Control=new Marquee('scrolldemo_ctl20');		//箭头控制滚动方向、加速及鼠标拖动实例
	MarqueeDiv2Control.Direction="left";
	MarqueeDiv2Control.Step=1;
	MarqueeDiv2Control.Width=820;
	MarqueeDiv2Control.Timer=20;
	MarqueeDiv2Control.ScrollStep=1;				//若为-1则禁止鼠标控制实例
	MarqueeDiv2Control.Start();
	MarqueeDiv2Control.BakStep=MarqueeDiv2Control.Step;
	byId('LeftButton_ctl20').onmouseover=function(){MarqueeDiv2Control.Direction=2};
	byId('LeftButton_ctl20').onmouseout=byId('LeftButton_ctl20').onmouseup=function(){MarqueeDiv2Control.Step=MarqueeDiv2Control.BakStep};
	byId('LeftButton_ctl20').onmousedown=byId('RightButton_ctl20').onmousedown=function(){MarqueeDiv2Control.Step=MarqueeDiv2Control.BakStep+3};
	byId('RightButton_ctl20').onmouseover=function(){MarqueeDiv2Control.Direction=3};
	byId('RightButton_ctl20').onmouseout=byId('RightButton_ctl20').onmouseup=function(){MarqueeDiv2Control.Step=MarqueeDiv2Control.BakStep};
	

