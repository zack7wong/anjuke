<script language="javascript">
    function openWindow(Address)
    {
        if(Address!="")
        {
            if(Address.indexOf("index")==0)
                location.href=Address;
            else
                window.open(Address);
        }
    }

    
    function ClearMenu()
    {
        for ( var i = 0 ; i < menuItem.length ; i++)
        {
            ClearMenuLastTwoItem ( menuItem[i] );
        }
    }
    
    
    function ClearMenuLastTwoItem(item)
    {
        var obj = document.getElementById( "cswmPopup" + item.toString() );
        if ( !obj ) {return false ;}
        var itemTable = obj.childNodes[0].rows[0].cells[0].childNodes[0].childNodes[0].childNodes[0];
        if ( !itemTable ) {return false; }
        var itemCount = itemTable.rows.length;
        itemTable.deleteRow( itemCount - 1);
        itemTable.deleteRow( itemCount - 2);
    }

    var menuItem = [];
</script>

