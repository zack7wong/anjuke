var speed=20//速度数值越大速度越慢
qdemo2.innerHTML=qdemo1.innerHTML
function Marquee(){
if(qdemo2.offsetWidth-qdemo.scrollLeft<=0)
qdemo.scrollLeft-=qdemo1.offsetWidth
else{
qdemo.scrollLeft++
}
}
var MyMar=setInterval(Marquee,speed)
qdemo.onmouseover=function() {clearInterval(MyMar)}
qdemo.onmouseout=function() {MyMar=setInterval(Marquee,speed)}
