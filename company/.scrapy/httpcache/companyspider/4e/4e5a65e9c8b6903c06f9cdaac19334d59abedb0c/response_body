	function flinkFunc(){
		var obj=document.getElementById("myURL");
		var varURL=obj.options[obj.options.selectedIndex].value;
		window.open(varURL);
	}
	function changeZS(){
		document.getElementById("zhaosheng").style.display="";
		document.getElementById("meishu").style.display="none";
	};

	function changeMS(){
		document.getElementById("zhaosheng").style.display="none";
		document.getElementById("meishu").style.display="";
		document.getElementById("zsa").className="spa";
		document.getElementById("msa").className="";
	};
	
