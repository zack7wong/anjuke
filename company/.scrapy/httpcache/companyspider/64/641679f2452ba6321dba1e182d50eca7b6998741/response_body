<script type="text/javascript">
(function () {
var num = $("#jsNav_2940 .imageshow li").length,
now = 0;
$("#jsNav_2940 .imageshow li:first").css({"z-index":"2","opacity":"1"});
$("#jsNav_2940 .btnimage li:first").addClass("over");

function next() {
now = (++now) % num;
return now;
}
function callback() {
$("#jsNav_2940 .btnimage").find('li').removeClass('over');
$("#jsNav_2940 .imageshow").find('li').css('z-index', '1');
$("#jsNav_2940 .btnimage").find('li:eq(' + now + ')').addClass('over');
$("#jsNav_2940 .imageshow").find('li:eq(' + now + ')').css('z-index', '2');
};

function animagea() {
$("#jsNav_2940 .imageshow").find('li:eq(' + now + ')').animate({opacity : 0}, 1500);
$("#jsNav_2940 .imageshow").find('li:eq(' + next() + ')').animate({	opacity : 1	}, 1500, callback);
}
var timer =  setInterval(animagea, 5000);

$("#jsNav_2940 .btnimage li").hover(
function(){
clearInterval(timer);
$("#jsNav_2940 .btnimage").find('li').removeClass('over');
$(this).addClass('over');
$("#jsNav_2940 .imageshow").find('li:eq(' + now + ')').animate({opacity : 0,'z-index':1}, 500);
now = $("#jsNav_2940 .btnimage li").index($(this)); 
$("#jsNav_2940 .imageshow").find('li:eq(' + now + ')').animate({	opacity : 1,'z-index':2	}, 500);

},										  
function(){
timer =  setInterval(animagea, 5000);
}										  
);
})();
