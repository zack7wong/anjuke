<script language="javascript" >
(function(){
var len = $("#idSlider_2938 > li").length;
var index = 0;
for(ii=1;ii<=len;ii++){
$('#idNum_2938').append('<li>'+ii+'</li>');
}
$("#idNum_2938 li").mouseover(function(){
index =   $("#idNum_2938 li").index(this);
showImg(index);
}); 
$('#idTransformView_2938').hover(function(){
if(MyTime){
clearInterval(MyTime);
}
},function(){
MyTime = setInterval(function(){
showImg(index)
index++;
if(index==len){index=0;}
} , 4000);
});
var MyTime = setInterval(function(){
showImg(index);
index++;
if(index==len){index=0;}
} , 4000);
function showImg(i){
$("#idSlider_2938").stop(true,false).animate({top : -349*i},800);
$("#idNum_2938 li")
.eq(i).addClass("on")
.siblings().removeClass("on");
}

})();
</script></div>
