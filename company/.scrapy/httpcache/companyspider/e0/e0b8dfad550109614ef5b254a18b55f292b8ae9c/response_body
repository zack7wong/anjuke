	$('#dh-box-tab #dh-title span').hover(function(){
			$('#dh-box-tab #dh-title span').removeClass('select');
			$(this).addClass('select');
			$('#dh-box-tab #tab-info ul').removeClass('on');
			$('#dh-box-tab #tab-info ul').eq($('#dh-box-tab #dh-title span').index(this)).addClass('on')
	});
});
</script>
