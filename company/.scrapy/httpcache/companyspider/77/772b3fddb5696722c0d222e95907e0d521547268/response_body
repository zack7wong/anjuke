        <!--div id="floatDiv" style=" z-index:10000;position:fixed;">
        <img src="images/kfr.png" style="cursor: pointer;" onclick="window.location.href='http://www.hecz.net/portal/KFRDJ.aspx'" /></div>

    <script type="text/javascript">
        var step = 1; // 移动的像素
        var y = -1; // 垂直移动的方向，-1表示向上，1表示向下
        var x = 1; // 水平移动的方向，-1表示向左，1表示向右
        function myFloat() {
            var img = document.getElementById("floatDiv");
            // 获取图片和当前浏览器窗口上边距，由于img.style.top获取的值带px单位
            var top = img.style.top.replace("px", "");
            // 获取图片和当前浏览器窗口左边距
            var left = img.style.left.replace("px", "");
            // 上下移动
            if (top <= 0) {
                y = 1;
            }
            if (top >= window.screen.height) {
                y = -1;
            }
            top = (top * 1) + (step * y);
            img.style.top = top + "px";
            // 左右移动
            if (left <= 0) {
                x = 1;
            }
            if (left >= (window.screen.width - img.clientWidth)) {
                x = -1;
            }
            left = (left * 1) + (step * x);
            img.style.left = left + "px";
            setTimeout("myFloat()", 20);
        }

        myFloat();
</script-->  


            

