    $('.videoInfo ul')
    $(function () {
        $('#videoInfo ul').click(function () {
            $(this).css("background-color", "#000");
        });
    });

    function showVideo(mp4File, wmvFile) {
        var html = "\
            <video width='352' height='196' class='mejs-wmp' controls='controls' autoplay='autoplay' preload='auto'>\
            <source type='video/mp4' src='video/mp4File' />\
           <embed mediatype='0' type='application/x-mplayer2' pluginspage='http://microsoft.com/windows/mediaplayer/en/download/' src='video/wmvFile' autostart='true' enabled='true' enablecontextmenu='true' fullscreen='false' invokeurls='true' mute='false' stretchtofit='true' windowlessvideo='true' width='352' height='196' palette='black|transparent'></embed>\
</video>\
            ";
        html = html.replace(/mp4File/g, mp4File).replace(/wmvFile/g, wmvFile);
        $('#videoInfo').html(html);

        if (!($.browser.msie && ($.browser.version == '6.0' || $.browser.version == '7.0' || $.browser.version == '8.0'))) {
            $('audio,video').mediaelementplayer({
                success: function (player, node) {
                    $('#' + node.id + '-mode').html('mode: ' + player.pluginType);
                }
            });
        }
    }
    

</script>
