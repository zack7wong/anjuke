 <div id="float_icon" style="Z-INDEX: 99999;position:absolute;visibility: visible;" >

     

  </div>
<script type="text/javascript">
    var ggRoll = {
        roll: document.getElementById("float_icon"),
        speed: 30,
        statusX: 1,
        statusY: 1,
        x: 100,
        y: 300,
        winW: document.documentElement.clientWidth - document.getElementById("float_icon").offsetWidth,
        winH: document.documentElement.clientHeight - document.getElementById("float_icon").offsetHeight,
        Go: function () {
            this.roll.style.left = this.x + 'px';
            this.roll.style.top = this.y + 'px';

            this.x = this.x + (this.statusX ? -1 : 1)
            if (this.x < 0) { this.statusX = 0 }
            if (this.x > this.winW) { this.statusX = 1 }

            this.y = this.y + (this.statusY ? -1 : 1)
            if (this.y < 0) { this.statusY = 0 }
            if (this.y > this.winH) { this.statusY = 1 }
        }
    }
    var interval = setInterval("ggRoll.Go()", ggRoll.speed);
    ggRoll.roll.onmouseover = function () { clearInterval(interval) };
    ggRoll.roll.onmouseout = function () { interval = setInterval("ggRoll.Go()", ggRoll.speed) };
 </script>  
<!--浮动窗口End-->

