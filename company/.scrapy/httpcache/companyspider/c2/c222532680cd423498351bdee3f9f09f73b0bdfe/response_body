				<script type="text/javascript">
						function login_valida(){
						    var txtName = document.getElementById('login_txtName');
						    var txtPwd = document.getElementById('login_txtPwd');
							if(txtName.value.replace(/s/g,'')==''){
								alert("请输入用户名");
								txtName.focus();
								return false;
							}
							if(txtPwd.value.replace(/s/g,'')==''){
								alert("请输入密码");
								txtPwd.focus();
								return false;
							}
						    return true;
						}
					</script>
					<form action="/User/login" method="post" onsubmit="return login_valida();">
