<script type="text/javascript" language="javascript">
/**
 * for menu-drop type
 */
var type = "slide";
if (type == 'click') {
	$(function(){
		$('#pro_type_5Rbbfh li:has(ul)').click(function(event){
	    	if (this == event.target) {
	        	if ($(this).children().is(':hidden')) {
	                $(this).css({background:'url(template/gszc-110402-a47/images/minus.gif) no-repeat left 13px','text-indent':'16px','padding-top':'3px'})
	                .children().show();
	                
	              	$(this).siblings().each(function (){
	              		$(this).find("ul").hide();
	              		if ($(this).children().is("ul")){
	              			$(this).css({background:'url(template/gszc-110402-a47/images/plus.gif) no-repeat left 13px','text-indent':'16px','padding-top':'3px'});
	              	    }
	              	});
	            } else {
	                $(this).css({background:'url(template/gszc-110402-a47/images/plus.gif) no-repeat left 13px','text-indent':'16px','padding-top':'3px'})
	                .find("ul").hide();
	            }  
	        }
		}).css('cursor','pointer');
	          
	    $('#pro_type_5Rbbfh li:has(ul)').css({background:'url(template/gszc-110402-a47/images/plus.gif) no-repeat left 13px','text-indent':'16px','padding-top':'3px'});
	        	
	    $('#pro_type_5Rbbfh li:not(:has(ul))').css({cursor: 'pointer','list-style-image':'none'});
	});
} else {
	$("#pro_type_5Rbbfh > ul").droppy();
	$("#pro_type_5Rbbfh ul ul li:last-child").css("border","0px");
}
</script>

