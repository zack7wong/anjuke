function operate_logo(dis,id){
	if(dis==0){//点击传过来是0，则表示已经隐藏，现在应该做显示处理
		$.post("index.php?_m=mod_media&_a=operate_logo",{id:id,display:dis},function(data){
			reloadPage();
		})	
		
	}else{
		$.post("index.php?_m=mod_media&_a=operate_logo",{id:id,display:dis},function(data){
			reloadPage();
		});
	}
}
function operate_banner(dis,id,url){
	if(dis==0){//点击传过来是0，则表示已经隐藏，现在应该做显示处理
