    var timeout     = 500;
    var closetimer  = 0;
    var ddmenuitem  = 0;

    // open hidden layer
    function mopen(id)
    {	
	    // cancel close timer
	    mcancelclosetime();

	    // close old layer
	    if(ddmenuitem) ddmenuitem.style.visibility = 'hidden';

	    // get new layer and show it
	    ddmenuitem = document.getElementById(id);
	    ddmenuitem.style.visibility = 'visible';

    }
    // close showed layer
    function mclose()
    {
	    if(ddmenuitem) ddmenuitem.style.visibility = 'hidden';
    }

    // go close timer
    function mclosetime()
    {
	    closetimer = window.setTimeout(mclose, timeout);
    }

    // cancel close timer
    function mcancelclosetime()
    {
	    if(closetimer)
	    {
		    window.clearTimeout(closetimer);
		    closetimer = null;
	    }
    }

    // close layer when click-out
    document.onclick = mclose; 
    // -->

    <!--加入搜藏-->

    function show_confirm()
    {
        var r=confirm("是否加入收藏");
        if (r==true)
        {
            alert("已添加");
        }
    }

    </script>

    <!--列表带有new样式-->

    <script type="text/javascript">
        function isnew(maxdays, addDate) {
            var oldDate = new Date(addDate);
            var newDate = new Date();
            var maxdaysold = maxdays * 24 * 60 * 60 * 1000;
            if ((newDate.getTime() - oldDate.getTime()) <= maxdaysold) {
                return true;
            }
            return false;
        }

        window.onload = function bb() {
            var ts = document.getElementById("zxxw1").getElementsByTagName("table");
            for (var i = 0; i < ts.length; i++) {
                var t = ts[i];
                if (t.className == "table") {
                    var tdimg = ts[i].rows[0].cells[1];
                    var tempdate = ts[i].rows[0].cells[2].innerText.replace("-", "/");
                    if (isnew(3, tempdate)) {
                        tdimg.innerHTML += '<img src="/Templets/Gxljweb/images/News.gif"/>';
                    }
                }
            }
            var ts = document.getElementById("zxxw2").getElementsByTagName("table");
            for (var i = 0; i < ts.length; i++) {
                var t = ts[i];
                if (t.className == "table") {
                    var tdimg = ts[i].rows[0].cells[1];
                    var tempdate = ts[i].rows[0].cells[2].innerText.replace("-", "/");
                    if (isnew(3, tempdate)) {
                        tdimg.innerHTML += '<img src="/Templets/Gxljweb/images/News.gif"/>';
                    }
                }
            }
        };
    </script>

