function info_notice() {
document.getElementById("info_notice").style.display = "";
setTimeout('leavePage()', 1000);
}
function cancel_info_notice() {
document.getElementById("info_notice").style.display = "none";
}
</script>
<script language="JavaScript">
var intLeft = 30; 

function leavePage() {
if (0 == intLeft)
cancel_info_notice();
else {

intLeft -= 1;
document.all.countdown.innerText = intLeft + " ";
setTimeout("leavePage()", 1000);
}
}
</script>

