<SCRIPT language=JavaScript>

/*在<body>下一行处执行*/
function Page_BodyBegin()
{
    DobAdv_Init();
}

/**********页面全局事件调用－结束**********/



/**********对联广告代码－开始**********/
var oDobAdv_Left = oDobAdv_Right = null;
var oDobAdv_W = 78;
var oDobAdv_h = 678;
var oDobAdv_R_Top = oDobAdv_L_Top = 100;
var oDobAdv_L_margin = 28;
var oDobAdv_R_margin = 20;

DobAdv_Init();

function DobAdv_Init()
{
    //对联广告
    var oDobAdv_L_Src = "l.jpg";
    var oDobAdv_R_Src = "r.jpg";
    var oDobAdv_L_Link = "http://www.gdzqzx.cn";
    var oDobAdv_R_Link = "http://www.gdzqzx.cn";

    document.write('<div id="left_173DobAdv" style="position:absolute;width:' + oDobAdv_W + 'px;height:' + oDobAdv_h + 'px;z-index:100;left:' + oDobAdv_L_margin + 'px;top:' + oDobAdv_L_Top + 'px;overflow:hidden;visibility:hidden;" onfocus="this.blur()"><img src="' + oDobAdv_L_Src + '" border="0" usemap="#Map_left_173DobAdv"><map name="Map_left_173DobAdv"> <area shape="rect" coords="0,0,106,17"  href="javascript:DobAdv_Show(false);void(0);" onfocus="this.blur()"><area shape="rect" coords="0,1,103,754" href="' + oDobAdv_L_Link + '" target="_blank" onfocus="this.blur()"></map></div>');
    document.write('<div id="right_173DobAdv" style="position:absolute;width:' + oDobAdv_W + 'px;height:' + oDobAdv_h + 'px;z-index:100;left:' + oDobAdv_R_margin + 'px;top:' + oDobAdv_R_Top + 'px;overflow:hidden;visibility:hidden;" onfocus="this.blur()"><img src="' + oDobAdv_R_Src + '" border="0" usemap="#Map_right_173DobAdv"><map name="Map_right_173DobAdv"> <area shape="rect" coords="0,0,106,17"  href="javascript:DobAdv_Show(false);void(0);" onfocus="this.blur()"><area shape="rect" coords="0,1,103,754" href="' + oDobAdv_R_Link + '" target="_blank" onfocus="this.blur()"></map></div>');

    oDobAdv_Left = document.getElementById("left_173DobAdv");
    oDobAdv_Right = document.getElementById("right_173DobAdv");

    DobAdv_Show(true);
    DobAdv_Resize();
	
	if(screen.width<=800)
	{
	DobAdv_Show(false);
	}
}

function DobAdv_Show(s)
{
    oDobAdv_Left.style.visibility = oDobAdv_Right.style.visibility = s?"visible":"hidden";
}

function DobAdv_Resize()
{
    if(!oDobAdv_Left || !oDobAdv_Right) return;

    var ClientW = document.body.clientWidth;
    oDobAdv_Left.style.left = oDobAdv_L_margin;
    oDobAdv_Right.style.left = ClientW - oDobAdv_W - oDobAdv_R_margin;

    setTimeout("DobAdv_Resize()", 600);
}                                          
/**********对联广告代码－结束**********/
</SCRIPT>

