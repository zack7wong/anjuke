 <script>
    var speed=30  
    var demoleft735=document.getElementById("demoleft735");
    var demoleft7352=document.getElementById("demoleft7352");
    var demoleft7351=document.getElementById("demoleft7351");
    demoleft7352.innerHTML=demoleft7351.innerHTML
    function Marquee(){
    if(demoleft7352.offsetWidth-demoleft735.scrollLeft<=0)
    demoleft735.scrollLeft-=demoleft7351.offsetWidth
    else{
    demoleft735.scrollLeft++
    }
    }
    var MyMar=setInterval(Marquee,speed)
    demoleft735.onmouseover=function() {clearInterval(MyMar)}
    demoleft735.onmouseout=function() {MyMar=setInterval(Marquee,speed)}
    </script>

