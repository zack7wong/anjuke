<script> 
var speed=10
demo20.innerHTML=demo10.innerHTML
demo10.scrollLeft=demoo.scrollWidth
function Marqueeo(){
if(demo20.offsetWidth-demoo.scrollLeft<=0)
		demoo.scrollLeft-=demo10.offsetWidth;
	else
		demoo.scrollLeft++;
}
var MyMar=setInterval(Marqueeo,speed)
demoo.onmouseover=function() {clearInterval(MyMar)}
demoo.onmouseout=function() {MyMar=setInterval(Marqueeo,speed)}





</script>
