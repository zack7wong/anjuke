	<script>
	
//////////////////////公告//////////////////////////////////////////////////

var speed9=40;
var demo9=document.getElementById("demo9"); 
var demo29=document.getElementById("demo29"); 
var demo19=document.getElementById("demo19"); 
demo29.innerHTML=demo19.innerHTML;
function Marquee9(){ 

if(demo29.offsetTop-demo9.scrollTop<=0) 
  demo9.scrollTop-=demo19.offsetHeight;
else{ 
  demo9.scrollTop++
} 
} 
var MyMar9=setInterval(Marquee9,speed9);
demo9.onmouseover=function() {clearInterval(MyMar9)}
demo9.onmouseout=function() {MyMar9=setInterval(Marquee9,speed9)} 

////////////////////////////////////////////////////////////////////////////
	</script>
