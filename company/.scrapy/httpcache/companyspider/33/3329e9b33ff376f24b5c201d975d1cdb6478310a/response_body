 .Scroller {line-height:20px; border:1px solid #D4D4D4; padding:0px 10px; height:40px; width:469px;
 margin-bottom:10px; text-align:right; color:#FF0000}
 .Scroller span{ float:left; color:#FF0000}
    .Scroller *{margin:0px; padding:0px;}
    .ScrollMid {float:left;}
