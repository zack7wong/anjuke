<script language="javascript"  type="text/javascript">
///自动滚动TAB脚本
var CurrentTab = 0;
var AutoScroll = true;

function tab(n){
	var titles = document.getElementById("newsTab").getElementsByTagName("li");
	var contents = document.getElementById("main" + n);	
	for(i=0;i<titles.length;i++){
		 titles[i].className = (i==n?"selected":"");
 		  document.getElementById("main" + i).style.display = (i == n?"block":"none");
	}
	CurrentTab  = n;
}
onload=init;
function init(){
	tab(CurrentTab);
	setTimeout(Next,60000);
}
function Next(){
	if(AutoScroll)
	{
		CurrentTab = (CurrentTab++ >= document.getElementById("newsTab").getElementsByTagName("li").length-1 ? 0 :CurrentTab++);
		tab(CurrentTab);
	}
	setTimeout(Next,60000)
}
function startScroll(k){ AutoScroll = true; }
function stopScroll(k){ AutoScroll = false; }
function linkMore()
{
	var link = "";
	switch(CurrentTab)
	{
		case 0:
			link = "/NewsList.asp?c=1";
			break;
		case 1:
			link = "/NewsList.asp?c=2";
			break;
		case 2:
			link = "/NewsList.asp?c=3";
			break;
	}
	window.open(link)
}

</script>


