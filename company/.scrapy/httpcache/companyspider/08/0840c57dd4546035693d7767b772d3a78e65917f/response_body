        <td class="wz_center"><script>
current1=0;
function secFrame(n)
{
	current1=n;
	for(i=1;i<5;i++)
		eval("tbl0"+i+".style.display='none'");
	eval("tbl0"+n+".style.display='block'");

	}
	
function changefm(n){
	if (current1!=n){
		switch(n)
		{

		  case 1:
			eval("img_xxgl.src='/static/images/left_frame_xxgl.gif'")
			eval("img_jhzj.src='/static/images/left_frame_jhzj.gif'")
