<!--float start><float end-->
<div id="myfloat_All" style="display:none;">   
<div id="myfloat_img" style="position: absolute; z-index:9999;">   
<a href="http://www.hnsoftedu.com:85/public/dzw/" target="_blank"><img src="http://www.hnsoftedu.com/images/float2018.png" border="0"  style="cursor: pointer;" /></a>
<script language="JavaScript">  
	var myfloat_xPos = document.body.clientWidth - 20;  
	var myfloat_yPos = document.body.clientHeight / 2;  
	var myfloat_step = 1;  
	var myfloat_delay = 10;  
	var myfloat_height = 0;  
	var myfloat_Hoffset = 0;  
	var myfloat_Woffset = 0;  
	var myfloat_yon = 0;  
	var myfloat_xon = 0;  
	var myfloat_pause = true;  
	myfloat_img.style.top = myfloat_yPos;  
	function myfloat_changePos() {  
		myfloat_width = document.body.clientWidth;  
		myfloat_height = document.body.clientHeight;  
		myfloat_Hoffset = myfloat_img.offsetHeight;  
		myfloat_Woffset = myfloat_img.offsetWidth;  
		myfloat_img.style.left = myfloat_xPos + document.body.scrollLeft;  
		myfloat_img.style.top = myfloat_yPos + document.body.scrollTop;  
		if (myfloat_yon) {  
			myfloat_yPos = myfloat_yPos + myfloat_step;  
		} else {  
			myfloat_yPos = myfloat_yPos - myfloat_step;  
		}    
		if (myfloat_yPos < 0) {  
			myfloat_yon = 1;  
			myfloat_yPos = 0;  
		}  
		if (myfloat_yPos >= (myfloat_height - myfloat_Hoffset)) {  
			myfloat_yon = 0;  
			myfloat_yPos = (myfloat_height - myfloat_Hoffset);  
		}  
		if (myfloat_xon) {  
			myfloat_xPos = myfloat_xPos + myfloat_step;  
		} else {  
			myfloat_xPos = myfloat_xPos - myfloat_step;  
		}  
		if (myfloat_xPos < 0) {  
			myfloat_xon = 1;  
			myfloat_xPos = 0;  
		}  
		if (myfloat_xPos >= (myfloat_width - myfloat_Woffset)) {  
			myfloat_xon = 0;  
			myfloat_xPos = (myfloat_width - myfloat_Woffset);  
		}  
	}   
	var myfloat_obj=document.getElementById("myfloat_All") ;
	var myfloat_itl= setInterval("myfloat_changePos()", myfloat_delay);
	myfloat_obj.onmouseover=function(){clearInterval(myfloat_itl)};
	myfloat_obj.onmouseout=function(){myfloat_itl=setInterval("myfloat_changePos()", myfloat_delay)};
</script>  
</div>  
