         <td colspan="9"    align="center"   valign="top" ><script language =javascript >
var curIndex=0;
//时间间隔 单位毫秒
var timeInterval=4000;
var arr=new Array();
arr[0]="images/kt1.jpg";
arr[1]="images/kt2.jpg";
arr[2]="images/kt3.jpg";
arr[3]="images/kt4.jpg";
arr[4]="images/kt5.jpg";
arr[5]="images/kt6.jpg";
arr[6]="images/kt7.jpg";
arr[7]="images/kt8.jpg";

setInterval(changeImg,timeInterval);
function changeImg()
{
    var obj=document.getElementById("obj");
    if (curIndex==arr.length-1) 
    {
        curIndex=0;
    }
    else
    {
        curIndex+=1;
    }
    obj.src=arr[curIndex];
}
</script>
           
