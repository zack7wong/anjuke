				$(function(){
					var nav = 0; // 默认值为0，二级菜单向下滑动显示；值为1，则二级菜单向上滑动显示
					if(nav ==0){
						$('.nav li').hover(function(){
							$('a',this).css('background','#669988');
							$('.second',this).css('top','0px').show();
						},function(){
							$('a',this).css('background','');
							$('.second',this).hide();
						});
					}else if(nav ==1){
						$('.nav li').hover(function(){
							$('.second',this).css('bottom','0px').show();
						},function(){
							$('.second',this).hide();
						});
					}
				});
			</script>
