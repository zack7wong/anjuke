<script>
function showIsReply($item){
	var value=$item.value;
	if(value==1){
	//document.getElementById('showinfo').style.display="block";
	$("#showinfo").show();
	}
	else{
	//	document.getElementById('showinfo').style.display="none";
		$("#showinfo").hide();
		}
	}
</script>
<script type="text/javascript">
	var DataCount = 0;
	var OpenWinObj;
	var TempHtml;
	var IsShow = false;
	//设置为同步
	$.ajaxSetup({ async: false });
	function OpenWin(){
		if(IsShow == false){
			$.post('SubmitContent.php?view=getcount', function (data, status) {
				if (status == "success") 
				DataCount = parseInt(data);
				
				}
				);
			
	TempHtml ='<div style="width:539px;height:210px; margin:5px;color:#277ab9;font-family:"黑体";">';
			TempHtml += '<div style=" padding-left:10px; font-size:18px; font-weight:bold; margin:10px; text-align:center;">意见与建议</div>';
			TempHtml += '<textarea id="TetareContent" style="width:505px; height:120px; margin-left:10px; overflow:auto; font-size:16px; padding:5px;" onkeyup="textCounter(500)" wrap="virtual"></textarea>';
			TempHtml += '<div style="text-align:right; margin-top:10px; margin-right:20px; margin-left:0px;">';
			TempHtml += '<table width="100%" border="0" cellspacing="-1" cellpadding="-1">';
			TempHtml += '<tr>';
			TempHtml += '<td width="10%"><span id="maxlimitlable" style="margin-left:20px;">1000/1000</span></td>';
			TempHtml += '<td width="50%" align="left"><span id="lable" style="margin-left:20px;"> 是否需要回复 :   是<input  name="IsReply" type="radio" onclick="showIsReply(this)"  value="1" />否<input  name="IsReply" type="radio"  value="0" onclick="showIsReply(this)"  /></span></td>';
			TempHtml += '<td width="40%">目前已有'+DataCount+'人留言</td>';
			TempHtml += '</tr>';
			TempHtml += '</table>';
			TempHtml += '<table width="100%" border="0" cellspacing="-1" cellpadding="-1" height="18">';
			TempHtml += '<tr id="showinfo" >';
			TempHtml += '<td width="10%" height="18" ></td>';
			TempHtml += '<td  width="100" height="18"  align="left">电话：<input id="Phone" type="text"  class="Newinput" /></td>';
				TempHtml += '<td  width="100" height="18"  align="left">邮箱：<input id="Email" type="text"  class="Newinput"  /></td>';
			TempHtml += '</tr>';
			TempHtml += '</table>';
			TempHtml += '</div>';
			TempHtml += '</div>';
			OpenWinObj=$.dialog({
				title:'学院领导信箱',
				//lock: true,遮罩
				content: TempHtml,
				id: 'SubmitContentWin',
				max:false,
				min:false,
				fixed: true,
				ok: function () {
					/* 这里要注意多层锁屏一定要加parent参数 */
					//$.dialog({content: '再来一个锁屏', lock: true, parent:this});
					//post方法,获得数据
					SubmitContent();
					IsShow = false			
					return false;
				},
				cancel: function () {
					IsShow = false;
				}
			});
			IsShow = true;
			TetareContent.focus();
			$("#showinfo").hide();	
		}
		else return;
		
	}


	var IsSubmitContent = true;
	var OldContent='';
	function SubmitContent(){
	var strContent = trim(TetareContent.value);
	   var IsReply=$('input:radio[name="IsReply"]:checked').val();  
            var strtemp="";

            if(IsReply==null){ 
                alert("请选择是否需要回复!"); 
				strtemp+="&IsReply="+IsReply;
                return false; 
            } 
			
			else{
					
				var Phone= document.getElementById('Phone').value;
				var Email= document.getElementById('Email').value;
				strtemp+="&IsReply="+IsReply+"&Phone="+Phone+"&Email="+Email;
				
			
				}


	 	if(IsSubmitContent == false) return;
		if(strContent==""||typeof(strContent)=="undefined") { 
			lable.innerHTML = "内容不能为！";
			return;
		}
		if(OldContent==strContent){
			lable.innerHTML = "您已经提交过相同的内容了！";
			return;
		}
		var pattern=/[`~!@#\$%\^\&\*\(\)_\+<>\?:"\{\},\.\\\/;'\[\]]/im;      
		if(pattern.test(strContent)){    
			lable.innerHTML = "提交失败，内容中包含非法字符！";      
			return;         
		}         
		//alert(strContent);
		//post方法,获得数据
		$.post(
			'SubmitContent.php?view=submitcontent&Content='+strContent+strtemp, function (data, status) {//Sys_FieldInfo
				if (status == "success") { 
				 if(data=="1") {
					lable.innerHTML = "提交成功！";
					OldContent=strContent;
					//btnOk.value = "30秒后解锁";
					//IsSubmitContent = false;
					//Timevalue(60);
					//$.dialog.alert('提交成功！',icon: 'success.gif',title:'提示');
					$.dialog({
						time: 3,
						title:'提示',
						icon: 'success.gif',
						content: '提交成功',
						cancelVal: '确定',
    					cancel: true /*为true等价于function(){}*/

					});
					OpenWinObj.close();	
				 }
				 else lable.innerHTML = "提交失败！";
				}
				else  lable.innerHTML = "提交失败！";
		});
	
	}
	function trim(str){
		str = str.replace(/^ +/,'');
		str = str.replace(/ +$/,'');
		var reg=new RegExp("--","g"); //创建正则RegExp对象      
		var newstr=str.replace(reg,"");   
		return newstr;
	} 
	var Timei=0;
	function Timevalue(s){//s为停止的时间
		if ( Timei++<s){
			var TextValue="秒后解锁";
			btnOk.value = (s-Timei)+TextValue;
			setTimeout("Timevalue("+s+");",1000); 
		}
		else{
			//延时S秒后执行的语句
			btnOk.value = "提 交";
			IsSubmitContent = true;
		}
	}
	function textCounter(maxlimit) { 
		if (TetareContent.value.length > maxlimit){ 
			TetareContent.value = TetareContent.value.substring(0, maxlimit); 
		}else{ 
			maxlimitlable.innerHTML = (maxlimit - TetareContent.value.length)+"/"+maxlimit; 
		} 
		//alert(TetareContent.value.length);
	}

</script>
