            var mst;



            jQuery(".multiUl li").hover(function(){



            var curItem = jQuery(this);



            mst = setTimeout(function(){//延时触发



                curItem.find("blockquote").slideDown('fast');



                mst = null;



            });



            }, function(){



            if(mst!=null)clearTimeout(mst);



            jQuery(this).find("blockquote").slideUp('fast');



            });



            



