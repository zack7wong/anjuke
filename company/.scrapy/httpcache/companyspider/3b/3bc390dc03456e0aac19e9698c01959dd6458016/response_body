            $('.flink span').click(function(){
                $(this).parent().find('ul').slideDown("",function(){$(this).parent().find('.flink ul').addClass("son_ul_down");});                
                $(this).parent().find('li').hover(function(){$(this).addClass('hover')},function(){$(this).removeClass('hover')});
                $(this).parent().hover(function(){},function(){

                    $(this).parent().find(".flink ul").removeClass("son_ul_down");

                    $(this).parent().find(".flink ul").slideUp();


                });

      });

