<script>
var bbb=document.getElementById("div1");
function offsetMove1(elem,offset){
    clearInterval(elem.timer);
    var speed = 0;
    elem.timer = setInterval(function(){
        speed = (offset - elem.offsetHeight)/15;
        speed = speed>0?Math.ceil(speed):Math.floor(speed);
        
        //是否到达到目标点
        if(elem.offsetLeft == offset){
            clearInterval(elem.timer);
        }else{
            elem.style.height = elem.offsetHeight + speed + "px";
        }
    },30)
}
function a()
		{	
			if(aaa.style.height=='500px'){offsetMove1(aaa,1);}
			else{offsetMove1(aaa,1);}
		}
</script>
