    <script language="javascript" type="text/javascript">
        var xx = 50, yy = 60
        var xxin = true, yyin = true
        var step = 1
        var delay = 30
        var obj = document.getElementById("adz");
        function floatAD() {
            var L = T = 0
            var R = document.body.clientWidth - obj.offsetWidth
            var B = document.body.clientHeight - obj.offsetHeight
            obj.style.left = xx + document.body.scrollLeft
            obj.style.top = yy + document.body.scrollTop
            xx = xx + step * (xxin ? 1 : -1)
            if (xx < L) { xxin = true; xx = L }
            if (xx > R) { xxin = false; xx = R }
            yy = yy + step * (yyin ? 1 : -1)
            if (yy < T) { yyin = true; yy = T }
            if (yy > B) { yyin = false; yy = B }
        }
        var itl = setInterval("floatAD()", delay)
        obj.onmouseover = function () { clearInterval(itl) }
        obj.onmouseout = function () { itl = setInterval("floatAD()", delay) }
    </script>
    
    <!--飘浮end-->
