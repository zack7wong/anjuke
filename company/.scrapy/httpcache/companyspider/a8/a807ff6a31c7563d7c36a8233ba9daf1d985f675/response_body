            <script type="text/javascript">
                function getT(Obj) {
                    var dtd = document.getElementById(Obj);
                    for (var i = 0; i < dtd.length; i++) {
                        if (dtd.options[i].selected == true) {
                            userRole = dtd.options[i].value;
                        }
                    }
                    if (userRole.length > 0) {
                        window.open(userRole);
                    }
                }
            </script>
            <script language="javascript" type="text/javascript">
                function SwapDiv(paramThis) {
                    var paramNode = paramThis.parentNode.children;
                    var paramIndex = 0;
                    for (var i = 0; i < paramNode.length; i++) {
                        if (paramThis == paramNode[i]) {
                            paramIndex = i;
                        }
                        paramNode[i].id = "biao-left-bian";
                    }
                    paramThis.id = "biao-left";

                    var paramNode = paramThis.parentNode.parentNode.children;
                    for (var i = 1; i < paramNode.length; i++) {
                        paramNode[i].style.display = "none";
                    }
                    paramIndex++;
                    try {
                        paramNode[paramIndex].style.display = "";
                        paramNode[paramIndex].id = "news-xin-content";
                    }
                    catch (e) {
                    }
                }
            </script>
