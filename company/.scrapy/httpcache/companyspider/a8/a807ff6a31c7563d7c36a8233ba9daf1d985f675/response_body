        <script type="text/javascript" language="javascript">
            function OpenCheck() {
                window.open("Page/SeeToupiaoPage.aspx");
            }

            function clickbutton() {
                if (event.keyCode == 13) {
                    $get("btnLogin").focus();
                    document.getElementById('btnLogin').click();
                }
            }

            function OpenSearchPage(myParam) {
                var returnValue = LindonSoft.Campus.WebPage.Index.IsOpenSearchPage();
                if (returnValue.value.length > 0) {
                    alert(returnValue.value);
                }
                else {
                    window.open("Page/SearchPage.aspx?Type=" + myParam);
                }
            }

            function OpenModule(ListID, ModuleID) {
                //var result = LindonSoft.Campus.WebPage.Index.GetEmpName();
                var UserType = '';
                var UserID = '';
                if (UserID == "") {
                    alert("仅供内部教师或学生查看,请先登录！");
                    return;
                }
                if (UserType == "1") {
                    var returnValue = LindonSoft.Campus.WebPage.Index.OpenModule(ListID, ModuleID);

                    if (returnValue.value == "true" || returnValue.value == true) {
                        window.open("Default.aspx");
                    }
                }
                else {
                    alert("仅供内部教师查看！");
                }
            }
    </script>
