<script language="javascript">
    /*层的显示隐藏*/
    function ShowMenu1(paramNumber)//层的显示隐藏
    {
        for (var i = 1; i <= 10; i++) {
            if (i == paramNumber) {
                document.getElementById("Layer" + i).style.display = "block";
            }
            else {
                document.getElementById("Layer" + i).style.display = "none";
            }
        }
    }

    function HideMenu(paramNumber) {

        document.getElementById("Layer" + paramNumber).style.display = "none";
    }
</script>

