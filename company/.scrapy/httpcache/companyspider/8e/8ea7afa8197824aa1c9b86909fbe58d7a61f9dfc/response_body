              <script type="text/javascript">







                            //图片无间断滚动代码，兼容IE、Firefox、Opera







                            //原脚本是IE only，来自网上，作者未知







                            //部分内容是由forestgan为了JS代码标准化，兼容以gecko为内核的浏览器







                            var speeda=15;







                            var FGDemo=document.getElementById('demo3');







                            var FGDemo1=document.getElementById('demo4');







                            var FGDemo2=document.getElementById('demo5');







                            FGDemo2.innerHTML=FGDemo1.innerHTML







                            function Marqueea(){







                            if(FGDemo2.offsetHeight-FGDemo.scrollTop<=0)







                            FGDemo.scrollTop-=FGDemo1.offsetHeight







                            else{







                            FGDemo.scrollTop++







                            }







                            }







                            var MyMara=setInterval(Marqueea,speeda)







                            FGDemo.onmouseover=function() {clearInterval(MyMara)}







                            FGDemo.onmouseout=function() {MyMara=setInterval(Marqueea,speeda)}







                       </script> 







