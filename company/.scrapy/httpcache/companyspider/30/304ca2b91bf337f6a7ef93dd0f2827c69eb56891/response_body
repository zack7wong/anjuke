                    ISiexplor: function () {
                        if ((navigator.userAgent.indexOf('MSIE') >= 0)){
                            return true;
                        } else {
                            return false;
                        }
                    },
                    open: function (b, c, sta, index) {
                        if(this.ISiexplor())
                        {
                            b.src = "images/" + imgbox.imagelist[index]["url"];
                            b.style.filter ="alpha(opacity=40)";
                        }
                       
                        imgbox.tm = setTimeout(function () {imgbox.opac(b, c, sta, index); }, 10);
                    },

                    TurnAround:function (index)
                    {
                        var box = document.getElementById("box");
                        var content = document.getElementById("content");
                        if(typeof imgbox.index_tm)
                        {
                            clearTimeout(imgbox.index_tm);
                        }
                        if(index<(imgbox.imagelist.length-1)&&index>=0)
                        {
                            if(index==0)
                            {
                                this.open(box,content,1,index);
                            }else
                            {
                                this.open(box,content,1,index);
                            }
                            index++;
                            imgbox.index_tm = setTimeout(function(){imgbox.TurnAround(index);},6000);
                        }else
                        {
                    
                            this.open(box,content,1,index);
                            index=0;
                            imgbox.index_tm = setTimeout(function(){imgbox.TurnAround(index);},6000);
                        }

                    },

                    opac: function (b, c, sta, index) {
                        if (imgbox.tm != null) {
                            clearTimeout(imgbox.tm);
                        } 
                        if (sta == 1) {
                            if (!imgbox.lingpai) {
                         
                                if(this.ISiexplor())
                                {
                                    if(b.filters[0].Opacity>60)
                                    {
                                        b.style.filter="alpha(opacity="+(b.filters[0].Opacity-2)+")";
                                        imgbox.lingpai = false;
                                        imgbox.tm = setTimeout(function () { imgbox.opac(b, c, sta, index); }, 1);
                                    }else
                                    {
                                        clearInterval(imgbox.tm);
                                        imgbox.lingpai = true;
                                        b.style.filter="alpha(opacity=50)";
                                        b.src = "images/" + imgbox.imagelist[index]["url"];
                                        imgbox.tm = setTimeout(function () { imgbox.opac(b, c, sta, index); }, 1);
                                    }
                                }
                                else
                                {
                                    if ((b.style.opacity * 100) > 60) 
                                    {
                                        b.style.opacity = ((b.style.opacity * 100) - 1) / 100;
                                        imgbox.lingpai = false;
                                        imgbox.tm = setTimeout(function () {imgbox.opac(b, c, sta, index);},1);
                                    }
                                    else 
                                    {
                                        clearInterval(imgbox.tm);
                                        imgbox.lingpai = true;
                                        b.style.opacity = 50 / 100;
                                        b.src = "images/" + imgbox.imagelist[index]["url"];
                                        imgbox.tm = setTimeout(function () {imgbox.opac(b, c, sta, index);},1);
                                    }
                                }

                            } 
                            else 
                            {
                                if(this.ISiexplor())
                                {
                                    if(b.filters[0].Opacity<100)
                                    {
                                        b.style.filter="alpha(opacity="+(b.filters[0].Opacity+2)+")";
                                        imgbox.lingpai = true;
                                        imgbox.tm = setTimeout(function () {imgbox.opac(b, c, sta, index);},1);
                                    }
                                    else
                                    {
                                        clearTimeout(imgbox.tm);
                                        imgbox.lingpai = false;
                                    }
                                }
                                else
                                {
                                    if ((b.style.opacity * 100) < 100) {
                                        b.style.opacity = ((b.style.opacity * 100) + 1) / 100;
                                        imgbox.lingpai = true;
                                        imgbox.tm = setTimeout(function () { imgbox.opac(b, c, sta, index); }, 1);
                                    } else {
                                        clearTimeout(imgbox.tm);
                                        imgbox.lingpai = false;
                                    }
                                }
                            }
                        }
                    }
                };

                var play = function (index) {
                    var box = document.getElementById("box");
                    var content = document.getElementById("content");
                    var arr = box.src.toString().split('/');

                    move.Init(content,index);
                }

                var move = {
                    tim: null,
                    Init: function (c,index) {
                        if (typeof c) {
                            c.style.zIndex = 1;
                            c.style.position = "relative";
                            c.style.top = "-350px";
                            c.style.left = "-160px";
                            c.innerHTML=imgbox.imagelist[index-1]["content"];
                            move.tim = setInterval(function () { move.offset(c); }, 50);
                        }
                    },
                    offset: function (c) {
                        var index = c.style.left.indexOf("px");
                        var left = c.style.left.substring(0, index);
                        if (left < 0) {
                            c.style.left = (parseInt(left) / 3 + 1) + "px";

                        } else {
                            clearInterval(move.tim);
                        }
                    }
                }
                LunZhuan.init(1);
                imgbox.TurnAround(0);

                function clearVideo()
                { 
                    var cl=document.getElementById("cl");
                    if(cl!=null){cl.style.display="none";
                    }
                }
        </script>
