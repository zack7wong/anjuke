        <script language="javscript" type="text/javascript">

            var ToUrl=function(word)
            {
                window.location.href="NewsList.aspx?word="+escape(word);
            }
            var LunZhuan = {
                index: 0,
                t: null,
                timeout: null,
                Select: function (index) {
                    if (typeof this.timeout) {
                        clearTimeout(this.timeout);
                    }
                    var range = 150 * (index - 1);
                    if (document.getElementById("pic").scrollTop < range) {
                        document.getElementById("pic").scrollTop = document.getElementById("pic").scrollTop + Math.ceil((range - document.getElementById("pic").scrollTop) / 3);
                        this.timeout = setTimeout(function () { LunZhuan.Select(index);}, 50);
                    } else if (document.getElementById("pic").scrollTop > range) {
                        document.getElementById("pic").scrollTop = document.getElementById("pic").scrollTop - Math.ceil((document.getElementById("pic").scrollTop - range) /3);
                        this.timeout = setTimeout(function () { LunZhuan.Select(index);}, 50);
                    }
                    else {
                        clearTimeout(this.timeout);
                    }
                },
                init: function (idx) {
                    if (typeof LunZhuan.t) {
                        clearTimeout(LunZhuan.t);
                    }
                    LunZhuan.index=idx;
                    if(8>0)
                        {
                            if (LunZhuan.index<=1) {
                                LunZhuan.index=1;
                                document.getElementById("tupian").innerHTML=LunZhuan.index+"/"+8;
                            LunZhuan.Select(LunZhuan.index);
                            LunZhuan.t = setTimeout(function () {  LunZhuan.index++;LunZhuan.init(LunZhuan.index); }, 6000);
                        }
                        else if (LunZhuan.index <=8&&LunZhuan.index>1) {
                            document.getElementById("tupian").innerHTML=LunZhuan.index+"/"+8;
                            LunZhuan.Select(LunZhuan.index);
                            LunZhuan.t = setTimeout(function () {LunZhuan.index++; LunZhuan.init(LunZhuan.index); }, 6000);
                        }
                        else if (LunZhuan.index>8) {
                            LunZhuan.index = 1;
                    document.getElementById("tupian").innerHTML=LunZhuan.index+"/"+8;
                            LunZhuan.Select(LunZhuan.index);
                           
                            LunZhuan.t = setTimeout(function () {  LunZhuan.index++; LunZhuan.init(LunZhuan.index); }, 6000);
                        }else
                        {
                            document.getElementById("tupian").innerHTML="0/0";
                        }
                    }
                },
                    up:function()
                    {
                        if (typeof LunZhuan.t) {
                            clearTimeout(LunZhuan.t);
                        }
                        LunZhuan.index++;
                        LunZhuan.init(LunZhuan.index);
                    },
                down:function()
                {
                    if (typeof LunZhuan.t) {
                        clearTimeout(LunZhuan.t);
                    }
                    LunZhuan.index--;
                    LunZhuan.init(LunZhuan.index);
                }
                }
                

                var DisPlay = function (Obj, index_str) {
                    document.getElementById("a_news").style.color = "#043779";
                    document.getElementById("a_media").style.color = "#403F41";
                    document.getElementById("News").style.display = "none";
                    document.getElementById("Media").style.display = "none";
                    if (typeof Obj) {
                        Obj.style.color = "#043779";
                        document.getElementById(index_str).style.display = "block";
                    }

                }
                var imgbox = {
                    index_tm:null,

                    tm: null,

                    lingpai: false,

                    status: 0,

                    imagelist: [
