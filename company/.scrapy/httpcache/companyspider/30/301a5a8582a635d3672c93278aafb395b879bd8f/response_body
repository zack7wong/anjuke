$(".bannerNews_dl dd").hover(function(){
	$(this).find("a").stop(true,true).animate({height:110},400)
	$(this).find(".pic").stop(true,true).animate({left:13,bottom:10,opacity:1},400)
	$(this).find(".bannerNewsR").stop(true,true).animate({paddingLeft:140},400)
	$(this).find(".bannerNewsR .txt").stop(true,true).animate({opacity:1},400)
	},function(){
