<script type='text/javascript'> 
    $().ready(function() { 
        $("#w80_loginButton").click(function() { 
            w80_userLogin(); 
        }); 
    }); 
    function w80_userLogin() { 
        if($('#w80_userName').val()=='') { 
           $('#w80_userName').focus(); 
            alert('请输入用户名！'); 
        } else if($('#w80_password').val()=='') { 
           $('#w80_password').focus(); 
            alert('请输入密码！'); 
        } else { 
            if($('#w80_validateCode').length>0 && $('#w80_validateCode').val()=='') { 
           $('#w80_validateCode').focus(); 
                alert('请输入验证码！'); 
                return; 
            } 
            $.post("/_web/ids/login.do?_p=YXM9MiZ0PTEmZD0xJnA9MSZtPVNOJg__&tt=" + Math.random(), {userName: $("#w80_userName").val(), password: $("#w80_password").val(), validateCode: $('#w80_validateCode').val()}, function(result) { 
                var loginInfo = eval('(' + result + ')'); 
                if (loginInfo.status !== 1) {  
                  getCode();
                   $("#w80_validateCode_img").trigger('click');
                  alert(loginInfo.message); return; } 
                var visitUrl = '/main.htm'; 
                visitUrl = visitUrl.replace('.htm', '.psp'); 
                window.location.href = visitUrl; 
            }); 
        } 
    } 
    function getCode() { 
   $('#w1_validateCode_img').on('click', function(event) { 
   event.preventDefault();
    document.getElementById("w80_validateCode_img").src="/_control/validateimage?tt="+ Math.random();
    }); 
    } 
</script>
