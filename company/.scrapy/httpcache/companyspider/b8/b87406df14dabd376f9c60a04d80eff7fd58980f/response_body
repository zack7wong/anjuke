<script language="javascript" type="text/javascript">
        // 初始化获取焦点
        jQuery(document).ready(function () {
            jQuery("#LoginName").focus();
        });

        // 登陆验证
        function JudgeUserLogin() {
            var txtLoginName = jQuery("#LoginName");
            var txtPassWord = jQuery("#PassWord");

            if (jQuery.trim(txtLoginName.val()) == "") {
                alert("用户名不能为空！");
                txtLoginName.focus();
                return false;
            }
            else if (jQuery.trim(txtPassWord.val()) == "") {
                alert("密码不能为空！");
                txtPassWord.focus();
                return false;
            }
            else {
				$("#log_random").val(Math.random());
				$("#email_form").submit();
				window.open("http://oa.msannu.cn/Function/Index.html","EmailFrame");
            }
        }

        // 重新填写
        function txtReSet() {
            var txtLoginName = jQuery("#LoginName");
            var txtPassWord = jQuery("#PassWord");
            txtLoginName.val("");
            txtPassWord.val("");
            txtLoginName.focus();
        }

        // 回车直接执行登陆验证
        function onKeyPressEnter() {
            if (event.keyCode == 13) {
                JudgeUserLogin();
                return false;
            }
        }
    </script>
    <script>
$(function(){
		$(".hour-item").each(function(index, el) {
		
			var $hour = $(".item-time", el);
			var hour = parseInt($hour.attr('data-time'),10);
			var label = "";
			if(0<=hour){
	            label = "am";
			}
			if(12<=hour){
	            label = "pm";
			}

			if(label!=="")
				label = " " + label;

	        $hour.html(function(index,html){
	            
	            return html+label;
	        });

		});
})
</script>

