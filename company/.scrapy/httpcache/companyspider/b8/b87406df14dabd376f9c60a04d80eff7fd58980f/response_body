<script language="javascript">
    var topw4 = document.getElementById("topw4");
    var beginw4 = document.getElementById("beginw4");
    var endw4 = document.getElementById("endw4");
    var tabw4 = document.getElementById("tabw4");
    //复制内容
    endw4.innerHTML = beginw4.innerHTML;
    //调整table的宽度
    tabw4.style.width = topw4.offsetWidth * 2;
    //循环函数
    function marqueew4() {
        if (endw4.offsetWidth - topw4.scrollLeft <= 0) {
            topw4.scrollLeft -= beginw4.offsetWidth;
        } else {
            topw4.scrollLeft += 1;
        }
    }
    //定时器
    var mymarw4 = setInterval(marqueew4, 20);
    //鼠标移入
    topw4.onmouseover = function() {
        clearInterval(mymarw4);
    };
    //鼠标移出
    topw4.onmouseout = function() {
        mymarw4 = setInterval(marqueew4, 20);
    };
</script>
