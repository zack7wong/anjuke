function getByteLen(val)
{
	var len = 0;
	for ( var i = 0; i < val.length; i++)
	{
		var length = val.charCodeAt(i);
		if (length >= 0 && length <= 100)
		{
			len += 1;
		} else
		{
			len += 2;
		}
	}
	return len;
}
function selectImg(id){
	$("div[name='contentImg']").each(function(){
		$(this).css("display","none");
	});
	$("#contentImg"+id).css("display","");
}
function getNewsContent(indexOrder){
	var url="guide!getNewsContent.do";
	if(indexOrder==107){
		url="guide!getImgNewsContent.do";
	}

	$.ajax({
      url:url,
      type:"post",
      data:"indexShowOrder="+ indexOrder,
      dataType:'html',
      async:false,
 	  cache:true,
      success:function(result){
      	document.getElementById("specialCon").innerHTML = result;
  	  }
	})
}
function getNewsInfoByTreeId(treeId,pid){
	var url="guide!getNewsInfoByTreeId.do";

	$.ajax({
      url:url,
      type:"post",
      data:"treeId="+ treeId,
      dataType:'html',
      async:false,
 	  cache:true,
      success:function(result){
      	document.getElementById("innerContent"+pid).innerHTML = result;
  	  }
	})
}
$("#loginForm").submit(function() {
	var username = $.trim($("#userName").val());
	var password = $.trim($("#password").val());
	if (username == '') {
		swal({
			title : "登录提示",
			text : "用户名不能为空，请您输入",
			type : "error",
			confirmButtonColor : "#DD6B55",
			confirmButtonText : "确定"
		});
		$("#userName").val('');
		$("#userName").focus();
		return false;
	}
	if (password == '') {
		swal({
			title : "登录提示",
			text : "密码不能为空，请您输入",
			type : "error",
			confirmButtonColor : "#DD6B55",
			confirmButtonText : "确定"
		});
		$("#password").val('');
		$("#password").focus();
		return false;
	}

});

$("#keywords").focus(function() {
	if ($.trim(this.value) == '请在搜索框输入您要查找的关键字') {
		this.value = "";
	}
});

$("#keywords").blur(function() {
	if ($.trim(this.value) == '') {
		this.value = "请在搜索框输入您要查找的关键字";
	}
});

$("#searchForm").submit(function() {
	if ($.trim($("#keywords").val()) == '请在搜索框输入您要查找的关键字') {
		alertBox("请输入查询关键字");
		return false;
	}
});

var treeid = '';
var id = '';
if (null != id && id != "") {
	treeid = id;
}
$('li.mainlevel').each(function() {
	var menuId = $(this).attr("data-treeId");
	if (treeid == menuId) {
		$("li.mainlevel").removeClass("selectMenu");
		$("#mainlevel" + menuId).addClass("selectMenu");
		$("#mainlevel" + menuId).find('a:first').css('color', '#e8f003');
	}
})


function init()
{ 
	
   

    	  var objKernalRoot = RTXAX.GetObject("KernalRoot");
         //  alert(objKernalRoot)
           var objRtcData = objKernalRoot.Sign;
           var strAccount = objKernalRoot.Account;
           var strSgin = objRtcData.GetString("Sign");
           
           //alert(strAccount)
           //alert(strSgin)
           LoginForm.userName.value = strAccount;
			LoginForm.sign.value = strSgin;
			
			LoginForm.action = "Login!rexLogin.do";
			LoginForm.submit();
			
   
}

