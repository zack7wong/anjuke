	window.contextPath = "/";
	var USER_ARRAY = "";
	var USER_ARRAY_WITH_ID = "";
	var tipTime = 2;
	//动态加载工具类
	var head = document.getElementsByTagName('head')[0];
	var script= document.createElement("script");
	script.type = "text/javascript";
	script.src= contextPath + "resource/javascript/commonUtils.js";
	head.appendChild(script);

	function setTitle(title)
	{
		if(document.documentElement)
			y = document.documentElement.scrollTop;
		else
			y =	document.body.scrollTop;
		if(div_title)
		{
			div_title.style.top = event.clientY + y ;
			div_title.style.left = event.clientX + 8 ;

			div_title.style.visibility = 'visible';
		}
	}

	function clearTitle()
	{
		if(div_title)
			div_title.style.visibility = 'hidden';
	}
	function checkExplorer(){

		if(navigator.userAgent.indexOf("MSIE")>0){
			if(navigator.userAgent.indexOf("MSIE 6.0")>0){
				return false;
			}
			if(navigator.userAgent.indexOf("MSIE 7.0")>0){
				return false;
			}
			if(navigator.userAgent.indexOf("MSIE 9.0")>0 && !window.innerWidth){//这里是重点，你懂的
				return false;
			}
			if(navigator.userAgent.indexOf("MSIE 9.0")>0){
				return true  ;
			}
		}
		return true;
	}
</script>
