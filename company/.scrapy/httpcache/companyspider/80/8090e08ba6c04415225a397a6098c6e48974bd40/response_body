<script type="text/javascript">
(function(){
var c = $('#banner_index_143 .banner_wrap li').length,w = 209,d = -c*w, temp = $('#banner_index_143 .banner_wrap').html();timer =  setInterval(move_anti, 5000);
$('#banner_index_143 .banner_wrap').css({width:7*c*w });
$(temp).appendTo($('#banner_index_143 .banner_wrap'));
$(temp).prependTo($('#banner_index_143 .banner_wrap'));
$('#banner_index_143 .banner_wrap').css({left:d});
$('#banner_index_143 .btn').click(function(){ clearInterval(timer);timer = null; move_anti($(this).attr('alt'))});

function move_anti(a){
if(a=='left'){ d +=w;	}else{	d -= w;	} 
$("#banner_index_143 .banner_wrap").animate({left:d+'px'},	function(){ if(d>=0) d=-(2*c)*w; else if (d <= -(3*c-1)*w) d=-(c-1)*w;	$('#banner_index_143 .banner_wrap').css({left:d}); if(!timer)timer =  setInterval(move_anti, 5000);} );	
}
})();
</script></div>
