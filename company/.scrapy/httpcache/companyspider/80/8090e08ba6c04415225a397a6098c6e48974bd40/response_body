(function(){
var ln = $('#pingpai_9').find('li').length;
var w = ln*2*(160+10);
$('#scroll_9').width(w);
$('#pingpaicopy_9').html($('#pingpai_9').html());
var MyMar=setInterval(Marquee,20); 
$('#scroll_9').hover( function(){ clearInterval(MyMar) }, function(){ MyMar=setInterval(Marquee,20) } );
function Marquee(){var tab=$('#pingpaibox_9')[0], tab1=$('#pingpai_9')[0],tab2=$('#pingpaicopy_9')[0];
if(tab2.offsetWidth-tab.scrollLeft<=0)tab.scrollLeft-= tab1.offsetWidth;else{tab.scrollLeft++;}
$('#tab').html(tab.scrollLeft);
$('#tab1').html(tab1.offsetWidth);
$('#tab2').html(tab2.offsetWidth);
}
})()
</script>
