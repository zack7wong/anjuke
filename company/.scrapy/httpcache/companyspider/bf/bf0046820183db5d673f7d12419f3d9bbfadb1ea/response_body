                                                        <script>
                                                            var speed1 = 50;
                                                            var colees2 = document.getElementById("colees2");
                                                            var colees1 = document.getElementById("colees1");
                                                            var colee = document.getElementById("colees");
                                                            colees2.innerHTML = colees1.innerHTML; //克隆colee1为colee2
                                                            function Marquees1() {
                                                                //当滚动至colee1与colee2交界时
                                                                if (colees2.offsetTop - colees.scrollTop <= 0) {
                                                                    colees.scrollTop -= colees1.offsetHeight; //colee跳到最顶端
                                                                } else {
                                                                    colees.scrollTop++
                                                                }
                                                            }
                                                            var MyMars1 = setInterval(Marquees1, speed1)//设置定时器
                                                            //鼠标移上时清除定时器达到滚动停止的目的
                                                            colee.onmouseover = function () { clearInterval(MyMars1) }
                                                            //鼠标移开时重设定时器
                                                            colee.onmouseout = function () { MyMars1 = setInterval(Marquees1, speed1) }
                                                        </script>
