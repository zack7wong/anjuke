<div id="swfDiv" style="position:absolute; top:0; left:0; z-index:99; border:solid 0px;" border=0><!-- 飘的CSS开始 -->
 <!-- <a href="http://www.gzhtzy.com:82/jsj/Direct/index.html" target="_new101"> -->
   <div class="slides"><!-- 切换的CSS 开始 -->
      <div class="ico"></div>  <a href="http://www.gzhtzy.com:82/jsj/Direct/index.html" target="_blank"><img src="images/1-1.jpg" ></a>
       <a href="http://www.eduknowhow.com"  target="_blank"><img src="images/1-2.jpg" ></a>      
   </div><!-- 切换的CSS 结束  -->
</div><!-- 飘的CSS 结束  -->
<script language="javascript" src="jquery-1.4.2.min.js"></script>

<!-- 以下JS是 图片切换的代码 -->
<script language="javascript">
$(function(){
 var _img=$('.slides img');
 var _len=_img.length;
 var _index=0;
 var _moving;
 var _ul='<ul>'
 for(var i=1; i<=_len; i++){
  _ul=_ul+'<li>'+'</li>';
 }
 _ul=_ul+'</ul>';
 $('div.ico').append(_ul);
 var _ico=$('.ico li');
 _ico.mouseover(function(){
  _index=_ico.index(this);
  _img.filter(':visible').fadeOut(10,function(){
   _img.eq(_index).fadeIn(10); //规定图片间的间隔
  })
  $(this).addClass('high').siblings().removeClass('high');
 }).eq(0).mouseover();
 _moving=setInterval(autoShow,2000);  //规定每张图片的间隔
 _img.hover(function(){clearInterval(_moving)},function(){
  _moving=setInterval(autoShow,2000);
 })
 function autoShow(){
  _index++;
  if(_index==_len) _index=0;
  _ico.eq(_index).trigger('mouseover');
 };
});
</script>


<!-- 以下是飘的JS代码 -->

<script>    
    var dx=1,dy=1;    
    var interval=50;    
    var intervalID=window.setInterval('moveDiv()',interval);    
    function moveDiv()    {
        if(window.closed)        {clearInterval(intervalID);}
        window.status=document.getElementById('swfDiv').style.posLeft+dx;
        //(window.screen.availWidth-160)   (window.screen.availHeight-80)
        if(((document.getElementById('swfDiv').style.posLeft+dx)>1200)||((document.getElementById('swfDiv').style.posLeft+dx)<0))//宽度
        {dx=-dx; }
        if(((document.getElementById('swfDiv').style.posTop+dy)>745)||((document.getElementById('swfDiv').style.posTop+dy)<0)) //高度
        {dy=-dy;}
        document.getElementById('swfDiv').style.posLeft += dx;
        document.getElementById('swfDiv').style.posTop += dy;    
		}
</script>