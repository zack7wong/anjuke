<script language=javascript>
function yn(s)
{
     if(!  confirm(s) )
     {
     	window.event.returnValue = false
	return false
     }
     return true
}

</script>

<script language=javascript>
function yn_del()
{
     if(!  confirm('是否删除所选数据') )
     {
     	window.event.returnValue = false
	return false
     }
     document.jcrj.act.value='del';document.jcrj.action='../../../table/table/table/del.asp';document.jcrj.submit()
}

</script>



