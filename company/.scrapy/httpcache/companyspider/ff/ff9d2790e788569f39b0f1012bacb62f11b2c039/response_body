<SCRIPT language=JavaScript>

var signover = 1;
function cmouseover(divName,over,counts,oclass){
	if(signover==1){
		for(var i=0;i<counts;i++){
			var divClassName = document.getElementById(divName+'top_'+i);
			if(divClassName.className.substr(divClassName.className.length-2,2)=="_1"){
				divClassName.className = divClassName.className.substr(0,divClassName.className.length-2);
			}
		}
		document.getElementById(divName+'top_'+over).className = document.getElementById(divName+'top_'+over).className+"_1";
		for(i=0;i<counts;i++){
			document.getElementById(divName+'_'+i).style.display = "none";
		}
		document.getElementById(divName+'_'+over).style.display = "";
	}
	signover = 0;
}
function addCookie(name,value,expireHours){
	var cookieString=name+"="+escape(value);
	//ÅĞ¶ÏÊÇ·ñÉèÖÃ¹ıÆÚÊ±¼ä
	if(expireHours>0){
		var date=new Date();
		date.setTime(date.getTime+expireHours*3600*1000); // ×ª»»ÎªºÁÃë
		cookieString=cookieString+"; expire="+date.toGMTString();
	}
	document.cookie=cookieString;
}
function getCookie(name){
	var strCookie=document.cookie;
	var arrCookie=strCookie.split("; "); // ½«¶àcookieÇĞ¸îÎª¶à¸öÃû/Öµ¶Ô
	for(var i=0;i<arrCookie.length;i++){ // ±éÀúcookieÊı×é£¬´¦ÀíÃ¿¸öcookie¶Ô
	var arr=arrCookie[i].split("="); // ÕÒµ½Ãû³ÆÎªuserIdµÄcookie£¬²¢·µ»ØËüµÄÖµ
	if(arr[0]==name)
		return arr[1];
	}
	return 0;
}

function theme_websq(num){
 
  for(var id = 1;id<=2;id++)
  {
    var content_Div="theme_webs_con"+id;
  
    if(id==num)
      document.getElementById(content_Div).style.display="block";
    else
      document.getElementById(content_Div).style.display="none";
  }  

  if(num==1) {
	document.getElementById("theme_webstitle1").className="theme_webs_title_jt";	
	document.getElementById("theme_webstitle2").className="theme_webs_title_js";	
	
  }
  if(num==2) {
	document.getElementById("theme_webstitle1").className="theme_webs_title_js";	
	document.getElementById("theme_webstitle2").className="theme_webs_title_jt";	
  }
}



</SCRIPT>
