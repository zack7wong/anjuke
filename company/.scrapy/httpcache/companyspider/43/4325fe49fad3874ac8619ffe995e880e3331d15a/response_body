    <script type="text/javascript">
    (function(){
            var ind = 0; //初始位置
            var nav= jQuery(".nav_top");
            var init = jQuery(".nav_top .m").eq(ind);
            var block = jQuery(".nav_top .block"); //滑块
            block.css({"left":init.position().left-1}); //初始化滑块位置
    
            jQuery(".nav_top").slide({ 
                    type:"menu", //效果类型
                    titCell:".m", // 鼠标触发对象
                    targetCell:".sub", // 效果对象，必须被titCell包含
                    delayTime:100, // 效果时间
                    triggerTime:0, //鼠标延迟触发时间
                    returnDefault:false,//on返回初始位置
                    defaultIndex:ind,//初始位置
                    startFun:function(i,c,s,tit){ //控制当前滑块位置
                        block.animate({"left":tit.eq(i).position().left-1},100);
                    }
                });
    })()
    </script> 
