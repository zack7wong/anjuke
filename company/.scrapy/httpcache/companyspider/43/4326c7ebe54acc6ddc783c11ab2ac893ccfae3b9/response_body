function selectTag(showContent,selfObj){
	// 操作标签
	
	var tag = document.getElementById("tags").getElementsByTagName("div");
	var taglength = tag.length;
	for(i=0; i<taglength; i++){
		tag[i].className = "off_bt";	
		
	}

	for(i=1; i<=taglength; i++){
		
		if (("tagContent"+i) == showContent)
		{
		var bb=i-1;
		tag[bb].className = "on_bt";
		if (i==1){
		document.getElementById("shijian").innerHTML ="<a href='/list/?213_1.html'>更多>></a>"
		}
		if (i==2)
		{
		document.getElementById("shijian").innerHTML ="<a href='/list/?229_1.html'>更多>></a>"
		}
		}
	}
	selfObj.parentNode.className = "selectTag";
	// 操作内容
	for(i=1; j=document.getElementById("tagContent"+i); i++){
	    
		j.style.display = "none";
		
	}
	document.getElementById(showContent).style.display = "block";
	
	
}
function aa(){
var tagt = document.getElementById("tags").getElementsByTagName("div");
	var taglength = tagt.length;
	tagt[0].className = 'on_bt';
}
</SCRIPT></head>
