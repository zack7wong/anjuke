      <DIV id="xf_left" style='left:5px;POSITION:absolute;TOP:120px; z-index:999999; text-align:right '><a href='http://www.jxslsd.com/news_detail.aspx?id=11216' target='_blank' title=""><img border=0 src='http://www.jxslsd.com/upload/image/20180314/20180314160616_0468.jpg' width='100' height='299'><br><a href=JavaScript:; onclick="leftclose()">关闭</a></div>
      
    <DIV id="xf_right" style='right:5px;POSITION:absolute;TOP:120px; z-index:999999; text-align:right' ><a href='http://www.jxslsd.com/news_detail.aspx?id=11236' target='_blank' title=""><img border=0 src='http://www.jxslsd.com/upload/image/20180328/20180328185117_3181.jpg' width='100' height='299'><br><a href=JavaScript:; onclick="rightclose()">关闭</a></div>
    
<script language="javascript">
    lastScrollY = 0;
    function leftclose() {
        document.getElementById("xf_left").style.display = 'none';
    }
    function rightclose() {
        document.getElementById("xf_right").style.display = 'none';
    }
    function heartBeat() {
        var diffY;
        if (document.documentElement && document.documentElement.scrollTop)
            diffY = document.documentElement.scrollTop;
        else if (document.body)
            diffY = document.body.scrollTop
        else
        { /*Netscape stuff*/ }
        //alert(diffY);
        percent = .1 * (diffY - lastScrollY);
        if (percent > 0) percent = Math.ceil(percent);
        else percent = Math.floor(percent);
        
        document.getElementById("xf_left").style.top = parseInt(document.getElementById("xf_left").style.top) + percent + "px";
         
        document.getElementById("xf_right").style.top = parseInt(document.getElementById("xf_right").style.top) + percent + "px";
         
      
        lastScrollY = lastScrollY + percent;
        //alert(lastScrollY);
    }
    
    window.setInterval("heartBeat()", 1);
</script>

