<script>
		// 选项卡
             function changebg(tar)
	        {
				if (tar.className == "btn-off")
				{
					$(".btn-on").attr("class","btn-off");
					$(".btn-in").attr("style","display:none");
					$("#"+tar.id).attr("class","btn-on");
					$("#it"+tar.id).attr("style","display:block");
				}
			}
	 function changebga(tar)
	        {
				if (tar.className == "btn-off")
				{
					$(".btn-on").attr("class","btn-off");
					$(".btn-in").attr("style","display:none");
					$("#"+tar.id).attr("class","btn-on");
					$("#it"+tar.id).attr("style","display:block");
				}
			}

</script>
