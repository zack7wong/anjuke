var marquee_142036283074618589 = document.getElementById("marquee_142036283074618589");
var marquee1_142036283074618589 = document.getElementById("marquee1_142036283074618589");
var marquee2_142036283074618589 = document.getElementById("marquee2_142036283074618589");
var speed=50;//控制移动的速度,数越大越慢
marquee2_142036283074618589.innerHTML=marquee1_142036283074618589.innerHTML;//把marquee1的内容复制到marquee2里面
function leftMarquee_142036283074618589(){ //向左
if(marquee_142036283074618589.scrollLeft>=marquee1_142036283074618589.scrollWidth){
marquee_142036283074618589.scrollLeft=0;
}else{
marquee_142036283074618589.scrollLeft++;
}
}
var MyMar_142036283074618589=setInterval(leftMarquee_142036283074618589,50);
marquee_142036283074618589.onmouseover=function() {clearInterval(MyMar_142036283074618589)} //鼠标移到上面停止并清除计数
marquee_142036283074618589.onmouseout=function() {
MyMar_142036283074618589=setInterval(leftMarquee_142036283074618589,50)//鼠标离开重新计数
}
</script>
