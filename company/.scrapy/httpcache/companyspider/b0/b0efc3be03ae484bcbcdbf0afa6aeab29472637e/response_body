<script type="text/javascript">
ExxQH("[edata='qh']", "[edata='qht']", "[edata='qhc']","[edata='more']", 'qiehuan1');
ExxQH('[edata="qh"]', '[edata="title"]', '[edata="content"]', '[edata="more"]', 'qiehuan');
LeftMove('[emove="LeftMove"]', false,50, 1);
$(function(){
	//标红最近两天的文章
	setFontRed("span",2);
	setFontRed("dl",2);
	
	//校园新闻中过滤头条新闻
	FilterTopnew("#hotnews tr","#topnews a");
});

//设置最近几日文字标红：用法setFontRed(2)表示最近两天的文章日期标红,data-time="20160928"
//num天数
function setFontRed(sel,num){
	var date=new Date()
	var year=date.getFullYear();
	var month=date.getMonth()+1;
	month=(month<10)?"0"+month:month;
	var day=date.getDate();
	day=(day<10)?"0"+day:day;
	var datestr=year.toString()+month.toString()+day.toString();
	for(var i=0;i<num;i++){
		$(sel+"[data-time='"+(datestr-i)+"']").css("color","red");
	}
}
	
//校园新闻中过滤头条新闻
//sel_news:选择新闻列表  sel_filter：选择头条新闻A标签
function FilterTopnew(sel_news,sel_filter){
	var id=$(sel_filter).attr("data-id");
	var deling=$(sel_news+"[data-id='"+id+"']");
	if(deling.length>0){
		$(sel_news+"[data-id='"+id+"']").remove();
	}else{
		$(sel_news+":last").remove();
	}
}

</script>

<script language="javascript">
function qihuan(a,b,c,d,e,f){
	$("#"+a).mouseover(function(){	
		$(this).removeClass(e);
		$(this).addClass(f);
		$($("#"+b).find("td:not(#"+a+")")).each(function(){
 			$(this).removeClass(f);
			$(this).addClass(e);
		});
		$($("#"+c).find("span")).each(function(){
			$(this).addClass("bjhid");
		});
		$($("#"+d).find("span")).each(function(){
			$(this).addClass("bjhid");
		});
		$("#"+a+"_content").removeClass("bjhid");
		$("#"+a+a+"_content").removeClass("bjhid");
	});
}
qihuan("new1","new_1","new_1c","new_1cc","qiehuan2","qiehuan1");
qihuan("new2","new_1","new_1c","new_1cc","qiehuan2","qiehuan1");
qihuan("new3","new_1","new_1c","new_1cc","qiehuan2","qiehuan1");
</script>
<script language="javascript">
function qihuan1(a,b,c,e,f){
	$("#"+a).mouseover(function(){	
		$(this).removeClass(e);
		$(this).addClass(f);
	$($("#"+b).find("td:not(#"+a+")")).each(function(){
 			$(this).removeClass(f);
			$(this).addClass(e);
		});
		$($("#"+c).find("span")).each(function(){
			$(this).addClass("bjhid");
		});
		
		$("#"+a+"_content").removeClass("bjhid");
			
	});
}
qihuan1("new11","new1_1","new1_1c","qiehuan12","qiehuan11");
qihuan1("new12","new1_1","new1_1c","qiehuan12","qiehuan11");
qihuan1("new13","new1_1","new1_1c","qiehuan12","qiehuan11");
qihuan1("new14","new1_1","new1_1c","qiehuan12","qiehuan11");
qihuan1("new15","new1_1","new1_1c","qiehuan12","qiehuan11");
qihuan1("new16","new1_1","new1_1c","qiehuan12","qiehuan11");
</script>