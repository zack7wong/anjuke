<!--------登录-------------------> 
<script type="text/javascript">
    function getloginmsg(obj) {
        for (o in obj) {
            if (o == "ErrorCode") {
                layer.tips(obj.ErrorMsg, '#checkbutton', {
                    tips: [1, '#ff0000'] //还可配置颜色
                });
                return;
            }
            break;
        }
        setislogin(obj);
    }
    function setislogin(obj) {
        for (o in obj) {
            if (!isNaN(o)) {
                $("#loginform").show();
                $("#isloginform").hide();
            } else {
                $("#isloginform").show();
                $("#loginform").hide();
            }
            break;
        }
        if(obj && obj.shenfen && (obj.shenfen==1 || obj.shenfen==99))
          $("#userinfo").html("您好，" + obj.truename + "！&nbsp<a href='/teacher' target='_blank'>进入后台</a>");
        else
          $("#userinfo").html("您好，" + obj.truename + "！");
		layer.closeAll();
    }
    function setloginout() {
        $("#isloginform").hide();
        $("#loginform").show();
        $("#password").val("");
    }
    $(document).ready(function () {
        var input = getcookies("exxd");
        $("#inputname").val(input);
    }); 
</script>
