  var speed=20//速度数值越大速度越慢
  index_teachers2.innerHTML=index_teachers1.innerHTML
  function Marquee(){
  if(index_teachers2.offsetWidth-index_teachers.scrollLeft<=0)
  index_teachers.scrollLeft-=index_teachers1.offsetWidth
  else{
  index_teachers.scrollLeft++
  }
  }
  var MyMar=setInterval(Marquee,speed)
  index_teachers.onmouseover=function() {clearInterval(MyMar)}
  index_teachers.onmouseout=function() {MyMar=setInterval(Marquee,speed)}
  </script>
