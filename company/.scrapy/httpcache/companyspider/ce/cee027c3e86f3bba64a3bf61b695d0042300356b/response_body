<script>
//师生风采
$(".ssfc_btn").hide().eq(0).show();

	var ssfc = 0;
	$(".cy_right_btn").click(function(){
		if(ssfc<$(this).prev(".ssfc_mbox").children("dl").children("dt").length-1){
			ssfc=ssfc+1;
			$(this).prev(".ssfc_mbox").children("dl").animate({marginLeft:-200*ssfc},1000)
		}else{}
	})
	$(".cy_left_btn").click(function(){
		if(ssfc>0){
			ssfc=ssfc-1
			$(this).next(".ssfc_mbox").children("dl").animate({marginLeft:-200*ssfc},1000)
		}else{}
	})
	
//文学天地
$(".wxtd_btn").hide().eq(0).show();

	var wxtd = 0;
	$(".wx_right_btn").click(function(){
		if(wxtd<$(this).prev(".wxtd_mbox").children("dl").children("dt").length-1){
			wxtd=wxtd+1;
			$(this).prev(".wxtd_mbox").children("dl").animate({marginLeft:-200*wxtd},1000)
		}else{}
	})
	$(".wx_left_btn").click(function(){
		if(wxtd>0){
			wxtd=wxtd-1
			$(this).next(".wxtd_mbox").children("dl").animate({marginLeft:-200*wxtd},1000)
		}else{}
	})
	
//校园动态
$(".xydt_btn").hide().eq(0).show();

	var xydt = 0;
	$(".xy_right_btn").click(function(){
		if(xydt<$(this).prev(".xydt_mbox").children("dl").children("dt").length-1){
			xydt=xydt+1;
			$(this).prev(".xydt_mbox").children("dl").animate({marginLeft:-334*xydt},1000)
		}else{}
	})
	$(".xy_left_btn").click(function(){
		if(xydt>0){
			xydt=xydt-1
			$(this).next(".xydt_mbox").children("dl").animate({marginLeft:-334*xydt},1000)
		}else{}
	})
</script>
