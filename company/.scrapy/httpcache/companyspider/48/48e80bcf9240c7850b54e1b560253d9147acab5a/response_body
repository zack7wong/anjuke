    function login(){
	var username = document.getElementById('username').value;
	var pwd = document.getElementById('password').value;
	window.open('/simas/newhome/login.jsp?agentid=0&username='+username+'&pwd='+pwd,'newwindow','top=0,left=0,toolbar=no,menubar=no,scrollbars=no, resizable=yes,location=no, status=n');
