    <td height="252" align="center" valign="top" style="padding-top:9px;"><form action="" method="post" name="Lonsun" id="Lonsun"><input name="LonsunImg_1" type="hidden" id="LonsunImg_1" value="/UploadFile/1/2018/5/29/201852947906624.jpg"><input name="LonsunTitle_1" type="hidden" id="LonsunTitle_1" value="合肥五中举行党员干部培训班开班仪式"><input name=LonsunURL_1 type=hidden id=LonsunURL_1 value="http://www.hfwz.net/DocHtml/1/2018/5/29/473614284551.html"><input name="LonsunImg_2" type="hidden" id="LonsunImg_2" value="/UploadFile/1/2018/5/29/201852924007241.jpg"><input name="LonsunTitle_2" type="hidden" id="LonsunTitle_2" value="铭记入团誓言 奋斗致敬青春"><input name=LonsunURL_2 type=hidden id=LonsunURL_2 value="http://www.hfwz.net/DocHtml/1/2018/5/29/842370384550.html"><input name="LonsunImg_3" type="hidden" id="LonsunImg_3" value="/UploadFile/1/2018/5/29/201852926885659.jpg"><input name="LonsunTitle_3" type="hidden" id="LonsunTitle_3" value="市政协委员走进五中开展“双联双创”工作..."><input name=LonsunURL_3 type=hidden id=LonsunURL_3 value="http://www.hfwz.net/DocHtml/1/2018/5/25/853452844548.html"><input name="LonsunImg_4" type="hidden" id="LonsunImg_4" value="/UploadFile/1/2018/5/24/201852440176369.jpg"><input name="LonsunTitle_4" type="hidden" id="LonsunTitle_4" value="合肥五中：积极关注亲子交往 倡导建构和..."><input name=LonsunURL_4 type=hidden id=LonsunURL_4 value="http://www.hfwz.net/DocHtml/1/2018/5/24/132269064546.html"><input name="LonsunImg_5" type="hidden" id="LonsunImg_5" value="/UploadFile/1/2018/5/23/201852316834542.jpg"><input name="LonsunTitle_5" type="hidden" id="LonsunTitle_5" value="合肥五中：积极调整心态 沉着自信迎考"><input name=LonsunURL_5 type=hidden id=LonsunURL_5 value="http://www.hfwz.net/DocHtml/1/2018/5/23/651798794543.html"></form><script language="JavaScript" type="text/JavaScript">
var imgUrl = new Array();
var imgLink = new Array();
var imgPre = new Array();
var adNum = 0;
var j = 0;
for (var i=1; i<=5; i++){
	imgUrl[i] = eval('document.all.LonsunImg_' + i + '.value');
	imgLink[i] = eval('document.all.LonsunURL_'+i+'.value');
}
for (i=1; i<=5; i++){
	if((imgUrl[i]!='')&& (imgLink[i]!='')){
		j++;
	}else{
		break;
	}
}
</script>
<table align="center" border="0" cellspacing="0" cellpadding="0"><tr><td width="287" valign="top" align="center"><a href="javascript:gotoUrl()"><img name="LonsunInit" style="FILTER:revealTrans(duration=2,transition=20);" width="287" height="194" border="0"></a></td></tr><tr><td height="1" bgcolor="#FFFFFF"></td></tr><tr><td align="right" height="14" background="/System/SysImage/ad/bg.jpg"><SCRIPT language=javascript>for (b=1;b<=j;b++){
	document.write('<img name=num'+b+' border=0 onClick=changimg('+b+') style=cursor:hand;>');
} 
</SCRIPT></td></tr>
<tr><td align="center" height="25" valign="bottom"><span id="td_Lonsun"></span></td></tr></table><script language="JavaScript" type="text/JavaScript">
function playTran(){
	if (document.all)
		LonsunInit.filters.revealTrans.play();
}
var key = 0;
function nextAd(){
	if (adNum<j) adNum++;
	else adNum = 1;
	if (key == 0){
		key = 1;
	}else if (document.all){ 
		LonsunInit.filters.revealTrans.Transition = 35;
		LonsunInit.filters.revealTrans.apply();
		playTran();
	}
	for (a=1;a<=j;a++)	{
		if (a==adNum)
			document.images["num"+a].src = "/System/SysImage/ad/gifqiehuan-"+a+"_"+a+".jpg";
		else
			document.images["num"+a].src = "/System/SysImage/ad/gifqiehuan-"+a+".gif";
	}
	document.images.LonsunInit.src = imgUrl[adNum];
	theTimer = setTimeout('nextAd()', 4000);
	td_Lonsun.innerHTML = '<a href=javascript:gotoUrl()><b>' + eval('document.all.LonsunTitle_' + adNum + '.value') + '</b></a>';
}
function changimg(m){
	if(adNum<m) adNum++ ;
	else adNum=1;
	for (a=1;a<=j;a++){
	if (a==m)
		document.images["num"+a].src="/System/SysImage/ad/gifqiehuan-"+a+"_"+a+".jpg";
	else
		document.images["num"+a].src="/System/SysImage/ad/gifqiehuan-"+a+".gif";
	}
	document.images.LonsunInit.src=imgUrl[m];
	td_Lonsun.innerHTML = '<a href=javascript:gotoUrl()><b>' + eval('document.all.LonsunTitle_' + m + '.value') + '</b></a>';
	adNum=m;
}
function gotoUrl(){
	jumpUrl = imgLink[adNum];
	jumpTarget = '_blank';
	if (jumpUrl != ''){
	if (jumpTarget != '')
		window.open(jumpUrl, jumpTarget);
	else
		location.href = jumpUrl;
	}
}
nextAd();
</script>
