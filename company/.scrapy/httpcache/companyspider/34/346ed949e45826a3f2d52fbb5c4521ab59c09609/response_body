$(function(){
          $(".nav li").mouseover(function(){
			$(".nav li .menu").hide();
			$(this).find(".menu").show(); 
			$(this).find("a").addClass("selected");
	 });
	 $(".nav li").mouseleave(function(){
			$(".nav li .menu").hide();
			$(this).find("a").removeClass("selected");
	 });

});
</script>
