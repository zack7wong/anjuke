<SCRIPT>
 lastScrollY=0;
 function fun_div_fly(){
  diffY=document.body.scrollTop;
  percent=.1*(diffY-lastScrollY);
  if(percent>0)
   percent=Math.ceil(percent);
  else
   percent=Math.floor(percent);
  document.getElementById("div_fly").style.top=parseInt(document.getElementById("div_fly").style.top.slice(0,document.getElementById("div_fly").style.top.length-2))+percent+"px";
  lastScrollY=lastScrollY+percent;
 }
 window.setInterval("fun_div_fly()",50);
</SCRIPT>


