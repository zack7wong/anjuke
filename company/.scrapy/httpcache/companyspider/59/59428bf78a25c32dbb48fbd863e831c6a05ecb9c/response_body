				<script type="text/javascript">jQuery("#xyfj").slide({ mainCell:".bd ul",effect:"leftMarquee",vis:"4",autoPlay:true,interTime:"20",trigger:"click"});

setTimeout("bannerEnter();",20000); 
function bannerEnter () {
	$(".closeVis").click();
}

			$(".closeVis").click(function(){
				if($(this).hasClass("up")){
					$(this).siblings("#xyfj").slideUp("fast");
					$(".pnBtn").fadeIn();
					$(this).removeClass("up");
					//$(this).parents(".visual").css({
					//	height: '205px',
					//});
					$(this).html('我要展开');
				}else{
					$(".closeVis").removeClass("up");
					$(this).siblings("#xyfj").slideDown('fast');
					$(".pnBtn").fadeOut();
					$(this).addClass("up");
					// $(this).parents(".visual").css({
					// 	height: '',
					// });
					$(this).html('我要收缩');
				}
			});
			// jQuery(".slider_wrap .bd li").first().before( jQuery(".slider_wrap .bd li").last() );
			// jQuery(".slider_wrap").hover(function(){ jQuery(this).find(".arrow").stop(true,true).fadeIn(300) },function(){ jQuery(this).find(".arrow").fadeOut(300) });
