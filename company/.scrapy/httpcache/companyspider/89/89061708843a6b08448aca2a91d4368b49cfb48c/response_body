var speed2=15
marquePic4.innerHTML=marquePic3.innerHTML 
function Marquee2(){ 
if(demo2.scrollLeft>=marquePic3.scrollWidth){ 
demo2.scrollLeft=0 
}else{ 
demo2.scrollLeft++ 
} 
} 
var MyMar2=setInterval(Marquee2,speed2) 
demo2.onmouseover=function() {clearInterval(MyMar2)} 
demo2.onmouseout=function() {MyMar2=setInterval(Marquee2,speed2)} 
