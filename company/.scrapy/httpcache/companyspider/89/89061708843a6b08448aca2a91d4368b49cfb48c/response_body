  <script language="javascript">
    var BodyHeight=document.body.scrollHeight;
    function ScrollTop(){
		var top=document.getElementById("da").offsetTop;
		var scrollHeight=document.documentElement.scrollTop;
		if(scrollHeight>top){
			document.getElementById("ScrollTop").setAttribute("style","position:absolute;top:"+(scrollHeight-top)+"px;right:0px;");
			}
		else
			document.getElementById("ScrollTop").setAttribute("style","position:absolute;top:0px;right:0px;");
		}
	//window.onscroll=function(){ScrollTop();};
  </script>
