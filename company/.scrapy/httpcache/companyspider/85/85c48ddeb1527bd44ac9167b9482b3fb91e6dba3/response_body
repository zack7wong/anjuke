<SCRIPT type="text/javascript">

//listCmsNotice2.size()
var curpop = 1;
var poplen = 0;
poplen = Number(1);
//poplen = Number(poplen) + 1;

function mainPopCon(vl){

	for(i=0; i < poplen ; i++){
		var pop = document.getElementById("pop"+(i+1));
		pop.style.display = "none";
	}

	if(vl == 'N'){
		if(curpop == poplen)
			curpop = 1;
		else
			curpop++;
	}else if(vl == 'P'){
		if(curpop == 1)
			curpop = 3;
		else
			curpop--;
	}else{
		curpop = vl;
	}
	pop = document.getElementById("pop"+curpop);
	pop.style.display = "block";
	PopIconSelector(curpop);
}

function PopIconSelector(num){
	for(i=0; i < poplen; i++){
		var popicon = document.getElementById("popicon"+(i+1));
		popicon.src = "images/main/"+(i+1)+"_off.gif";
	}
	popicon = document.getElementById("popicon"+num);
	popicon.src = "images/main/"+ num +"_on.gif";
}
</SCRIPT>
