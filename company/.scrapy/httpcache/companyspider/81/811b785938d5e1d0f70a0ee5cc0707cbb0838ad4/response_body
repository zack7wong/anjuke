		var mst;
		jQuery("#navIn li").hover(function(){
			var curItem = jQuery(this);
			mst = setTimeout(function(){//延时触发
					curItem.find("blockquote").fadeIn("fast");
					mst = null;
				}, 300);
		}, function(){
			if(mst!=null)clearTimeout(mst);
			jQuery(this).find("blockquote").fadeOut("fast");
		});
