                var _lb1 = document.getElementById("demol1");
                var _lb2 = document.getElementById("demol2");
                _lb2.innerHTML = _lb1.innerHTML;
                function LB_Marquee() {
                    if (_lb2.offsetWidth - _lb.scrollLeft <= 0)
                        _lb.scrollLeft -= _lb1.offsetWidth
                    else {
                        _lb.scrollLeft++;
                    }
                }
                var lb_MyMar = setInterval(LB_Marquee, 20);
                _lb.onmouseover = function () { clearInterval(lb_MyMar) };
                _lb.onmouseout = function () { lb_MyMar = setInterval(LB_Marquee, 20) };

