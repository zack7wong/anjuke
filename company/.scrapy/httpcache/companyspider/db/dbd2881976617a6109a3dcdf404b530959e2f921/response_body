    <script>

        $(function () {
            getAdvert();
        });

        function getAdvert() {
            $.ajax({
                type: "POST",
                url: "/BaShu/Home/getAdvert",
                data: {
                    advert: "2",
                },
                beforeSend: function (XMLHttpRequest) {
                },
                success: function (data) {
                    if (data.success) {
                        $.floatingAd({
                            delay: 60,
                            isLinkClosed: false,
                            ad: data.adverts,
                            onClose: function (elem) {
                            }
                        });

                    } else {
                    }

                },
                complete: function (XMLHttpRequest, textStatus) {
                },
                error: function () {
                }
            });
        }

    </script>

