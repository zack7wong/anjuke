function MM_jumpMenu(targ,selObj,restore){ //v3.0
  if(selObj.options[selObj.selectedIndex].value==null){

  }else if(selObj.options[selObj.selectedIndex].value=="1"){
alert('网站建设中');selObj.selectedIndex=0;
}
  else{
  	eval(targ+".open('"+selObj.options[selObj.selectedIndex].value+"')");
    selObj.options[0].selected = true;//选中
  if (restore) {selObj.selectedIndex=0;} 
}
}
</script>
<p style="height: 10px; display: block">&nbsp;</p>
