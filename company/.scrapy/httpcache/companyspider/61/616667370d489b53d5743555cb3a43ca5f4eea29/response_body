var Hoffset = 0;
var Woffset = 0;
var yon = 0;
var xon = 0;
var pause = false;
var interval;
document.getElementById("img_152755560129859137").style.top = yPos;
function changePos() {
width = document.documentElement.clientWidth;
height = document.documentElement.clientHeight;
Hoffset = document.getElementById("img_152755560129859137").offsetHeight;
Woffset = document.getElementById("img_152755560129859137").offsetWidth;
document.getElementById("img_152755560129859137").style.left = xPos + document.documentElement.scrollLeft+"px";
document.getElementById("img_152755560129859137").style.top = yPos + document.documentElement.scrollTop+"px";
if (yon) {
yPos = yPos + step;
}else {
yPos = yPos - step;
}
if (yPos < 0) {
yon = 1;
yPos = 0;
}
if (yPos >= (height - Hoffset)) {
yon = 0;
yPos = (height - Hoffset);
}
if (xon) {
xPos = xPos + step;
}
else {
xPos = xPos - step;
}
if (xPos < 0) {
xon = 1;
xPos = 0;
}
if (xPos >= (width - Woffset)) {
xon = 0;
xPos = (width - Woffset);
}
}
function start() {
document.getElementById("img_152755560129859137").visibility = "visible";
interval = setInterval('changePos()', delay);
}
function closeDiv(closedivid){
document.getElementById(closedivid).style.display="none";
}
start();
document.getElementById("img_152755560129859137").onmouseover=function() {clearInterval(interval)} //鼠标移到上面停止
document.getElementById("img_152755560129859137").onmouseout=function() {start();} //鼠标离开重新运动
