    <script>
        function run() {
            $(".con_box").animate({ marginTop: -30 }, 800,
			function () {
			    //第一条换到外部结尾处
			    $(".con_box a:last").after($(".con_box a:first"));
			    $(".con_box").css({ marginTop: 0 });
			});
        }
        var runInterval = setInterval(run, 1500);

        $(".con_box").mouseenter(function () {
            $(".con_box").stop();
            clearInterval(runInterval);
        }).mouseleave(function () {
            runInterval = setInterval(run, 1500);
        })
    </script>
