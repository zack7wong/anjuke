    <p class="footer_tj bc" >  今日访问量<span id="rfwl"> </span> 次>>总访问量<span id="kkhz"> </span> 次  </p>
     <script language="javascript" type ="text/javascript">
         //点击量增加 
         var xmlHttp;
         if (window.ActiveXObject) {
             xmlHttp = new ActiveXObject("Microsoft.XMLHTTP");
         }
         else if (window.XMLHttpRequest) {
             xmlHttp = new XMLHttpRequest();
         }
         var now = new Date();

         var url = "/Actions/ZDjl.ashx";
         xmlHttp.open("GET", url, true);
         xmlHttp.onreadystatechange = yang;
         xmlHttp.send(null);

         function yang() {
             if (xmlHttp.readyState == 4) {
                 if (xmlHttp.status == 200) {
                     var mes = xmlHttp.responseText;
                     var arr = mes.split('|');
                     document.getElementById("kkhz").innerHTML = arr[0];
                     document.getElementById("rfwl").innerHTML = arr[1];
                 }
             }
         }
</script>
</div>

            
