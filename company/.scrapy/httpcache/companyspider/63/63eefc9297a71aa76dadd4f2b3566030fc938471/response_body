      </div>
      <script>
var speedi=40
demo2i.innerHTML=demo1i.innerHTML
function Marqueei(){
if(demo2i.offsetWidth-demoi.scrollLeft<=0)
demoi.scrollLeft-=demo1i.offsetWidth
else{
demoi.scrollLeft++
}
}
var MyMari=setInterval(Marqueei,speedi)
demoi.onmouseover=function() {clearInterval(MyMari)}
demoi.onmouseout=function() {MyMari=setInterval(Marqueei,speedi)}
                </script></td>
