var imgUrl = new Array();
var imgLink = new Array();
var imgPre = new Array();
var adNum = 0;
var j = 0;
for (var i=1; i<=5; i++){
	imgUrl[i] = eval('document.all.LonsunImg_' + i + '.value');
	imgLink[i] = eval('document.all.LonsunURL_'+i+'.value');
}
for (i=1; i<=5; i++){
	if((imgUrl[i]!='')&& (imgLink[i]!='')){
		j++;
	}else{
		break;
	}
}
</script>
