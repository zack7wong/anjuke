	function marquee(w,l,n){switch(w){case 0:marWay[n]?TheWay=0:TheWay=1;if(l[1].offsetWidth-l[2].scrollLeft<=0){l[2].scrollLeft-=l[0].offsetWidth;}else{l[2].scrollLeft++;}break;case 1:		marWay[n]?TheWay=0:TheWay=1;if(l[1].offsetWidth-l[2].scrollLeft>=l[1].offsetWidth){l[2].scrollLeft+=l[0].offsetWidth;}else{l[2].scrollLeft--;}break;case 2:				if(l[1].offsetHeight-l[2].scrollTop<=0){l[2].scrollTop-=l[0].offsetHeight;}else{if(nh==sh){if(!nt){l[2].scrollTop = sh*Math.floor(l[2].scrollTop/sh);}nt++;			if(nt==st){nh=nt=0;}}else{l[2].scrollTop++;nh++;}}break;}}

	function _marquee(w,l,n){return function() {marquee(w,l,n);}}

	function  formatmarquee(){var o,l,num;num=arguments[0];if(arguments.length<3){o=$(arguments[1]);l=getNames(o,"m","div");l[l.length]=o;l[1].innerHTML=l[0].innerHTML;marTime[num]=setInterval(_marquee(2,l),speed);}else{o=$(arguments[1]);var o2=$(arguments[2]);l=getNames(o,"m","td");l[l.length]=o;l[1].innerHTML=l[0].innerHTML;var l1=getNames(o2,"m","img");marWay[num] = 0;	l1[0].onmouseover = function(){this.className="m";clearInterval(marTime[num]);marWay[num]=0;marTime[num] = setInterval(_marquee(marWay[num],l,num),speed);};l1[1].onmouseover=function(){this.className="m";clearInterval(marTime[num]);marWay[num]=1;marTime[num]=setInterval(_marquee(marWay[num],l,num),speed);};l1[0].onmouseout=function(){this.className="";clearInterval(marTime[num]);marTime[num]=0};l1[1].onmouseout=function(){this.className="";clearInterval(marTime[num]);marTime[num]=0};}}

