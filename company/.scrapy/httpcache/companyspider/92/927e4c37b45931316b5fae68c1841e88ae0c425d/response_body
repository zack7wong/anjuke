<script type="text/javascript">
	function showA(n){
		$(".xyxw").removeClass("content-oneC_TitNav0");
		$(".xyxw").addClass("content-oneC_TitNav1");
		$("#xyxw"+n).addClass("content-oneC_TitNav0");
		$("#xyxw"+n).removeClass("content-oneC_TitNav1");
		$(".Content1_List").hide();
		$("#Content1_List"+n).show();
		$(".xyxwMore").hide();
		$("#xyxwMore"+n).show();
	}
	function showB(n){
		$(".jsfc").removeClass("fourTitNav0");
		$(".jsfc").addClass("fourTitNav1");
		$("#jsfc"+n).addClass("fourTitNav0");
		$("#jsfc"+n).removeClass("fourTitNav1");
		$(".jsfcList").hide();
		$("#jsfcList"+n).show();
		$(".jsfcMore").hide();
		$("#jsfcMore"+n).show();
	}
</script>

