<script type="text/javascript" language="JavaScript">
	//调用向右滚动
	function $Ps(id) { return document.getElementById(id) }

	//向右滚动函数，demo包含demo1与demo2,speed是滚动速度，flag一个网页内有多个时设置为不同的任意字符。
	//by 柳永法(yongfa365)'Blog　www.yongfa365.com
	function toright(demoA, demo1B, demo2C, speeds, flags) {
		demoA = $Ps(demoA); demo1B = $Ps(demo1B); demo2C = $Ps(demo2C)
		demo2C.innerHTML = demo1B.innerHTML
		function Marquee_T() {
			if (demoA.scrollLeft <= 0) {
				demoA.scrollLeft = demo2C.offsetWidth
			}
			else {
				demoA.scrollLeft--
			}
		}
		flags = setInterval(Marquee_T, speeds)
		demoA.onmouseover = function () { clearInterval(flags); }
		demoA.onmouseout = function () { flags = setInterval(Marquee_T, speeds); }
	}
</script>
