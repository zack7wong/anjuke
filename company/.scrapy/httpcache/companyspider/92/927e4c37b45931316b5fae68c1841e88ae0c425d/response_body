<script type="text/javascript">
    function CheckInput() {
        if (document.getElementById("jr").style.display == "block") {
            document.getElementById("LoginName").value = "";
            return true;
        } else {
            var loginName = document.getElementById("LoginName").value;
            if (loginName == "") {
                alert("用户名不能为空！");
                return false;
            }
            var pwd = document.getElementById("pass").value;
            if (pwd == "") {
                alert("密码不能为空！");
                return false;
            }
        }
    }

    function getd(id) {
        $.post("Ajax.ashx?class=BUser&method=getu", { loginame: id }, function (data) {
        });
    }

    function getpas(id) {
        $.post("Ajax.ashx?class=BUser&method=getp", { pas: id }, function (data) {
        });
    }

    function onP() {
        str = document.getElementById("ContentPlaceHolder1_LoginAdmin_HiddenField1").value;
        if (str != "") {
            document.getElementById("mm1").style.display = "none";
            document.getElementById("mm2").style.display = "none";
            document.getElementById("userName").style.display = "none";
            document.getElementById("btnSubmit").style.display = "none";
            document.getElementById("jr").style.display = "block";
            document.getElementById("jrs").style.display = "block";
            document.getElementById("yhm").innerHTML = str;
        }
    }

    //提交登录到数字校园的表单
    function FormSubmit() {
        var form = document.getElementById("form1");
        form.submit();
    }

    function keydown(et) {
        var e = window.event || et;
        if (e.keyCode == 13) {
            FormSubmit();
        }
    }

    function onclickOut() {
        var loginn = document.getElementById("ContentPlaceHolder1_LoginAdmin_HiddenField1").value;
        $.post("Ajax.ashx?class=BUser&method=LogOut", { loginame: loginn }, function (data) {
            document.getElementById("ContentPlaceHolder1_LoginAdmin_HiddenField1").value = "";
            location.href = "http://szh.yqer.cn/LoginOut.aspx?ReturnUrl=http://www.hqez.net/Index.aspx";
        });
    }
</script>
