    $(document).ready(function() {
	var IsOn = -1	
	if(IsOn > 0)
	{
	   $("#login").hide();
	   $("#loginin").show();
	   $("#loginout").show();
	}
        else
        {
	   $("#login").show();
	   $("#loginin").hide();
	   $("#loginout").hide();
	}

        $("[class^='A_box']:eq(0)").show();

    });




    $('.change').soChange({
        thumbObj: '#featurenav a',
        //导航为数字形式，选择器指向包含数字的a对象 
        thumbNowClass: 'active',
        //自定义导航对象当前class为active 
        changeTime: 4000//自定义切换时间为4000ms 
    });


    $(".close").click(function() {
        $(".login_box").hide();
    });

    $(".login").click(function() {
        $(".login_box").show();
    });

    $(".loginBtn").click(function() {
        $(".login_box").hide();
    });

    function ShowSubject(SID) {
        $("#li_" + SID).show().siblings("[id]").hide();
    }


    $("#loginout").click(function() {//登出
        if (confirm("确实要退出吗？") == true) {
            $.ajax({
              url:"../../BasePlate/Interface/ICheckWS.asmx/LoginOut",
              async:false,
              dataType:"text",
              success:function(data){
                 window.location.href="273001.htm?RD="+Math.random(); 
              }    
            }); 
            return true; 
        }
        return false;
    })

    var OpenType = "flash";
    $("[name='sp']").find("img").attr("src", "")
    $("[name='sp']").click(function() {

        if ($(this).find("img").length > 0) {
            var oUrl = $(this).find("img").attr("src");
            if (oUrl.length > 0) {
                oUrl = oUrl.substr(0, oUrl.length - 3) + "flv";

                if (OpenType.length > 0) {
                    if (OpenType != "flash") $.fn.media.mapFormat("flv", OpenType);
                    $(this).media({
                        width: "328",
                        height: "218",
                        bgColor: "#000000", // background color  
                        autoplay: true,
                        src: oUrl,
                        params: { allowfullscreen: true, maintainaspect: true, controls: "ImageWindow,ControlPanel" }, // object params/embed attrs 最后一个不显示控制条，因为首页上控制条太占地方了
                        flashvars: { autostart: true }
                    });
                }
            }
        }
    });
</script>

