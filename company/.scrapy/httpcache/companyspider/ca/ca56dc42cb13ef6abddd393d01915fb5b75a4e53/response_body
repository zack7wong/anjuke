            <div class="new_level">
                <script language="JavaScript" type="text/JavaScript" xmlns:msxsl="urn:schemas-microsoft-com:xslt" xmlns:GoodoNS="urn:WT">
			var nCount=2;
			var oArray = new Array();
			function changeSubject(oTag)
			{
			for(i=0;i < nCount;i++)
			{
			var id="mt_"+oArray[i];

			if(i+1==oTag)
			{
			document.getElementById("mt_"+oArray[i]).className="";
			document.getElementById("mt_"+oArray[i]).firstChild.className="levelH";
			document.getElementById("vt_"+oArray[i]).style.display="";
			document.getElementById("kt_"+oArray[i]).style.display="";
			}
			else
			{
			document.getElementById("mt_"+oArray[i]).className="";
			document.getElementById("mt_"+oArray[i]).firstChild.className="";
			document.getElementById("vt_"+oArray[i]).style.display="none";
			document.getElementById("kt_"+oArray[i]).style.display="none";
			}
			}

			}
		</script><div class="level_bar" xmlns:msxsl="urn:schemas-microsoft-com:xslt" xmlns:GoodoNS="urn:WT"><ul><script language="JavaScript" type="text/JavaScript">
