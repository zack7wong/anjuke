var speedbb=10
demo2bb.innerHTML=demo1bb.innerHTML
function Marqueebb(){
if(demo2bb.offsetWidth-demobb.scrollLeft<=0)
demobb.scrollLeft-=demo1bb.offsetWidth
else{
demobb.scrollLeft++
}
}
var MyMarbb=setInterval(Marqueebb,speedbb)
