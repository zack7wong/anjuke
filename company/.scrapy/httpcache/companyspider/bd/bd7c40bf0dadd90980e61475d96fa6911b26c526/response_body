<script type="text/javascript">
	$(function(){$("#KinSlideshow").KinSlideshow();})
    function showContentDiv(id){
      $(".Pop-layer").each(function(index,ob){
         if(!$(ob).is(":hidden") && $(ob).attr('id')!=id){
             $(ob).hide();
          }
         if($(ob).is(":hidden") && $(ob).attr('id')==id){
             $(ob).slideDown();
          }
      });
    }
    
    function showTab(){
       $(".sub_nav .Sub-classification").toggle('fast');
    }
    
    function hideContentDiv(obj){
       $(obj).parents(".Pop-layer").slideUp();
    }
    
    var count = 2;
    function selectTag(num){
       for(var i=0;i<count;i++){
          if(i!=num){
             $("#seltag_"+i).removeClass();
             $("#moretag_"+i).hide();
             $("#contenttag_"+i).hide();
          }else{
             $("#seltag_"+i).removeClass().addClass("clcik");
             $("#moretag_"+i).show();
             $("#contenttag_"+i).show();
          }
       }
    }
</script>
