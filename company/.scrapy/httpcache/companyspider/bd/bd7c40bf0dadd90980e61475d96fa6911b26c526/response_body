<script type="text/javascript" src="/r/cms/sdfzmxhzx/stpl2/js/jquery.hoverIntent.minified.js"></script>
<script type="text/javascript">
$(function(){
	function megaHoverOver(){
	    $(this).children(".down-menu").stop().fadeTo('fast', 1).show();
        }
        function megaHoverOut(){ 
	  $(this).children(".down-menu").stop().fadeTo('fast', 0, function() {
		  $(this).hide(); 
	  });
	}
        function megaHoverOver2(){
	    $(this).children(".down-Float").stop().fadeTo('fast', 1).show();
        }
        function megaHoverOut2(){ 
	  $(this).children(".down-Float").stop().fadeTo('fast', 0, function() {
		  $(this).hide(); 
	  });
	}
        var config = {sensitivity: 2,interval:100,over: megaHoverOver,timeout: 500,out: megaHoverOut };
        var config2 = {sensitivity: 2,interval:100,over: megaHoverOver2,timeout: 500,out: megaHoverOut2 };
	$("ul.nav-left li").hoverIntent(config);
	$("div.down-menu ul li").hoverIntent(config2);
});
</script>
