                      </div>
                    <script type="text/javascript">
                        var ad1 = new AdMove("gg1");
                        ad1.Run();
                        var close=document.getElementById("closeP");
                        var gg1=document.getElementById("gg1");                        
                        close.onclick=function(){
                        gg1.style.display="none";                   
                        }
