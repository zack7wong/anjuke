    <script type="text/javascript">

        $(document).ready(function () {
            $("#div_weixin").mousemove(function () { window.clearInterval(settime) }).mouseout(function () {
                settime = setInterval("move()", 100);
            });
        });
        var windowHeight = $(window).height();
        var windowWidth = $(window).width();
        var settime;
        var currenttop = 0;
        var currentleft =0;
        var moverpx = 5;
        $(document).ready(function () {
            $("#div_weixin").css({"top":windowHeight / 2});
            currenttop = windowHeight / 2;
             settime = setInterval("move()",100);
        });
        var topmovetype = 0;
        var leftmovetype = 0;
        function move() {
            if (currenttop - moverpx >= 0 && topmovetype == 0) {
                $("#div_weixin").css({ "top": currenttop - moverpx });
                currenttop = currenttop - moverpx;
            }
            else {
                topmovetype = 1;
                if (currenttop + moverpx <= windowHeight - 120) {
                    $("#div_weixin").css({ "top": currenttop + moverpx });
                    currenttop = currenttop + moverpx;
                }
                else
                    topmovetype = 0;
            }
            if (currentleft + moverpx <= windowWidth - 135 && leftmovetype == 0) {
                $("#div_weixin").css({ "left": currentleft + moverpx });
                currentleft = currentleft + moverpx;
            }
            else {
                leftmovetype = 1;
                if (currentleft - moverpx >= 0) {
                    $("#div_weixin").css({ "left": currentleft - moverpx });
                    currentleft = currentleft - moverpx;
                }
                else
                    leftmovetype = 0;
            }
        }
    </script> 
