function swapimg(x) {
var nowimg = document.all('Image' + x);
if(!nowimg) return;
restoreimg();
nowimg.src = whiteArray[x];
}

function restoreimg() {
for(var i=0;i<11;i++) {
document.all('Image' + i).src = redArray[i];
}
}

function showtr(y) {
var nowtr = document.all('tr' + y);
if (!nowtr) return;
hidalltr();
nowtr.style.display = 'block';
}

function hidalltr() {
for(var i=0;i<11;i++) {
document.all('tr' + i).style.display = 'none';
}
}

