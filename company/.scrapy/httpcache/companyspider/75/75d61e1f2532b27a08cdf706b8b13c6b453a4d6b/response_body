var imgUrl = new Array();
var imgLink = new Array();
var imgPre = new Array();
var adNum = 0;
var j = 0;
for (var i=1; i<=5; i++){
	imgUrl[i] = eval('document.all.LonsunImg_' + i + '.value');
	imgLink[i] = eval('document.all.LonsunURL_'+i+'.value');
}
for (i=1; i<=5; i++){
	if((imgUrl[i]!='')&& (imgLink[i]!='')){
		j++;
	}else{
		break;
	}
}
</script>
<table align="center" border="0" cellspacing="0" cellpadding="0"><tr><td width="283" valign="top" align="center"><a href="javascript:gotoUrl()"><img name="LonsunInit" style="FILTER:revealTrans(duration=2,transition=20);" width="283" height="200" border="0"></a></td></tr><tr><td height="1" bgcolor="#FFFFFF"></td></tr><tr><td align="right" height="14" background="/System/SysImage/ad/bg.jpg"><SCRIPT language=javascript>for (b=1;b<=j;b++){
	document.write('<img name=num'+b+' border=0 onClick=changimg('+b+') style=cursor:hand;>');
} 
</SCRIPT></td></tr>
<tr><td align="center" height="25" valign="bottom"><span id="td_Lonsun"></span></td></tr></table><script language="JavaScript" type="text/JavaScript">
function playTran(){
	if (document.all)
		LonsunInit.filters.revealTrans.play();
}
var key = 0;
function nextAd(){
	if (adNum<j) adNum++;
	else adNum = 1;
	if (key == 0){
		key = 1;
	}else if (document.all){ 
		LonsunInit.filters.revealTrans.Transition = 23;
		LonsunInit.filters.revealTrans.apply();
		playTran();
	}
	for (a=1;a<=j;a++)	{
		if (a==adNum)
			document.images["num"+a].src = "/System/SysImage/ad/gifqiehuan-"+a+"_"+a+".jpg";
		else
			document.images["num"+a].src = "/System/SysImage/ad/gifqiehuan-"+a+".gif";
	}
	document.images.LonsunInit.src = imgUrl[adNum];
	theTimer = setTimeout('nextAd()', 5000);
	td_Lonsun.innerHTML = '<a href=javascript:gotoUrl()><b>' + eval('document.all.LonsunTitle_' + adNum + '.value') + '</b></a>';
}
function changimg(m){
	if(adNum<m) adNum++ ;
	else adNum=1;
	for (a=1;a<=j;a++){
	if (a==m)
		document.images["num"+a].src="/System/SysImage/ad/gifqiehuan-"+a+"_"+a+".jpg";
	else
		document.images["num"+a].src="/System/SysImage/ad/gifqiehuan-"+a+".gif";
	}
	document.images.LonsunInit.src=imgUrl[m];
	td_Lonsun.innerHTML = '<a href=javascript:gotoUrl()><b>' + eval('document.all.LonsunTitle_' + m + '.value') + '</b></a>';
	adNum=m;
}
function gotoUrl(){
	jumpUrl = imgLink[adNum];
	jumpTarget = '_blank';
	if (jumpUrl != ''){
	if (jumpTarget != '')
		window.open(jumpUrl, jumpTarget);
	else
		location.href = jumpUrl;
	}
}
nextAd();
</script>
