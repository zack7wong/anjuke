           <script>
var speedi=30
demoi2.innerHTML=demoi1.innerHTML
function Marqueei1(){
if(demoi2.offsetWidth-demoi.scrollLeft<=0)
demoi.scrollLeft-=demoi1.offsetWidth
else{
demoi.scrollLeft++
}
}
var MyMari1=setInterval(Marqueei1,speedi)
demoi.onmouseover=function() {clearInterval(MyMari1)}
demoi.onmouseout=function() {MyMari1=setInterval(Marqueei1,speedi)}
                </script></td>
        </tr>
