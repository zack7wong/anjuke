				<script>
					var p2089 = document.getElementById("p2089");
					var p2089_1 = document.getElementById("p2089_1");
					var p2089_2 = document.getElementById("p2089_2");
					p2089_2.innerHTML = p2089_1.innerHTML;
					var p2089_MyMar = setInterval(p2089_Marquee, 50);
					p2089.onmouseover = function() {
						clearInterval(p2089_MyMar)
					};
					p2089.onmouseout = function() {
						p2089_MyMar = setInterval(p2089_Marquee, 50)
					};
					function p2089_Marquee() {
						if (p2089_2.offsetWidth - p2089.scrollLeft <= 0) {
							p2089.scrollLeft -= p2089_1.offsetWidth;
						} else {
							p2089.scrollLeft += 1;
						}
					}
				</script>

