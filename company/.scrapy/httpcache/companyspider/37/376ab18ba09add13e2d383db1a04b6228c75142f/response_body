		lastScrollY=0;
		function heartBeat(){
		var diffY;
		if (document.documentElement && document.documentElement.scrollTop)
		diffY = document.documentElement.scrollTop;
		else if (document.documentElement)
		diffY = document.documentElement.scrollTop
		else
		{/*Netscape stuff*/}
		//alert(diffY);
		percent=.1*(diffY-lastScrollY);
		if(percent>0)percent=Math.ceil(percent);
		else percent=Math.floor(percent);
		
		document.getElementById("webAdLeft").style.top=parseInt(document.getElementById("webAdLeft").style.top)+percent+"px";
		
		document.getElementById("webAdRight").style.top=parseInt(document.getElementById("webAdRight").style.top)+percent+"px";
		
		lastScrollY=lastScrollY+percent;
		//alert(lastScrollY);
		}
