<script type="text/javascript">
    function nTabs(tabObj, obj) {
        var tabList = document.getElementById(tabObj).getElementsByTagName("li");
        for (i = 0; i < tabList.length; i++) {
            if (tabList[i].id == obj.id) {
                document.getElementById(tabObj + "_Title" + i).className = "active";
                document.getElementById(tabObj + "_Content" + i).style.display = "block";
            } else {
                var temp = document.getElementById(tabObj + "_Title" + i);
                if (temp != null) {
                    document.getElementById(tabObj + "_Title" + i).className = "normal";
                    document.getElementById(tabObj + "_Content" + i).style.display = "none";
                }
            }
        }
    }
</script>
<div class="z7 floatLeft">
    
    <div class="n4Tab" id="n4Tabctl00_NewsTab_Default_151944018847435">
        <div class="TabTitle" >
            
          <li  id="n4Tabctl00_NewsTab_Default_151944018847435_Title0" class="active" onmouseover="nTabs('n4Tabctl00_NewsTab_Default_151944018847435',this);">
