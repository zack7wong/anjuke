<!--原来的报考宣传<a class="attract" href="http://www2.jgy.gx.cn/zsxc" target="_blank"><img src="http://www.jgy.gx.cn/images/attract.jpg" alt="招生宣传"/></a>-->
        <script>
            $(function () {
                $(window).resize(function () {
                    view();
                })
                view();
                function view() {
                    var $win = $(window),
                        $winW = $win.width(),
                        $wrap = $('#wrap'),
                        $wrapW = $wrap.outerWidth(),
                        $qrcode = $('.qrcode'),
                        $qrcodeW = $qrcode.width(),
                        $attract = $('.attract'),
                        $attractW = $attract.width(),
                        offset = ($winW - $wrapW) / 2;
                    $qrcode.css({
                        left: (offset - 200) / 2
                    })
                    $attract.css({
                        right: (offset - 200) / 2
                    })
                }
            });
        </script>	<!--footer  end-->
    </div><!--wrap  end-->
