<SCRIPT language=javascript>
var Marquee_15_isMar = true;
var Marquee_15_1=document.getElementById("Marquee_15_1");
var Marquee_15_2=document.getElementById("Marquee_15_2");
var Marquee_15_3=document.getElementById("Marquee_15_3");
if (Marquee_15_1.style.pixelHeight){
    Marquee_15_isMar = Marquee_15_2.offsetHeight > Marquee_15_1.style.pixelHeight;
}else{
    var Marquee_15_height = parseInt(Marquee_15_1.style.height.replace('%', '').replace('px', ''));
    Marquee_15_isMar = Marquee_15_2.offsetHeight > Marquee_15_height;
}
if (Marquee_15_isMar){
    Marquee_15_3.innerHTML=Marquee_15_2.innerHTML;
    function Marquee_15_function(){
	     <!--if(Marquee_15_3.offsetTop-Marquee_15_1.scrollTop<=0)-->
        if(Marquee_15_2.offsetHeight*2+1==Marquee_15_1.scrollTop+Marquee_15_1.offsetHeight)
		    Marquee_15_1.scrollTop-=Marquee_15_2.offsetHeight;
	    else{
		    Marquee_15_1.scrollTop++
	    }
    }
    var Marquee_15_myMar=setInterval(Marquee_15_function,40);
    Marquee_15_1.onmouseover=function() {clearInterval(Marquee_15_myMar)}
    Marquee_15_1.onmouseout=function() {Marquee_15_myMar=setInterval(Marquee_15_function,40)}
}
</SCRIPT>
<SCRIPT language=javascript>
var Marquee_20_isMar = true;
var Marquee_20_1=document.getElementById("Marquee_20_1");
var Marquee_20_2=document.getElementById("Marquee_20_2");
var Marquee_20_3=document.getElementById("Marquee_20_3");
if (Marquee_20_1.style.pixelWidth){
    Marquee_20_isMar = Marquee_20_2.offsetWidth > Marquee_20_1.style.pixelWidth;
}else{
    var Marquee_20_width = parseInt(Marquee_20_1.style.width.replace('%', '').replace('px', ''));
    Marquee_20_isMar = Marquee_20_2.offsetWidth > Marquee_20_width;
}
if (Marquee_20_isMar){
    function Marquee_20_function(){
	    Marquee_20_3.innerHTML=Marquee_20_2.innerHTML;
	    <!--if(Marquee_20_3.offsetLeft-Marquee_20_1.scrollLeft<=0)-->
        if(Marquee_20_2.offsetWidth*2+1==Marquee_20_1.scrollLeft+Marquee_20_1.offsetWidth )
		    Marquee_20_1.scrollLeft-=Marquee_20_2.offsetWidth;
	    else{
		    Marquee_20_1.scrollLeft++
	    }
    }
    var Marquee_20_myMar=setInterval(Marquee_20_function,40);
    Marquee_20_1.onmouseover=function() {clearInterval(Marquee_20_myMar)}
    Marquee_20_1.onmouseout=function() {Marquee_20_myMar=setInterval(Marquee_20_function,40)}
}
</SCRIPT>
