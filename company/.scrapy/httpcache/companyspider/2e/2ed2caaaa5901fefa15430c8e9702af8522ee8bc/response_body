function gSearch(){
	var keyword = document.getElementById('topKeyWord').value;
	if(keyword=='' || keyword=="请输入搜索关键字"){ alert("请输入搜索关键字"); }
	else{
		window.location = '/search.aspx?searchtype=0&fieldOption=title&Keyword='+encodeURI(keyword);
	}
}
