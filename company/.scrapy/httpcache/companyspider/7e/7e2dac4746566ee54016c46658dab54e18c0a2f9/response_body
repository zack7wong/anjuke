var u_u8_imgs = new VsbPreloadImgArray();
function u_u8_init_img()
{

  u_u8_imgs.addImg("\/images\/_thumb\/ysw2018.jpg", "0", u_u8_onimgload);

  u_u8_imgs.addImg("\/images\/_thumb\/19da.jpg", "1", u_u8_onimgload);

  u_u8_imgs.addImg("\/images\/_thumb\/ysw2018.jpg", "2", u_u8_onimgload);

  u_u8_imgs.addImg("\/images\/_thumb\/dyjt.jpg", "3", u_u8_onimgload);

}
function u_u8_onimgload(img, imgid)
{
 var imgobjs = document.getElementsByName("u_u8_img");
    var imgindex = new Number(imgid);
 img.showImg(imgobjs[imgindex], 196, 55, true, true);
    img.showImg(imgobjs[imgindex+ 4], 196, 55, true, true);
}
var u_u8_MyMar;
var u_u8_speed3;
function u_u8_Marquee()
{
 try
 {
  if(document.getElementById("u_u8_demo2").offsetHeight-document.getElementById("u_u8_demo").scrollTop<=0)
  {
   document.getElementById("u_u8_demo").scrollTop-=document.getElementById("u_u8_demo1").offsetHeight
  }
  else
  {
   document.getElementById("u_u8_demo").scrollTop++;
  }
 }
 catch(e)
 {
 }
}
function u_u8_clearInterval()
{
 clearInterval(u_u8_MyMar);
}
function u_u8_setInterval()
{
 u_u8_MyMar = setInterval(u_u8_Marquee,u_u8_speed3);
}
function u_u8_init()
{
 u_u8_speed3 = 60;
 if(u_u8_speed3 < 1)
 {
  u_u8_speed3 = 1;
 }

 document.getElementById("u_u8_demo").onmouseover = u_u8_clearInterval;
 document.getElementById("u_u8_demo").onmouseout = u_u8_setInterval;
 document.getElementById("u_u8_demo2").innerHTML = document.getElementById("u_u8_demo1").innerHTML;

 u_u8_init_img();
 u_u8_setInterval();
}
u_u8_init();
</SCRIPT>
