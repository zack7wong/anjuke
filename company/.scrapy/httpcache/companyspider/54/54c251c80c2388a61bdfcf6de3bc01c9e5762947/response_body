                  //Tab控制函数
                  function tabs(tabId, tabNum) {
                  //设置点击后的切换样式
                  $(tabId + " .tab_nav li").removeClass("selected");
                  $(tabId + " .tab_nav li").eq(tabNum).addClass("selected");
                  //根据参数决定显示内容
                  $(tabId + " .tab_con").hide();
                  $(tabId + " .tab_con").eq(tabNum).show();

                  $(tabId + " .newsMore").hide();
                  $(tabId + " .newsMore").eq(tabNum).show();
                  }
