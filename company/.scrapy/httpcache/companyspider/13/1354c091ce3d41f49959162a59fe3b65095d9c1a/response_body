<script language="javascript" type="text/javascript">
    function divMMPopclose_findpwd() {
        document.getElementById("iframevideo_findpwd").src = "";
        $('#popclose_findpwd').click();
    }    
    function openfindpwdview() {
        document.getElementById("iframevideo_findpwd").src = "/findpwd.aspx";
        (function ($) {
            $(function () {
                $('#element_to_pop_up_findpwd').bPopup({
                    contentContainer: '.content',
                    modalClose: false
                });
            });
        })(jQuery);
    }
</script>

