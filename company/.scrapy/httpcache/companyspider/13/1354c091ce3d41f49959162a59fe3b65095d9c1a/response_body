                                function doLogin() {
                                    var form = document.forms[0];
                                    /*
                                    var err = "";
                                    var strrandom = form.txt_random;
                                    if (strrandom.value == "") {
                                        err = "请输入验证码！";                                    
                                    }
                                    var usename = form.j_username;
                                    var p1 = form.password;
                                    if (p1.value == "") {
                                        if (err == "") {
                                            err = "请输入用户名！";
                                        }
                                        else {
                                            err += "\n请输入用户名！";
                                        }
                                    }
                                    if (usename.value == "") {
                                        if (err == "") {
                                            err = "请输入密码！";
                                        }
                                        else {
                                            err += "\n请输入密码！";
                                        }
                                    }

                                    if (err != "") {
                                        alert(err);
                                        return;
                                    }

                                    if (strrandom.value != '6343') {
                                        alert("验证码错误，请重新输入！");
                                        return;
                                    }
                                    */
                                    var p1 = form.password;
                                    var p2 = form.j_password;
                                    if (p1.value != p2.value) {
                                        p2.value = b64_md5(p1.value) + "==";
                                    }
                                    document.forms[0].action = 'http://www.jzjyy.cn/zhxy/j_spring_security_check';
                                    document.forms[0].submit();
                                }
                                function checkLogin() {                                     

                                    var form = document.forms[0];
                                    var err = "";
                                    var strrandom = form.txt_random;
                                    if (strrandom.value == "") {
                                        err = "请输入验证码！";
                                    }

                                    var usename = form.j_username;
                                    var p1 = form.password;
                                    if (p1.value == "") {
                                        if (err == "") {
                                            err = "请输入密码！";
                                        }
                                        else {
                                            err += "\n请输入密码！";
                                        }
                                    }

                                    if (usename.value == "") {
                                        if (err == "") {
                                            err = "请输入用户名！";
                                        }
                                        else {
                                            err += "\n请输入用户名！";
                                        }
                                    }

                                    if (err != "") {
                                        alert(err);
                                        return false;
                                    }

                                    if (strrandom.value != '6343') {
                                        alert("验证码错误，请重新输入！");
                                        return false;
                                    }
                                    document.getElementById("hidUserName").value = usename.value;
                                    document.getElementById("hidUserPwd").value = p1.value;
                                    return true;
                                }
                            </script>
