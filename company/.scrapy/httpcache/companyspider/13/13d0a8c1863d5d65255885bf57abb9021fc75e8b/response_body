<script>
	var x = 50,y = 60
	var xin = true, yin = true
	var step = 1
	var delay = 20
	var obj=document.getElementById("floatWindow")
	function floatWindow() {
    var L=T=0
    var cH = document.documentElement.clientHeight == 0 ? document.body.clientHeight : document.documentElement.clientHeight
	var R= document.body.clientWidth-obj.offsetWidth
	var B = cH-obj.offsetHeight
	obj.style.left = x + document.body.scrollLeft+"px"
	obj.style.top = y + document.body.scrollTop+"px"
	x = x + step*(xin?1:-1)
	if (x < L) { xin = true; x = L}
	if (x > R){ xin = false; x = R}
	y = y + step*(yin?1:-1)
	if (y < T) { yin = true; y = T }
	if (y > B) { yin = false; y = B }
	}
	var itl= setInterval("floatWindow()", delay)
	obj.onmouseover=function(){clearInterval(itl)}
	obj.onmouseout=function(){itl=setInterval("floatWindow()", delay)}
</script>
