			     { $(this).on("click",function(){
					  $(".xuexiao-title > ul > li > a",$("#xuexiaobutton")).removeClass("active");	
					  $(".xuexiao-content",$("#xuexiaobutton")).hide();
					  $(".xuexiao-title > ul > li > a",$("#xuexiaobutton")).eq(i).addClass("active");
					  $(".xuexiao-content",$("#xuexiaobutton")).eq(i).show();
					}) 
