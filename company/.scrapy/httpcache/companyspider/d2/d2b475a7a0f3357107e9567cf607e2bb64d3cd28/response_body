<script>
var logoFilePath = new Array();
var bannerFilePath = new Array();

	logoFilePath[0] = '1476686005968.jpg';


	bannerFilePath[0] = '1447721160844.swf';

	bannerFilePath[1] = '1400141093659.swf';

var logoFileCount = logoFilePath.length;
var bannerFileCount = bannerFilePath.length;
var logoFileCurrentIndex = 0;
var bannerFileCurrentIndex = 0;

function getLogoBannerFilePath(isBanner){
	if(!isBanner){
	  if(logoFileCurrentIndex > logoFileCount - 1){
		logoFileCurrentIndex -= logoFileCount;
	  }
	  return logoFilePath[logoFileCurrentIndex++];
	}
	else {
	  if(bannerFileCurrentIndex > bannerFileCount - 1){
		bannerFileCurrentIndex -= bannerFileCount;
	  }
	  return bannerFilePath[bannerFileCurrentIndex++];
	}

}



function displayLogo(){
var jsStr='';

	
	var logoBannerFilePath = '/cnet/dynamic/maintain/banner/photo/'+getLogoBannerFilePath(false);


jsStr +=     '    <td width="250" align="left" height="145" valign="top">';
if(logoBannerFilePath.toLowerCase().search(".swf$") != -1) {
jsStr +=     '<object classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000" codebase="http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=6,0,29,0"  width="250" height="145" >                                                                    ';
jsStr +=     '  <param name="movie" value="' + logoBannerFilePath +'">                                                                                                                                                                                                                                                               ';
jsStr +=     '  <param name="quality" value="high">';

jsStr +=     '  <embed src="'+ logoBannerFilePath +'"  width="250" height="145" quality="high" pluginspage="http://www.macromedia.com/go/getflashplayer" type="application/x-shockwave-flash" width="250" height="145" ></embed></object>';

 }else {

jsStr +=     '<img src="'+ logoBannerFilePath +'"  width="250" height="145" >';
}

jsStr +=     '</td>';

	document.all('logoofunit').innerHTML = jsStr;
}


function displayBanner(){
var jsStr='';


	
	var logoBannerFilePath = '/cnet/dynamic/maintain/banner/photo/'+getLogoBannerFilePath(true);


jsStr +=     '    <td width="450" align="left" height="145" valign="top">';
if(logoBannerFilePath.toLowerCase().search(".swf$") != -1) {
jsStr +=     '<object classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000" codebase="http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=6,0,29,0"  width="450" height="145" >                                                                    ';
jsStr +=     '  <param name="movie" value="' + logoBannerFilePath +'">                                                                                                                                                                                                                                                               ';
jsStr +=     '  <param name="quality" value="high">';

jsStr +=     '  <embed src="'+ logoBannerFilePath +'"  width="450" height="145" quality="high" pluginspage="http://www.macromedia.com/go/getflashplayer" type="application/x-shockwave-flash" width="450" height="145" ></embed></object>';

 }else {

jsStr +=     '<img src="'+ logoBannerFilePath +'"  width="450" height="145" >';
}

jsStr +=     '</td>';


	document.all('bannerofunit').innerHTML = jsStr;
}
</script>

