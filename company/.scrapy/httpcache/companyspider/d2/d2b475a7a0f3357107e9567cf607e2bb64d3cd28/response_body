function check_radio(radioarray){
	if(radioarray==null){
		return true;
	}else{
		if(isNaN(radioarray.length)){//只有一个值得时候
			if(radioarray.checked == true)
				return true;
		}else{
			for(var i=0; i<radioarray.length; i++){
				if(radioarray[i].checked == true)
					return true;
			}
		}
		return false;
	}
}
