            <script type="text/javascript" src="js/wordbox.js"></script>
			<script type="text/javascript">
                $(function() {
                    var wordbox = $(".wordbox");
                    wordbox.eq(0).show();
                    $(".boxwrapleft").click(function(){
                        if(wordbox.eq(0).is(':hidden')){
                            $(this).removeClass("lcur");
