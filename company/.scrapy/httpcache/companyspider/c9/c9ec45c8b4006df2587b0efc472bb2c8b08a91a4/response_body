            <script>
			$(function(){
				navShow();
				window.onresize = function(){
					navShow();
				}
			})
			function navShow(){
					var w = $(window).width();
					if(w > 720){
						$("#webNav>li>i").unbind();
						$("#webNav>li").hover(function(){
							$(this).addClass("cur");
							$(this).children("ul").slideDown("normal");
							},
							function(){
								$("#webNav>li").removeClass("cur");
								$(this).children("ul").slideUp("fast");
							}
						)
					}else{
						$("#webNav>li").unbind();
						$("#webNav>li>i").toggle(function(e){
							$(this).parent("li").children("ul").slideDown("normal");
						},
						function(){
							$(this).parent("li").children("ul").slideUp("fast");
						})
					}
			}
			</script>
        </div>
        
