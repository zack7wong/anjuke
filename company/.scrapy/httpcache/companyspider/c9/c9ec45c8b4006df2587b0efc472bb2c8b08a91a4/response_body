			$(function(){
				var nwtNavTab = $(".nwtNavTab");
				var nwtContTab = $(".innewslistTab");
				var sInt = 0;
				
				//每五秒自动切换前两个tab
				var timer = setInterval( function(){
					if(sInt == 0){
						nwtNavTab.removeClass("cur");
						nwtNavTab.eq(sInt+1).addClass("cur");
						nwtContTab.hide();
						nwtContTab.eq(sInt+1).show();
						sInt = 1;
					}else{
						sInt = 0;
						nwtNavTab.removeClass("cur");
						nwtNavTab.eq(sInt).addClass("cur");
						nwtContTab.hide();
						nwtContTab.eq(sInt).show();
					}
				}, 5000);
				
				//初始化tab的位置
				nwtNavTab.eq(0).addClass("cur");
				nwtContTab.eq(0).show();
				
				nwtNavTab.mouseover(function(){
					clearInterval(timer);//自动切换
					var nwIn = $(this).index(".nwtNavTab");
					nwtNavTab.removeClass("cur");
					$(this).addClass("cur");
					nwtContTab.hide();
					nwtContTab.eq(nwIn).show();
				});
				
				//激活自动切换
				nwtNavTab.mouseout(function(){
					sInt = 1; //从一第个开始切换
					//每五秒自动切换前两个tab
					timer = setInterval( function(){
						if(sInt == 0){
							nwtNavTab.removeClass("cur");
							nwtNavTab.eq(sInt+1).addClass("cur");
							nwtContTab.hide();
							nwtContTab.eq(sInt+1).show();
							sInt = 1;
						}else{
							sInt = 0;
							nwtNavTab.removeClass("cur");
							nwtNavTab.eq(sInt).addClass("cur");
							nwtContTab.hide();
							nwtContTab.eq(sInt).show();
						}
					}, 5000);
				});
			})
			</script>
        </div>
        
        <div class="clear" style="height:7px;"></div>
        
