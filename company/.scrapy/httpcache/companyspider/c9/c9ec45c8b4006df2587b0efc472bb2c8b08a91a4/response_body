<script type="text/javascript">

var d=new Date();
var y=d.getFullYear();
var s=d.getHours();
var f=d.getMinutes();
var n=d.getSeconds();
if(d.getMonth()>=0 && d.getMonth()<=9){
	var m="0"+(d.getMonth()+1);
}else{
	var m=d.getMonth()+1;
}
if(d.getDate()>0 && d.getDate()<=9){
	var r="0"+d.getDate();
}else{
	var r=d.getDate();
}
var time=y+"-"+m+"-"+r+" "+s+":"+f+":"+n;
setTimeout(function(){
	if("2018-04-04 23:59:59" <= time){
		document.getElementById("gg1").style.display = "none";
	}
},10)
</script>
