<script language="javascript" type="text/javascript"> 
/*
*截取信息简介
*summaryStr:String 信息简介
*lengTh:int 截取的长度
*/
function cutSummary(summaryStr,lengTh){
	if($(window).width()>720){
		if(summaryStr.length>lengTh){
			summaryStr=summaryStr.substring(0,lengTh)+"...";
		}
	}
	document.write(summaryStr);
}
/*
*替换位置导航显示效果
*/
function glaReplace(str){
	var glaStr = str.replace('&nbsp;>&nbsp;<a href="/sy/">江西环境工程职业学院</a>&nbsp;>&nbsp;', '：<a href="/">首页</a>&nbsp;>&nbsp;');
	document.write(glaStr);
}
</script>

