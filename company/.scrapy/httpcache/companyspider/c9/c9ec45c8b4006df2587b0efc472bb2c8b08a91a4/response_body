            <script>
			$(document).ready(function(){
				var xgLink = "tLndiys",
					xgLinkUrl = $(".linkUrl");
				
				xgLinkUrl.hide();
				
				$(document).click(function(e){
					var cName = e.target.className;
					if(cName == xgLink){
						xgIns = $(e.target).index('.'+xgLink);
						xgLinkUrl.fadeOut();
						xgLinkUrl.eq(xgIns).fadeIn();
					}else{
						xgLinkUrl.fadeOut();
					}
				}); 
			})
			</script>
