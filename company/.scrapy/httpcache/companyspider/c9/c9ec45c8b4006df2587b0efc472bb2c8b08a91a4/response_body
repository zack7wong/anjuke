            <script type="text/javascript">
                 $(function(){
                    var wordbox = $(".wordbox");
                    wordbox.eq(0).show();
                    $(".boxwrapleft").click(function(){
                        if(wordbox.eq(0).is(':hidden')){
                            $(this).removeClass("lcur");
                            $(".boxwrapright").removeClass("rcur");
                            wordbox.hide();
                            wordbox.eq(0).fadeIn(200);
                        }
                    });
                    $(".boxwrapright").click(function(){
                        if(wordbox.eq(1).is(':hidden')){
                            $(this).addClass("rcur");
                            $(".boxwrapleft").addClass("lcur");
                            $(".wordbox").hide();
                            $(".wordbox").eq(1).fadeIn(200);	
                        }
                    });
				})

			</script>
			<script>
				var wapimglist=document.getElementsByClassName("box_wapimglist");
				var widths=document.documentElement.scrollWidth || document.body.scrollWidth;
				var img = new Image();
				for(i=0;i<wapimglist.length;i++){
					var box=wapimglist[i].getElementsByClassName("box");
					for(b=0;b<box.length;b++){
						var boxA=box[b].getElementsByTagName("a")[0];
						var boxImg=box[b].getElementsByTagName("img")[0];
						var boxAWidth=(widths*0.5-10);
						var boxAHeight=(boxAWidth/300)*260;
						img.src=boxImg.src;
						var boxImgHeight=((boxAWidth/img.width)*img.height);
						var boxImgWidth=((boxAHeight/img.height)*img.width);
						if(boxImgHeight<boxAHeight){
							boxImg.style.height=boxAHeight+"px";
							boxImg.style.left="-"+((boxImgWidth-boxAWidth)/2)+"px";
						}else if(boxImgHeight>boxAHeight){
							boxImg.style.width=boxAWidth+"px";
							boxImg.style.top="-"+((boxImgHeight-boxAHeight)/2)+"px";
						}
						console.log(boxImgWidth);
						boxA.style.height=boxAHeight+"px";
					}
				}
			</script>
