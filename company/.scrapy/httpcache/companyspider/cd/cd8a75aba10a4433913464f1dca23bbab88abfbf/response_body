<script type="text/javascript">
    var slider = {
        imgCnt: $(".cycle img").length
        , index: $(".cycle img").length - 1
        , distance: 50
        , moveDistance: $(".cycle").outerWidth() - $(".cycle img").length * 50
        , interval: 3000
        , t: null
        , moveState: false
        , mouseOver: false
        , autoMove: function () {
            this.moveState = true;
            this.index = (this.index + 1) % this.imgCnt;

            if (this.index > 0)
                $(".cycle img").eq(this.index).animate({ "left": (this.index * this.distance) + "px" }, function () {
                    slider.moveState = false;
                    if (!slider.mouseOver)
                        slider.t = setTimeout("slider.autoMove()", slider.interval);
                });
            else {
                for (var i = this.imgCnt - 1 ; i > 1 ; i--)
                    $(".cycle img").eq(i).animate({ "left": this.moveDistance + (i * this.distance) + "px" });

                $(".cycle img").eq(1).animate({ "left": this.moveDistance + this.distance + "px" }, function () {
                    slider.moveState = false;
                    if (!slider.mouseOver)
                        slider.t = setTimeout("slider.autoMove()", slider.interval);
                });
            }
        }
        , manualMove: function (nextIndex) {
            this.moveState = true;
            var oldIndex = this.index;
            this.index = nextIndex;

            if (this.index > oldIndex) {
                for (var i = oldIndex + 1; i < this.index; i++)
                    $(".cycle img").eq(i).animate({ "left": (i * this.distance) + "px" });

                $(".cycle img").eq(this.index).animate({ "left": (this.index * this.distance) + "px" }, function () {
                    slider.moveState = false;
                    if (!slider.mouseOver)
                        slider.t = setTimeout("slider.autoMove()", slider.interval);
                });
            }
            else {
                for (var i = oldIndex; i > this.index + 1; i--)
                    $(".cycle img").eq(i).animate({ "left": this.moveDistance + (i * this.distance) + "px" });

                $(".cycle img").eq(this.index + 1).animate({ "left": this.moveDistance + ((this.index + 1) * this.distance) + "px" }, function () {
                    slider.moveState = false;
                    if (!slider.mouseOver)
                        slider.t = setTimeout("slider.autoMove()", slider.interval);
                });
            }
        }
    };

    $(".cycle img").each(function (i) {
        $(this).css("left", i * slider.distance);
        $(this).css("width", $(".cycle").outerWidth() - (slider.imgCnt - 1) * 50);
    });
    slider.t = setTimeout("slider.autoMove()", slider.interval);

    $(".cycle").hover(function () {
        slider.mouseOver = true;
        clearTimeout(slider.t);

    }, function () {
        slider.mouseOver = false;
        if (!slider.moveState)
            slider.t = setTimeout("slider.autoMove()", slider.interval);
    });

    $(".cycle img").each(function (i) {
        $(this).hover(function () {
            if (slider.moveState)
                return;

            if (i == slider.index)
                return;

            slider.manualMove(i);
        });
    });
</script>
