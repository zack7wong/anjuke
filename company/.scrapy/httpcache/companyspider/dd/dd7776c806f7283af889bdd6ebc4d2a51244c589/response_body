				mouseover : function(){
					THIS = $(this);
					var pop_info = THIS.find(".qr");
				 
					hovertime = setTimeout(function(){
						var position = THIS.position();
						var new_left,new_top;
						var cssShadow;
						var _margin;
						//alert(position.top+" "+mem_div.height());
						new_left = position.left-THIS.height()/2;
						new_top = position.top + THIS.height();	
						pop_info.css("left",new_left);
						pop_info.css("display","block");
					/* 	if(position.left <= mem_div.width() - pop_info.width()){
							new_left = position.left;
