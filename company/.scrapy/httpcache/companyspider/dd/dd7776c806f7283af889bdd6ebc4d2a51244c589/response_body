							pop_info.find("img").css("float","left");
							pop_info.find("p").css({"float":"left","margin-left":"-50px"});
						} */
						
					},300);
				},
				mouseout: function(){
					
					THIS = $(this);
					var pop_info = THIS.find(".qr");
					 
					pop_info.css({"display":"none"});
					clearTimeout(hovertime);
					 
					
				}
				
			})
		 

	  }
	  
	  $(document).ready(function(){
			memberShow();
		}) 
