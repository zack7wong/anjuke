<script>
    function SetNavMouseOver(obj, id) {
        jQuery(".nav_tile_lever1 li").removeClass("current1");
        jQuery(obj).addClass("current1");
        jQuery("#nav_title_ContentPlaceHolder1_Compose_137749720846284_Nav_137749734548284 div[class=navtitlelist]").hide();
        jQuery("#Layer" + id).show();
    }
    
    function SetNavMouseOut(obj, id) {        
        jQuery(obj).removeClass("current1");        
        jQuery("#Layer" + id).hide();
    }
    
    function SetNavMouseOver1(obj) {        
        jQuery("#nav_title_ContentPlaceHolder1_Compose_137749720846284_Nav_137749734548284 div[class=navtitlelist]").hide();
        jQuery(obj).show();
    }

    function SetNavMouseOut1(obj) {       
        jQuery(obj).hide();
    }
</script>

