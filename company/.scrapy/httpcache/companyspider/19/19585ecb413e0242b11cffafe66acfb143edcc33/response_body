				<script type="text/javascript">
					if('' != '') {
						document.getElementById('key').value = '';
					}
					document.getElementById("txt_serach").onkeydown = function(e) {
						e = e || window.event;
						if(e.keyCode == 13) {
							gSearch();
						}
					}

					function gSearch() {
						var keyword = document.getElementById('key').value;
						if(keyword == '' || keyword == "请输入搜索关键字") {
							alert("请输入搜索关键字");
						} else {
							var s = window.location.href;
							var m = s.substring(s.lastIndexOf('/') + 1);
							if(m == "") {
								s = window.location.href + "search.html"
							} else {
								s = s.replace(m, "search.html");
							}

							window.location = s + '?Keyword=' + escape(keyword);
						}
					}
				</script>

