<!--
var speed1=20; //数字越大速度越慢
var tab=document.getElementById("demo");
var tab1=document.getElementById("demo1");
var tab2=document.getElementById("demo2");
tab2.innerHTML=tab1.innerHTML; //克隆demo1为demo2
function Marquee1(){
	if(tab2.offsetTop-tab.scrollTop<=0)//当滚动至demo1与demo2交界时
		tab.scrollTop-=tab1.offsetHeight //demo跳到最顶端
	else{
		tab.scrollTop++
	}
}

var MyMar1=setInterval(Marquee1,speed1);
tab.onmouseover=function() {clearInterval(MyMar1)};//鼠标移上时清除定时器达到滚动停止的目的
tab.onmouseout=function() {MyMar1=setInterval(Marquee1,speed1)};//鼠标移开时重设定时器

var ttab=document.getElementById("tdemo");
var ttab1=document.getElementById("tdemo1");
var ttab2=document.getElementById("tdemo2");
ttab2.innerHTML=ttab1.innerHTML; //克隆demo1为demo2
function tMarquee(){
	if(ttab2.offsetTop-ttab.scrollTop<=0)//当滚动至demo1与demo2交界时
		ttab.scrollTop-=ttab1.offsetHeight //demo跳到最顶端
	else{
		ttab.scrollTop++
	}
}

var tMyMar=setInterval(tMarquee,speed1);
ttab.onmouseover=function() {clearInterval(tMyMar)};//鼠标移上时清除定时器达到滚动停止的目的
ttab.onmouseout=function() {tMyMar=setInterval(tMarquee,speed1)};//鼠标移开时重设定时器
