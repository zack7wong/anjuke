<script language="JavaScript">
            var w51imgUrl = new Array();
            var w51imgLink = new Array();
            var w51imgTitle = new Array();

            var w51count = 0;
            var w51running = false;
            var w51index = 0;
            var w51nextAdTimer;
            var w51k = 0;
            function w51nextAd(num) {
                if (num >= 0 && num < w51count - 1) {
                    w51index = num;
                    clearTimeout(w51nextAdTimer);
                } else {
                    num = w51index;
                }
                if (w51index > w51count - 1) {
                    w51index = 0;
                }
                if (!w51running) {
                    w51running = true;

                } else if (document.all) {
                    w51imgShow.filters.revealTrans.Transition = 50;
                    w51imgShow.filters.revealTrans.apply();
                    w51imgShow.filters.revealTrans.play();
                }
                document.getElementById('w51imgShow').src = w51imgUrl[w51index];
                if (w51imgTitle[w51index] != '') {
                    document.getElementById('w51imgTitle').innerHTML = '<a href="javascript:w51openWindow();" title="' + w51imgTitle[w51index] + '">' + w51imgTitle[w51index] + '</a>';
                }
                document.getElementById('w51imgNumber').innerHTML = "";
                w51k = 0;
                for (var i = 0; i < w51count; i++) {
                    w51k++;
                    if (i != w51index) {
                        document.getElementById('w51imgNumber').innerHTML += '<a href="javascript:w51nextAd(' + i + ')" class="nomal">' + w51k + '</a>';
                    } else {
                        document.getElementById('w51imgNumber').innerHTML += '<a href="javascript:w51nextAd(' + i + ')" class="current">' + w51k + '</a>';
                    }
                }

                w51index++;
                if (w51count > 1) {
                    w51nextAdTimer = setTimeout("w51nextAd(-1)", 4000);
                }
            }

            function w51openWindow() {
                var jumpUrl = w51imgLink[w51index - 1];
                if (jumpUrl && jumpUrl != '') {
                    window.open(jumpUrl);
                }
            }

            $().ready(function() {
                try {
                    var objs = w51imgJsons;
                    w51count = objs.length;
                    if (w51count == 0) {
                        document.getElementById('w51imgShow').style.display = "none";
                        document.getElementById('w51imgNumber').style.display = "none";
                        document.getElementById('w51imgTitle').style.display = "none";
                        return;
                    }
                    for (var i = 0; i < objs.length; i++) {
                        w51imgUrl[i] = objs[i].src;
                        w51imgLink[i] = objs[i].url;
                        w51imgTitle[i] = objs[i].title;
                    }
                    w51nextAd(-1);
                } catch (e) {

                }
            });
</script>

