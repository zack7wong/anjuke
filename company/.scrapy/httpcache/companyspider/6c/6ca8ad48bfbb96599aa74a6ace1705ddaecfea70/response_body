<script language="javascript" type="text/javascript" >
function formSubmit(){
		if($("#userName").attr("value") == ""){
			alert("用户姓名不能为空，请输入！");
		}
		else if($("#userPwd").attr("value") == ""){
			alert("密码不能为空，请输入！");
		}
		else if($("#corpaccount").attr("value") == "")
		{
			alert("学校编码不能为空，请输入！");
		}
		else if($("#corpaccount").attr("value").length<9 & $("#corpaccount").attr("value")!="10000"){
			alert("学校编码不正确！");
		}
		else
		{
			if($("#savebox").attr("checked") == true)
			{
				if($("#submitForm").attr("action") == "teacher_login.htm")
				{
					setCookie('staffAccount',$("#userName").attr("value"));
				}
				else
				{
					setCookie('mobile',$("#userName").attr("value"));
				}
				setCookie($("#corpaccount").attr("name"),$("#corpaccount").attr("value"));
			}
			
			$('#submitForm').submit(0);
		}
		
	}
	

</script> 

