var pic_index=1;
var max_index=2;
var inter=null;
inter=setInterval;
function changeall() {
for (i=1;i<3;i++) {
eval("tb"+i).style.display="none";
eval("bt"+i).src="image/news_bt"+i+"1.gif";
}
}
function change(a) {
changeall();
pic_index=a;
eval("tb"+a).style.display="";
eval("bt"+a).src="image/news_bt"+a+"2.gif";
}

function writeNewsImage(url, img) {	
	var str = '<a href='+url;
		if (-1 < url.indexOf("(") || -1 < url.indexOf("javascript:") )		
			str+=' target=_self>';
		else
			str+=' target=_blank>';
		str+='<img src='+img+' width=417 height=99 border=0 onmouseout="inter=setInterval"  onmouseover="clearInterval(inter)" /></a>';
		document.write(str);
}
function writeNewsTitle(url, des) {
	var str = '<a href='+url;
	if (-1 < url.indexOf("(") || -1 < url.indexOf("javascript:") )
		str+=' target=_self ';
	else
		str+=' target=_blank ';
	str += ' class=news0601 onmouseout="javascript:inter=setInterval" onmouseover="clearInterval(inter)" >';
	str += des;
	str += '</a>';
	document.write(str);
}
