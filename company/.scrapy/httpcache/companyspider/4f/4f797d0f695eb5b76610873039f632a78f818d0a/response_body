<script type="text/javascript">
  $('.popBox').each(function() {
      var $target = $($(this).attr('code'));
      if ( !$target.length ) {
          return false;
      }

      $(this).hover(function(){
          $target.css({
              left: $(this).offset().left - $target.outerWidth() -120,
              top: $(this).offset().top + $(this).height()
          });
          $target.fadeIn();
      }, function() {
          $target.fadeOut();
      });
      
  });
</script>

