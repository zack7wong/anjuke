                      var bID=0;
                        function ShowTabs2(ID){
                        if(ID!=bID){
                        TabTitle2[bID].className="main_menu_title1";
                        TabTitle2[ID].className="main_menu_title2";
                        Tabs2[bID].style.display="none";
                        Tabs2[ID].style.display="";
                        bID=ID;
                         }
