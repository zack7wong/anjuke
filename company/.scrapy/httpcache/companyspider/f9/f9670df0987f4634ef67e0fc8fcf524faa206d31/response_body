  var speed=25
  demo12.innerHTML=demo11.innerHTML
  function Marquee11()
   {
     if(demo12.offsetWidth-demo.scrollLeft<=0)
      demo.scrollLeft-=demo11.offsetWidth
     else
       {
        demo.scrollLeft++
       }
   }
  var MyMar1=setInterval(Marquee11,speed)
