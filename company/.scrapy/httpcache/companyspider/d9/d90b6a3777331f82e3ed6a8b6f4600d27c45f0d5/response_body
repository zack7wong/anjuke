<script type="text/javascript">
(function () {
var num = $("#jsNav_2946 li").length;
$("#play_2946 ul li:first").css("display", "block");
$("#jsNav_2946 li:first").addClass("over");
var curr = 0;
$("#jsNav_2946 li").each(function (i) {
$(this).click(function () {
curr = i;
$("#play_2946 ul li").eq(i).css("z-index", "10").siblings("li").css("z-index", "1");
$("#play_2946 ul li").eq(i).fadeIn(1500).siblings("li").fadeOut(1500);
$(this).siblings("li").removeClass("over").end().addClass("over");
return false;
});
});
//自动翻
var timer = setInterval(function () {
todo = (curr + 1) % num;
$("#jsNav_2946 li").eq(todo).click();
}, 9000);

//鼠标悬停在触发器上时停止自动翻
$("#jsNav_2946 li").hover(
function () {
clearInterval(timer);
},
function () {
timer = setInterval(function () {
todo = (curr + 1) % num;
$("#jsNav_2946 li").eq(todo).click();
}, 5000);
}
);
})();
</script></div><link rel="stylesheet" type="text/css" href="http://www.fzjsxy.cn/skin/school45/cms/style.css" /><link rel="stylesheet" type="text/css" href="http://www.fzjsxy.cn/skin/school45/cms/../cms/index/index.css" />
