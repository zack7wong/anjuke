<script type="text/javascript">
    function openLink(target) {
        var url = $(target).val();
        if (url != "") {
            var deviceAgent = navigator.userAgent;
            var ios = deviceAgent.toLowerCase().match(/(iphone|ipod|ipad)/);
            if (ios) {
                document.location = url;
            }
            else {
                window.open(url);
            }
        }
    }
</script>
