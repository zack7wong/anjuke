function sheight(){//设置高度
	$("#float_div_fzxy").height($(".container").height());
}


$(function(){
	$("body a").eq(0).remove();
	moiveContent=$(".moiveContent").html();
	$(".moiveContent").remove();
	$(".spdp").css("cursor","pointer").click(function(){
		stitle=$(this).find("img").attr("title");
		tipsWindown(stitle,"iframe:"+moiveContent,640,480,"true","","true","text");
		});
		
	moiveContent_zs=$(".moiveContent_zs").html();
	$(".moiveContent_zs").remove();
	$(".btn_img_zsxcp").css("cursor","pointer").click(function(){
		tipsWindown("重庆市工业学校招生宣传片","iframe:"+moiveContent_zs,640,480,"true","","true","text");
		});

	tm = setInterval("movdiv('floatBanner')", 100);
	//dragable('floatBanner');
	$("#floatBanner").mouseover(function(){
		clearInterval(tm);
	}).mouseout(function(){
		tm = setInterval("movdiv('floatBanner')", 500);
	})

	$(".btn_close_floatBanner").click(function(){
		clearInterval(tm);
		$(".btn_close_floatBanner").remove();
		$("#floatBanner").remove();
		})

		setInterval("sheight()", 10); 
		
		function setScroll(){
			//$('.btn_fzxy ').scrollFollow({speed: 1000, offset: 284});
			$('.siderBar ').scrollFollow({speed: 1000, offset: 240});	   	   
		}
		setScroll();

	})
</script>
