            <script type="text/javascript">
var mf=myFocus;//简称
var html=mf.$('myFocus-wrap').innerHTML;//保存原html
function getParam(){//获取全部参数
	return {
		id:'myFocus',
		pattern:'mF_YSlider',
		time:3,
		trigger:'mouseover',
		width:342,
		height:264,
		txtHeight:23,
		auto:true,/*自动播放*/
		autoZoom:false,/*是否缩小居中图片以适应焦点图大小*/
		wrap:false,/*显示自带边框(如果有的话)*/
		index:0,/*开始显示的图片顺序*/
		delay:0,/*按钮悬停(mouseover)模式下的延迟*/
		//额外参数
		duration:600,/*运动速度*/
		direction:'top',/*运动方向*/
		easing:'easeOut',/*运动形式*/
		less:1,/*是否无缝*/
		chip:8,/*图切片数量(能被图高整除才有效)*/
		type:4,/*切片效果*/
		pad:32,/*图片留边宽度*/
		txtWidth:38,/*文字段宽度*/
		gray:0,/*非当前图片是否变灰*/
		direct:'random',/*切出方向*/
		turn:'random'/*翻牌方向*/
	};
}
function reHTML(){//还原
	mf.$('myFocus-wrap').innerHTML=html;
	var css=mf.$$('style')[0];
	css.parentNode.removeChild(css);
};

function apply(){//应用
	reHTML();//还原
	var p=getParam();//获取参数
	p.waiting=false;//不等待图片加载
	myFocus.set(p,true);
}
//开始设置
myFocus.set(getParam(),true);
//屏蔽IE执行误差
window.onerror=function(){return true};
</script> 
          </div>
        </div>
