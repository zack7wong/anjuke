            <script>
			 var speed = 5000;
			function scroll_news(){
				$(function(){
					$('.container_ggtz li').eq(0).fadeOut('slow',function(){
						$(this).clone().appendTo($(this).parent()).fadeIn('slow');
						$(this).remove();
					});
				});
			}
		
			var MyMar=setInterval(scroll_news,speed)
			$(function(){
			$("#ggtz,.container_ggtz,.newslist_3 li a").hover(function(){
				clearInterval(MyMar);
				},function(){
					MyMar=setInterval(scroll_news,speed);
					})
			 })
   		 </script> 
