    <script type="text/javascript">
        function showCollegebulletinOrInstitutestuff(tag) {
            if (tag == 1) {
                $("#div_collegebulletin").show();
                $("#div_institutestuff").hide();
                $("#a_collegebulletin").addClass("aTitleHover");
                $("#a_institutestuff").removeClass("aTitleHover");
            }
            else {
                $("#div_collegebulletin").hide();
                $("#div_institutestuff").show();
                $("#a_institutestuff").addClass("aTitleHover");
                $("#a_collegebulletin").removeClass("aTitleHover");
            }
        }
    </script>

	

    <script type="text/javascript">
        var menu_timer = 0;
        $(function () {
            //导航栏自适应分布算法
            var li_size = $("#menu>li").size();
            var menu_width = $("#menu").width();
            var li_width = 0;
            $("#menu>li").each(function (i) {
                li_width = li_width + $(this).width();
            });
            pa = (Math.floor((menu_width - li_width) / (li_size * 2)) - 0);
            $("#menu>li").css("padding", "3px " + pa + "px");

            //绑定事件
            $("#menu li a").not($("#menu li ul li a")).mouseover(function () {
                show_sub_menu($(this));
            });
            $("#menu li a").not($("#menu li ul li a")).click(function () {
                show_sub_menu($(this));
            });
            $("#menu li a").not($("#menu li ul li a")).mouseout(function () {
                hide_sub_menu();
            });
            $(".dropdown_pane").mouseover(function () {
                show_sub_menu($(this).prev());
            });
            $(".dropdown_pane").mouseout(function () {
                hide_sub_menu();
            });
        });
        function show_sub_menu(el) {
            clearTimeout(menu_timer);
            $(".dropdown_pane").hide();
            el.next().show();
        }
        function hide_sub_menu() {
            menu_timer = setTimeout(function () { $(".dropdown_pane").hide() }, 30);
        }

        function setTab(name, cursel, n) {
            for (i = 1; i <= n; i++) {
                var menu = document.getElementById(name + i);
                var con = document.getElementById("con_" + name + "_" + i);
                menu.className = i == cursel ? "hover" : "";
                con.style.display = i == cursel ? "block" : "none";
            }
        }
		
		function changeword1(){
			var kw=document.getElementById("textfield");
			if(kw.value=="请输入关键字查询")
				{kw.value="";}
		}
		
		function changeword2(){
			var kw=document.getElementById("textfield");
			if(kw.value=="")
				{kw.value="请输入关键字查询";}
		}		
		
    </script>
