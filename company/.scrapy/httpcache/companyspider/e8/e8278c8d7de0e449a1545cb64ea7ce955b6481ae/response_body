                <div class="gdnews">
                    <p>
                        滚动新闻&gt;<span><a id="HotNews" href=""></a>
                            <script type="text/javascript">

                                var NewsTime = 2000;	//每条新闻的停留时间
                                var TextTime = 50;		//新闻标题文字出现等待时间，越小越快

                                var newsi = 0;
                                var txti = 0;
                                var txttimer;
                                var newstimer;

                                var newstitle = new Array();	//新闻标题
                                var newshref = new Array();		//新闻链接



                                 newstitle[0] ="福建教育学院关工委编书回眸岁月";newshref[0] ="http://www.fjjyxy.com/news/SzqyW8f.htm";newstitle[1] ="我省3名校长入选成为第二期“国培计划” 中小学名校长领航班学员";newshref[1] ="http://www.fjjyxy.com/news/wPiZ9LF.htm";newstitle[2] ="我省4名教师入选成为首期“国培计划” 中小学名师领航班学员";newshref[2] ="http://www.fjjyxy.com/news/jG4BHsx.htm";newstitle[3] ="教育部公布“国培计划”中小学名师领航班培养基地名单";newshref[3] ="http://www.fjjyxy.com/news/r5dp6aG.htm";newstitle[4] ="2018年全省教师进修院校校长培训班结业";newshref[4] ="http://www.fjjyxy.com/news/VXLm2P3.htm";;
                                //newstitle[0] = "教育学院滚动新闻01！";
                                //newshref[0] = "javascript:;";

                                //newstitle[1] = "教育学院滚动新闻02!";
                                //newshref[1] = "javascript:;";

                                //newstitle[2] = "教育学院滚动新闻03！";
                                //newshref[2] = "javascript:;";

                                //newstitle[3] = "教育学院滚动新闻04！";
                                //newshref[3] = "javascript:;";

                                //newstitle[4] = "教育学院滚动新闻05";
                                //newshref[4] = "javascript:;";

                                function shownew() {
                                    var endstr = "_"
                                    hwnewstr = newstitle[newsi];
                                    newslink = newshref[newsi];
                                    if (txti == (hwnewstr.length - 1)) { endstr = ""; }
                                    if (txti >= hwnewstr.length) {
                                        clearInterval(txttimer);
                                        clearInterval(newstimer);
                                        newsi++;
                                        if (newsi >= newstitle.length) {
                                            newsi = 0
                                        }
                                        newstimer = setInterval("shownew()", NewsTime);
                                        txti = 0;
                                        return;
                                    }
                                    clearInterval(txttimer);
                                    document.getElementById("HotNews").href = newslink;
                                    document.getElementById("HotNews").innerHTML = hwnewstr.substring(0, txti + 1) + endstr;
                                    txti++;
                                    txttimer = setInterval("shownew()", TextTime);
                                }
                                shownew();

                            </script>
                        </span>
                    </p>
                </div>
