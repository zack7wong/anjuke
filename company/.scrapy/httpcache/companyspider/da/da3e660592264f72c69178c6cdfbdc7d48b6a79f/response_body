var speed5=40//速度数值越大速度越慢
de2.innerHTML=de1.innerHTML
 
function Marquee5(){
if(de2.offsetWidth-de.scrollLeft<=0)
de.scrollLeft-=de1.offsetWidth
else{
de.scrollLeft++
}
}
var MyMar5=setInterval(Marquee5,speed5)
de.onmouseover=function() {clearInterval(MyMar5)}
de.onmouseout=function() {MyMar5=setInterval(Marquee5,speed5)}
