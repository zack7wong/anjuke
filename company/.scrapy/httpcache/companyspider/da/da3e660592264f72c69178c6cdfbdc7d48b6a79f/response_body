<script language=JavaScript> 
var speed5a=40//速度数值越大速度越慢
de2a.innerHTML=de1a.innerHTML
 
function Marquee5a(){
if(de2a.offsetWidth-dea.scrollLeft<=0)
dea.scrollLeft-=de1a.offsetWidth
else{
dea.scrollLeft++
}
}
var MyMar5a=setInterval(Marquee5a,speed5a)
dea.onmouseover=function() {clearInterval(MyMar5a)}
dea.onmouseout=function() {MyMar5a=setInterval(Marquee5a,speed5a)}
</script>
