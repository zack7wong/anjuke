			}}
			var MyMarleft20179066350658 = setInterval(MarqueeLeft20179066350658, leftspeed20179066350658)
			document.getElementById("Roll20179066350658").onmouseover=function() {clearInterval(MyMarleft20179066350658)}
			document.getElementById("Roll20179066350658").onmouseout=function() {MyMarleft20179066350658=setInterval(MarqueeLeft20179066350658,leftspeed20179066350658)}
			//-->
		 </script>
   

</div>

