<script>
	function xw(n)
	  {
	  	switch(n)
		{

			case 1:
			
			document.getElementById("bq1").src="/images/bydt_blue.gif"
			document.getElementById("bq2").src="/images/tzgg_orange.gif"		
			
			document.getElementById("tab_xydt").style.display="block"
			document.getElementById("tab_tzgg").style.display="none"	
			
			break;
			case 2:
			
			document.getElementById("bq1").src="/images/bydt_orange.gif"
			document.getElementById("bq2").src="/images/tzgg_blue.gif"	
			
			document.getElementById("tab_xydt").style.display="none"
			document.getElementById("tab_tzgg").style.display="block"
			
			
			break;		
		}
	  }
	  </script>
