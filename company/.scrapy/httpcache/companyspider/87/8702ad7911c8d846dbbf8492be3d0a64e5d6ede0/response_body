	function MM_swapTopMenuRestore(td,count)
	{
		$(td).removeClass();
		$(td).addClass("top_menu"+count);
	}

	function MM_swapTopMenu2(td,count)
	{
		$(td).removeClass();
		$(td).addClass("top_menu_e"+count);
	}

function onnj(str)
		{
			if(str=="")
			{
				this.location="/default.aspx";
			}
			else
			{
				this.location="/"+str+"/default.aspx";
			}
		}
