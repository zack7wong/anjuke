var header_demox = document.getElementById("header_demo1x");
var header_demo1x = document.getElementById("header_demo11x");
var header_demo2x = document.getElementById("header_demo12x");
var speed3=20;  //数值越大滚动速度越慢
header_demo2x.innerHTML = header_demo1x.innerHTML;

//header_demo2.offsetWidth 总宽度 

//alert(header_demo2.offsetWidth +"-"+header_demo.scrollLeft);
function Marquee3(){
    if(header_demo2x.offsetWidth-header_demox.scrollLeft<=0)
        header_demox.scrollLeft-=header_demo1x.offsetWidth
    else{
        header_demox.scrollLeft++
    }
}
var MyMar3 = setInterval(Marquee3,speed3);
header_demox.onmouseover = function(){clearInterval(MyMar3)}
header_demox.onmouseout = function(){MyMar3 = setInterval(Marquee3,speed3)}
</SCRIPT>
