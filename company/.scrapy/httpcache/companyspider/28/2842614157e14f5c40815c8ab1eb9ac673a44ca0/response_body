<script language="JavaScript" type="text/javascript">
function Menu(id_num,num){
for(var i=0;i<2;i++){document.getElementById("Con_"+id_num+i).style.display="none";}
for(var i=0;i<2;i++){document.getElementById("Tab_"+id_num+i).className="";}
document.getElementById("Tab_"+id_num+num).className="userTabsOn";
document.getElementById("Con_"+id_num+num).style.display="block";
}
</script>
  <div class="left_box">
  <dl>
    <dt><em>查询系统(成绩/分班)</em></dt>
    <dd>
	<ul>
	<li>学　号：<input name="TxtStudentNum" type="text" maxlength="20" id="TxtStudentNum" class="inputtext" style="width: 100px;" /></li>
	<li>姓　名：<input name="TxtStudentName" type="text" maxlength="20" id="TxtStudentName" class="inputtext" style="width: 100px;" /></li>
	<li>入　学：<input name="TxtBirthday" type="text" maxlength="20" id="TxtBirthday" class="inputtext" style="width: 100px;" title="格式如'2008-08-08'" /></li>
	<li id="loginButton">
<input id="StuLogOn" class="inputbutton" type="submit" onclick="ScoreLogin();" value="进入查询" name="BtnLogOn"/>
</li>
	</ul>
	<div class="clearbox"></div>
  <script language="javascript" type="text/javascript">
  function ScoreLogin()
  {
    var stuentNum = document.getElementById("TxtStudentNum").value;
    var stuentName = document.getElementById("TxtStudentName").value;
    var birthday = document.getElementById("TxtBirthday").value;
    if (stuentNum == '') {
        alert("请填写学号！");
        document.getElementById("TxtStudentNum").focus();
        return;
    }
    if (stuentName == '') {
        alert("请填写姓名！");
        document.getElementById("TxtStudentName").focus();
        return;
    }
    if (birthday == '') {
        alert("请填写生日！");
        document.getElementById("TxtBirthday").focus();
        return;
    }
    window.location.href = "/Ssms/ScoreQuery.aspx?num="+escape(stuentNum)+"&name="+escape(stuentName)+"&birthday="+birthday;
  }
  </script>
    </dd>
	<dd class="bottom"></dd>
  </dl>
  
</div>
