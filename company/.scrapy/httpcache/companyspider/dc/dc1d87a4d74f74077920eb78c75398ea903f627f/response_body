var speed=30
demo7.innerHTML=demo6.innerHTML
function Marquee(){
if(demo7.offsetWidth-demo8.scrollLeft<=0)
demo8.scrollLeft-=
demo6.offsetWidth
else{
demo8.scrollLeft++
}}
var MyMar=setInterval(Marquee,speed)
demo8.onmouseover=function() {clearInterval(MyMar)}
demo8.onmouseout=function() {MyMar=setInterval(Marquee,speed)}
