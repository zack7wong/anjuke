<script language="javascript">
function CheckLogin(){
  var taget_obj = document.getElementById('_loginform');
  myajax = new DedeAjax(taget_obj,false,false,"","","");
  myajax.SendGet2("/eduweb/member/loginsta.php");
  DedeXHTTP = null;
}
function CheckForm(){
			 //用户名
			 if (loginform.userid.value==""||loginform.pwd.value=="")
			{
				alert("请输入用户名及密码！");
				loginform.userid.focus();
				return (false);
			}
			 if (loginform.pwd.value=="888888")
			{
				alert("请更改密码，否则可能出现安全问题！！");
			}
			//附加码
			/*if (theForm.checknum.value=="")
			{
				alert("请输入下面显示的附加码！");
				theForm.checknum.focus();
				return (false);
			}*/
			return true;
		}
		function doSubmitByClickImage(theForm){
			if(CheckForm()){
				theForm.submit();
			}
		}
</script>
