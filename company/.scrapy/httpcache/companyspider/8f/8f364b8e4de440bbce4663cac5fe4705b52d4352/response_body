<script type="text/javascript">
    $(function () {
        $('#change_32 div.changeDiv').soChange({//对象指向层，层内包含图片及标题
            thumbObj: '#change_32 .ul_change_a2 li',
            thumbNowClass: 'on'//自定义导航对象当前class为on
        });
