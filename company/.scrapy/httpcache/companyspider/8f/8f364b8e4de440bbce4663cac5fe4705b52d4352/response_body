	function showN(n){
		$(".xyxw").removeClass("Box1R_newTit1");
		$(".xyxw").addClass("Box1R_newTit2");
		$("#xyxw"+n).addClass("Box1R_newTit1");
		$("#xyxw"+n).removeClass("Box1R_newTit2");
		$(".xyxwMore").hide();
		$("#xyxwMore"+n).show();
		$(".xyxwNote").hide();
		$("#xyxwNote"+n).show();
	}
	function showP(n){
		$(".homePic").removeClass("Box3R_PicTit1");
		$(".homePic").addClass("Box3R_PicTit2");
		$("#homePic"+n).addClass("Box3R_PicTit1");
		$("#homePic"+n).removeClass("Box3R_PicTit2");
		$(".homePicNote").hide();
		$("#homePicNote"+n).show();
		$(".homePicMore").hide();
		$("#homePicMore"+n).show();
	}
</script>

