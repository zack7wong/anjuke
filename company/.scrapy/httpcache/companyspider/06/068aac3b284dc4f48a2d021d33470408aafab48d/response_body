<script type="text/javascript">
   var sideFixed = $(".side-fixed");
   $(document).ready(function(){if($(window).width()>1366){sideFixed.css({marginRight:"-670px",right:"50%"});}else{sideFixed.css({marginRight:"0",right:"0"});}});
   $(window).resize(function(){if($(window).width()>1366){sideFixed.css({marginRight:"-670px",right:"50%"});}else{sideFixed.css({marginRight:"0",right:"0"});}});

    //返回顶部
    var backtop = $(".gt");
    var winH,winT;
    var winFun = function(){
        winH = $(window).height()
        winT = $(document).scrollTop();	
        if( winT > 100 ){ backtop.fadeIn(); }else{ backtop.fadeOut(); }
    }
    backtop.click(function(){ $("html,body").animate({ scrollTop:0 },200) });
    $(window).scroll(winFun)
    $(window).resize(winFun);
</script>
