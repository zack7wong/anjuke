<script type="text/javascript">
function changesize() {
var w = $(window).width();
    var h = w * 0.520833333;
    
    $(".dd").css({ 
        position: "absolute", 
        left: 0,
        top: ($(window).height() - h )/2
    }).attr("width", w).attr("height",h);
}
$(window).resize(function(){ 
            changesize();
});
changesize();
</script>


