               <script>
  var speed=20//速度数值越大速度越慢
  index_photo2.innerHTML=index_photo1.innerHTML
  function Marquee(){
  if(index_photo2.offsetWidth-index_photo.scrollLeft<=0)
  index_photo.scrollLeft-=index_photo1.offsetWidth
  else{
  index_photo.scrollLeft++
  }
  }
  var MyMar=setInterval(Marquee,speed)
  index_photo.onmouseover=function() {clearInterval(MyMar)}
  index_photo.onmouseout=function() {MyMar=setInterval(Marquee,speed)}
  </script>
