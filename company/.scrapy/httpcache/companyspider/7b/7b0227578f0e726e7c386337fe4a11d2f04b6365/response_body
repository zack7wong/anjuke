		lastScrollY=0;
		function heartBeat(){
		var diffY;
		if (document.documentElement && document.documentElement.scrollTop)
		diffY = document.documentElement.scrollTop;
		else if (document.documentElement)
		diffY = document.documentElement.scrollTop
		else
		{/*Netscape stuff*/}
		//alert(diffY);
		percent=.1*(diffY-lastScrollY);
		if(percent>0)percent=Math.ceil(percent);
		else percent=Math.floor(percent);
		
		document.getElementById("webAdLeft").style.top=parseInt(document.getElementById("webAdLeft").style.top)+percent+"px";
		
		document.getElementById("webAdRight").style.top=parseInt(document.getElementById("webAdRight").style.top)+percent+"px";
		
		lastScrollY=lastScrollY+percent;
		//alert(lastScrollY);
		}
		var webAdLeft = "<DIV id=\"webAdLeft\" style='left:20px;POSITION:absolute;TOP:69px;'>&nbsp;<a href=\"/webschool/News/news_detail.jsp?newsId=100007555&amp;pnType=\" target=\"_blank\"><img alt=\"\" src=\"/webschool/xheditor/upload/10001970/201707/admin5/News/img/创建全国文明城市2.jpg\" /></a><a href=\"/webschool/News/news_detail.jsp?newsId=100007555&amp;pnType=\"></a><a href=\"/webschool/News/news_detail.jsp?newsId=100007143&amp;siteId=0&amp;pnType=news34\"></a><a href=\"/webschool/News/news_detail.jsp?newsId=100007143&amp;siteId=0&amp;pnType=news34\"></a><br><a href=JavaScript:; onclick=\"document.getElementById('webAdLeft').style.display = 'none'\"><img border=0 src=/webschool/News/img/ad_close.gif></a></div>";
		var webAdRight = "<DIV id=\"webAdRight\" style='right:18px;POSITION:absolute;TOP:69px;'><p><a href=\"/webschool/News/news_detail.jsp?newsId=100007654\" target=\"_blank\"><img alt=\"\" src=\"/webschool/xheditor/upload/10001970/201712/admin5/News/img/80校庆.jpg\" /></a><a href=\"/webschool/News/news_detail.jsp?newsId=100007621\"></a></p><br><a href=JavaScript:; onclick=\"document.getElementById('webAdRight').style.display = 'none'\"><img border=0 src=/webschool/News/img/ad_close.gif></a></div>";
		
