<script>
$(document).ready(function(){
	$(".TD1").hover(function(){
		$(this).find("div").stop().animate({left:10},300);
	},function(){
		$(this).find("div").stop().animate({left:0},300);
	});
	$(".TD3").hover(function(){
		$(this).find("div").stop().animate({left:10},300);
	},function(){
		$(this).find("div").stop().animate({left:0},300);
	});
	$(".TD6").hover(function(){
		$(this).css("background-color","#F9F9F9");
		$(this).find("div").stop().animate({left:10},300);

	},function(){
		$(this).css("background-color","");
		$(this).find("div").stop().animate({left:0},300);
	});

	 var move = 0;
	//缩放比例，1.2 =120％
	var zoom = 1.2;
	$('.itemA').hover(function(){
		//根据缩放百分比设置宽度和高度
		var width = $('.itemA').width() * zoom;
		var height = $('.itemA').height() * zoom;
		//移动和缩放图像
		$(this).find('img').stop(false,true).animate({'width':width, 'height':height, 'top':move, 'left':move}, {duration:300});
		//显示标题
		$(this).find('div.captionA').stop(false,true).fadeOut(300);
	},function(){
		//复位图像
		var width = $(this).find('img').width();
		var height = $(this).find('img').height();
		$(this).find('img').stop(false,true).animate({'width':54, 'height':66, 'top':'2', 'left':'2'}, {duration:150});
		//隐藏标题
		$(this).find('div.captionA').stop(false,true).fadeIn(150);
	});
});
</script>
