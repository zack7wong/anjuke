<script type="text/javascript">


$(function(){
	//拉出子菜单
	$(".main-menu-item").mouseenter(function(){
		//$(".inAdBox").slideUp("fast");
		$(".inAdBox").hide();
		$("#" + $(this).attr("subid")).slideDown(500);
		
	});
	
	//子菜单回缩
	$(".inAdBox").mouseleave(function(){$(this).slideUp("fast");});
	
	//首屏招生小图片变化效果
	$("#inAdZs img").mouseenter(function(){
		$(this).removeClass("alpha60");
	});
	$("#inAdZs img").mouseleave(function(){
		$(this).addClass("alpha60");
	});
	
	//学生活动背景改变效果
	$(".studentActivity").mouseenter(function(){
		$(this).find("a").addClass("alpha70b").addClass("active");
	});
	$(".studentActivity").mouseleave(function(){
		$(this).find("a").removeClass("alpha70b").removeClass("active");
	});
	
	//专题网站背景变化效果
	$(".sp a").mouseenter(function(){
		$(this).removeClass("alpha60b");
		$(this).addClass("bgBlue");
	});
	$(".sp a").mouseleave(function(){
		$(this).addClass("alpha60b");
		$(this).removeClass("bgBlue");
	});
	
	//面板事件
	$("#inAdPanel").mouseleave(function(){ $(".p-subArea").hide("fast");});
	$(".p-menu li").mouseenter(function(){
		var I=$(this);
		if(I.attr("data-id")!=undefined)
			$(".p-subArea").html($("#" + I.attr("data-id")).html());
		else
			$(".p-subArea").load($(this).attr("data-url"));
		$(".p-subArea").show();
	});
	
	
	//课程试学
	$(".vl-name a").click(function(){
		var data=$(this).attr("data-url");
		var html="<iframe frameborder=\"no\" style=\" width:100%; height:305px;\" allowtransparency=\"true\" scrolling=\"auto\" src=\"http://www.nblll.cn/Player.aspx?play=1&URL=" + data + "\"></iframe>";
		$("#v-box").html(html);
		$("#v-name").html("正在播放：" + $(this).html());
	});
	
	
	//频道加载
	if($(".channelLink").length>0)
	{
		loadChannel();
	}
	
	//频道选择链接点击后的动作
	$(".channelLink").click(function(){
		var url="/Apps/PowerCMS/Channel.aspx?contents=true";
		var id=$(this).attr("channel");
		url=url + " #" + id;
		
		var cover=url + "-cover";
		$("#coverbox").load(cover);
		$("#channelbox").load(url);
	})
	
	$(".helps-1").addClass("current");
	switchHelps();
	
	
	//新闻切换
	$(".newsControlBar").click(function(){
		var I=$(this);
		if(!I.hasClass("Current")){
			var boxname=I.attr("newsbox");
			//隐藏原内容
			$("#" + boxname + " h2 a").addClass("hide");
			$("#" + boxname + " ul").addClass("hide");
			//显示新内容
			var more=I.attr("newsmore");
			var list=I.attr("newslist");
			$("#" + more).removeClass("hide");
			$("#" + list).removeClass("hide");
			//改变控制栏
			$("#" + boxname + " div.newsControlBar").removeClass("Current");
			I.addClass("Current");
		}
	});
	
	
	//招生信息与服务最后一行边框隐掉
	$("#b3ul1 li:last").addClass("last");
	
	//QQ在线查询
	//腾讯接口改变，已无效
	/*
	qqq=new QQOnlineQuery
	(
		[
			{Id:"QQ1",QQ:"472600499",Online:true,Descripting:"教务处咨询QQ",site:"www.nbtvu.net.cn"},
			{Id:"QQ2",QQ:"42575498",Online:true,Descripting:"开放网络学院咨询QQ",site:"www.nbtvu.net.cn"},
			{Id:"QQ3",QQ:"605013356",Online:true,Descripting:"成人教育学院咨询QQ",site:"www.nbtvu.net.cn"},
			{Id:"QQ5",QQ:"5120902",Online:true,Descripting:"继续教育学院咨询QQ",site:"www.nbtvu.net.cn"},
			{Id:"QQ6",QQ:"492400629",Online:true,Descripting:"文法系咨询QQ",site:"www.nbtvu.net.cn"},
			{Id:"QQ7",QQ:"9481677",Online:true,Descripting:"外语系咨询QQ",site:"www.nbtvu.net.cn"},
			{Id:"QQ8",QQ:"2351043670",Online:true,Descripting:"经管系咨询QQ",site:"www.nbtvu.net.cn"},
			{Id:"QQ9",QQ:"809121501",Online:true,Descripting:"信息系咨询QQ",site:"www.nbtvu.net.cn"}
		]
	).Query();
	*/
	
	//加载标识证书
	$.getScript("http://dcs.conac.cn/js/12/187/1398/60417986/CA121871398604179860001.js");
	
	//漂浮广告
	//flyingAdInit();
	
	
});

</script>

