function startTime (obj) {
var temp,
tempStr,
weekDay=["星期日","星期一", "星期二", "星期三", "星期四", "星期五", "星期六"] ,
now=new Date(),
year=now.getFullYear(),
month=now.getMonth()+1,
day=now.getDate(),
dayIndex=now.getDay(),
week=weekDay[dayIndex],
h=now.getHours(),
min=now.getMinutes(),
sec=now.getSeconds(),
tempStr=year+"年"+checkTime(month)+"月"+checkTime(day)+"日"+"&nbsp;"+week;
document.getElementById(obj).innerHTML=tempStr;
var args=arguments;
setTimeout(function(){
args.callee(obj)
},1000);
function checkTime (i) {
i=i<10?"0"+i:i;
return i;
}
}
startTime("Time");
</script>
