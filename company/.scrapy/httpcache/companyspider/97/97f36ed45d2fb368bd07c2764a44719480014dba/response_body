    <td width="28" height="73"><img src="/News/images/btn_prev.jpg" width="14" height="59" alt="点住不放可以快速向左滚动" border="0"  class="opacity" style="cursor:hand;" onMouseDown="runover();runstar(-3,2,2)" onMouseUp="runover();runstar(-1,20,2)" onMouseOver="runstar(-1,20,2);x_down(this)" onMouseOut="runover();x_up(this)"/></td>
    <td width="100%" align="center" valign="middle"><iframe src="/News/links.jsp" allowtransparency="true" name="frm_piclist" width="100%" marginwidth="0" height="73" marginheight="0" scrolling="No" frameborder="0" id="frm_piclist" border="0" framespacing="2" noresize="noresize" vspace="0"></iframe></td>
    <td width="28" align="right"><img src="/News/images/btn_next.jpg" alt="点住不放可以快速向右滚动"  width="14" height="59" border="0" class="opacity" style="cursor:hand;" onMouseDown="runover();runstar(3,2,2)" onMouseUp="runover();runstar(1,20,2)" onMouseOver="runstar(1,20,2);x_down(this)" onMouseOut="runover();x_up(this)" /></td>
    <script>
function runstar(a,time,flag){
	if (1 == flag){runx=setInterval("run("+a+")",10)}
	else{runx=setInterval("run2("+a+")",10)}
}
function runover(){
clearInterval(runx)
}
function run(a){
scrollx=frm_frdlist.document.body.scrollLeft
scrolly=frm_frdlist.document.body.scrollTop
scrollx=scrollx+a
frm_frdlist.window.scroll(scrollx,scrolly)
}
function run2(a){
scrollx=frm_piclist.document.body.scrollLeft
scrolly=frm_piclist.document.body.scrollTop
scrollx=scrollx+a
frm_piclist.window.scroll(scrollx,scrolly)
}
function x_down(theobject){
	object=theobject
	
	while(object.filters.alpha.opacity>60){
		object.filters.alpha.opacity+=-60
	}
}
function x_up(theobject){
	object=theobject
	while(object.filters.alpha.opacity<60){
		object.filters.alpha.opacity+=60
	}
}
function wback(){
	if(frm_frdlist.history.length==0){window.history.back()}
	else{frm_frdlist.history.back()}
}
</script>
  </tr>
</table>
