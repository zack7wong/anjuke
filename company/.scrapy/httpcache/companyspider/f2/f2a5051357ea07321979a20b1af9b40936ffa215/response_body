    var _area = function(obj, action) {
        $obj = $(obj);
        if (action == "show") {
            $obj.addClass("active").find(".sub").stop(1,1).slideDown(100);
        } else {
            $obj.removeClass("active").find(".sub").stop(1,1).slideUp(100);
        }
    }
    $("#footNav").find(".link2 .area").hover(function(){
        _area(this, "show");
    },function(){
        _area(this, "hide");
    });
    $("#footNav").find(".link2 .area .head").click(function(){
        if ($(this).parent().hasClass("active")) {
            _area($(this).parent(), "hide");
        } else {
            _area($(this).parent(), "show");
        }
    })
