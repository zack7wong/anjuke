    <script type="text/javascript" language="javascript">

        var sliderDiretion = true;
        var Interval = null;
        var sliderImgWidth = 750;
        $(function () {
            RegisterTabEvt();
            IMGTabPos();
        });


        function RegisterTabEvt() {
            $("#IMGTab div.menuRounded a.rounded").live("click", function () {
                clearInterval(Interval);

                var cIndex = $(this).prevAll().length;

                var $active = null;

                if (cIndex == 0) {
                    sliderDiretion = false;
                    $active = $("#IMGTab > div.m-img > div:eq(" + (cIndex + 1) + ")");
                    $active.addClass("active").siblings().removeClass("active");
                } else if (cIndex == $(this).parents(".menuRounded").find("a.rounded").length - 1) {
                    sliderDiretion = true;
                    $active = $("#IMGTab > div.m-img > div:eq(" + (cIndex - 1) + ")");
                    $active.addClass("active").siblings().removeClass("active");
                } else {
                    if (sliderDiretion) {
                        $active = $("#IMGTab > div.m-img > div:eq(" + (cIndex - 1) + ")");
                        $active.addClass("active").siblings().removeClass("active");
                    } else {
                        $active = $("#IMGTab > div.m-img > div:eq(" + (cIndex + 1) + ")");
                        $active.addClass("active").siblings().removeClass("active");
                    }
                }

                IMGTabSlider();

                Interval = setInterval(function () {
                    IMGTabSlider();
                }, 4000);
            });
        }

        function IMGTabPos() {
            $("#IMGTab > div.m-img > div.img").each(function (idx) {
                if (idx != 0) {
                    $(this).hide();
                }
                $(this).css({
                    left: idx * sliderImgWidth,
                    top: 0
                });
            });


            Interval = setInterval(function () {
                IMGTabSlider();
            }, 4000);
        };

        function IMGTabSlider() {
            var $slider = $("#IMGTab > div.m-img > div.img");
            var $active = $("#IMGTab > div.m-img > div.active");
            var activeIndex = $slider.index($active);

            if (activeIndex == 0) {
                sliderDiretion = true;
                $active.removeClass("active");
                $active.fadeOut(1800);
                $active.animate({
                    left: -sliderImgWidth,
                    top: 0
                }, 1000);

                $active.nextAll().each(function (idx) {
                    $(this).animate({
                        left: idx * sliderImgWidth,
                        top: 0
                    }, 500);
                });

                $active.next().addClass("active").show();

                $("#IMGTab div.menuRounded a.rounded").eq(activeIndex + 1)
                        .addClass("current").siblings().removeClass("current");

            } else if (activeIndex == $slider.length - 1) {
                sliderDiretion = false;
                $active.removeClass("active");
                $active.fadeOut(1800);
                $active.animate({
                    left: sliderImgWidth,
                    top: 0
                }, 1000);

                $active.prevAll().each(function (idx) {
                    $(this).animate({
                        left: idx * sliderImgWidth * (-1),
                        top: 0
                    }, 500);
                });

                $active.prev().addClass("active").show();

                $("#IMGTab div.menuRounded a.rounded").eq(activeIndex - 1)
                        .addClass("current").siblings().removeClass("current");
            } else {
                if (sliderDiretion) {
                    $active.removeClass("active");
                    $active.fadeOut(1800);
                    $active.animate({
                        left: -sliderImgWidth,
                        top: 0
                    }, 1000);

                    $active.nextAll().each(function (idx) {
                        $(this).animate({
                            left: idx * sliderImgWidth,
                            top: 0
                        }, 500);
                    });

                    $active.prevAll().each(function (idx) {
                        $(this).animate({
                            left: (idx + 1) * sliderImgWidth * (-1) - sliderImgWidth,
                            top: 0
                        }, 500);
                    });

                    $active.next().addClass("active").show();

                    $("#IMGTab div.menuRounded a.rounded").eq(activeIndex + 1)
                            .addClass("current").siblings().removeClass("current");
                } else {
                    $active.removeClass("active");
                    $active.fadeOut(1800);
                    $active.animate({
                        left: sliderImgWidth,
                        top: 0
                    }, 1000);

                    $active.nextAll().each(function (idx) {
                        $(this).animate({
                            left: (idx + 1) * sliderImgWidth,
                            top: 0
                        }, 500);
                    });

                    $active.prevAll().each(function (idx) {
                        $(this).animate({
                            left: idx * sliderImgWidth * (-1),
                            top: 0
                        }, 500);
                    });

                    $active.prev().addClass("active").show();

                    $("#IMGTab div.menuRounded a.rounded").eq(activeIndex - 1)
                            .addClass("current").siblings().removeClass("current");
                }
            }
        }



        $(function () {
            $("#lk_wcms_edit_topnav > ul > li").hover(
                function () {
                    $(this).children("span").removeClass("menuon");
                    $(this).children("span").addClass("menuoff");
                    $(this).children("ul").show();
                },
                function () {
                    $(this).children("span").removeClass("menuoff");
                    $(this).children("span").addClass("menuon");
                    $(this).children("ul").hide();
                });

            $("#lk_wcms_edit_topnav a").hover(
                function () {
                    $(this).removeClass("off");
                    $(this).addClass("on");
                }, function () {
                    $(this).addClass("off");
                    $(this).removeClass("on");
                });


        });


    </script>

        </div>
