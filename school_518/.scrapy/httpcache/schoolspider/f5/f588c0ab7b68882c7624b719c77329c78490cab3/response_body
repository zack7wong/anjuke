<script type="text/javascript">
function GetDateT()
{
var d,s,str,num,day;
d = new Date();
mum=d.getDay();
switch (mum) {
case 0:
day = "日";
break;
case 1:
day= "一";
break;
case 2:
day = "二";
break;
case 3:
day = "三";
break;
case 4:
day = "四";
break;
case 5:
day = "五";
break;
case 6:
day = "六";
break;
}
s = d.getFullYear() + " 年 ";
s = s+(d.getMonth() +1) + " 月 ";
s +=d.getDate() + " 日 ";
s =s+" 星期"+day+" ";
s+=d.getHours()+":"+d.getMinutes();
return(s);
}
myDate=GetDateT();
document.write(myDate) ;
</script>
