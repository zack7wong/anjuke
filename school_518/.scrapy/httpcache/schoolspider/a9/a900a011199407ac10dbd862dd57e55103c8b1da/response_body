	//img鼠标移动换图片
	function over(imgObj){
		var imgSrc = imgObj.src;
		imgSrc = imgSrc.replace("-1","-2");
		imgObj.src = imgSrc;
	}

	function out(imgObj){
		var imgSrc = imgObj.src;
		imgSrc = imgSrc.replace("-2","-1");
		imgObj.src = imgSrc;
	}
