                    <select onchange="if(this.options[this.selectedIndex].value!=''){ window.open(this.options[this.selectedIndex].value);this.selectedIndex=0  }">
                        <option>区市级教育网站</option>
                        



                    </select>
                    <select onchange="if(this.options[this.selectedIndex].value!=''){ window.open(this.options[this.selectedIndex].value);this.selectedIndex=0  }">
                        <option>兄弟学校网站</option>
                        



                    </select>


        </div>
        </div>
        </div>    
       <!-- cloumnd E -->       
    

 </div>

