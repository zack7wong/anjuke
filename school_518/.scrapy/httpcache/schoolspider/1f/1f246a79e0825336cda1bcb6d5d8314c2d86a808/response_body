<script type="text/javascript">	
var titles = "";
var imgs = "";
var urls = "";
function addtitle(title){
	if(titles!=""){
		titles+="|";
	}
	titles+=title;
}

function addphotourl(photourl){
	if(imgs!=""){
		imgs+="|";
	}
	imgs+=photourl;
}

function addlink(link){
	if(urls!=""){
		urls+="|";
	}
	urls+=link;
}


addtitle("曹县三桐中学2018届田径运动会");
addphotourl("/news/UploadFiles_9967/201805/2018050620114030.jpg");
addlink("/news/ShowArticle.asp?ArticleID=234");

