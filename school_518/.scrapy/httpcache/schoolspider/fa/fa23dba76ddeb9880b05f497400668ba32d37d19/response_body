 <script>
     function padPreUrl(str) {
         var r = str.indexOf('/', 0);
         var APP_BASE_URL="http://www.cdivtc.com.cn/"
         if (r == 0) {
             str = APP_BASE_URL + str;
         }
         str = str.replace(/(\/$)/g, "");
         return str;
     }

     function MenuActive(ele) {
         var current_url = window.location.href;
         current_url = current_url.replace(/(\/*$)/g, "");
         if (ele.children('li').size() > 0) {
             ele.children('li').each(function (indx, ele2) {
                 if ($(ele2).children('a').size() > 0) {
                     var e2_a = $(ele2).children('a');
                     var href_url = padPreUrl(e2_a.attr('href'));
                     var re = new RegExp(current_url);
                     if (href_url == current_url && current_url != APP_BASE_URL) {
                         e2_a.addClass('on');
                     }
                     if ($(ele2).children('dl').children('dd').size() > 0) {
                         $(ele2).children('dl').children('dd').each(function (idex_3, ele_3) {
                             if ($(ele_3).children('a').size() > 0) {
                                 var e3_a = $(ele_3).children('a');
                                 var href_url = padPreUrl(e3_a.attr('href'));
                                 var re = new RegExp(current_url);
                                 if (href_url == current_url) {
                                     $(ele2).children('a').addClass('on');
                                 }
                             }
                         });
                     }
                 }
             });
         } else {
             return false;
         }
     }
     $(function () {
         MenuActive($('.nav-box ul'));
         /*************************导航*********************************/
         $(".nav .nav-box li").mouseover(function () {
             $(this).find("dl").show();
             $(this).find("a").addClass("current");
         });
         $(".nav .nav-box li").mouseout(function () {
             $(this).find("dl").hide();
             $(this).find("a").removeClass("current");
         });

     });
    </script>

