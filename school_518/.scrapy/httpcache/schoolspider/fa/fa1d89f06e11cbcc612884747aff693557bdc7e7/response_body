        (function ($) {
            $('#nav > li:not(li.line)').each(function () {
                var isDefault = false;
                if ($(" > a", this).hasClass("selected"))
                    isDefault = true;
                $(this).mouseenter(function () {
                    if (!isDefault)
                        $(" > a", this).addClass("selected");
                    $("> .subnav", this).show();
                }).mouseleave(function (e) {
                    if (!isDefault)
                        $(" > a", this).removeClass("selected");
                    $("> .subnav", this).hide();
                });
            });
        })(jQuery);
    </script> 
