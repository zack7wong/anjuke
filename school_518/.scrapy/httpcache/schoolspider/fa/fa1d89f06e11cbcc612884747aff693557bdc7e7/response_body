    <script type="text/javascript">
        function imgscroll() {
            if (marqclone.offsetWidth <= marq.scrollLeft) {
                marq.scrollLeft = marq.scrollLeft - marqorg.offsetWidth;
            }
            else {
                marq.scrollLeft++;
            }
        }

        var martime = "";
        var speed = 20;
        var marq = document.getElementById("marq");
        var marqorg = document.getElementById("marqorg");
        var marqclone = document.getElementById("marqclone");

        if (marq) {
            if (marqorg.getElementsByTagName("img").length > 5) {
                marqclone.innerHTML = marqorg.innerHTML;
                martime = setInterval(imgscroll, speed);
            }

            marq.onmouseover = function () {
                clearInterval(martime);
            }

            marq.onmouseout = function () {
                martime = setInterval(imgscroll, speed);
            };
        }

        $(function () {
            $("#idx_t2_l1_t1 .item").mouseenter(function () {
                $(".sub", this).show();
            }).mouseleave(function () {
                $(".sub", this).hide();
            });
        });
    </script>
