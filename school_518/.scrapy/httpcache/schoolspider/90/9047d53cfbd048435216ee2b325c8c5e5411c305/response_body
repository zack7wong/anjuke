<script type="text/javascript">
function createlist(){
  var pics = $("#piclist div");
  var h = "";
  h+="<tr>";
  h+="<td align=center  onmouseover='showpic(1)' onmouseout='closepic(1)' width=120 height=100 bgcolor=#00a1e1><div id=pic1>" + $(pics[2]).html() + "</div></td>";
  h+="<td align=center  onmouseover='showpic(1)' onmouseout='closepic(1)'><a href='" + $(pics[1]).html() + "' target='_blank'><img border=0 src='" + $(pics[0]).html() + "' width=119  height=100/></a></td>";
  h+="<td align=center  onmouseover='showpic(2)' onmouseout='closepic(2)' width=120 height=100 bgcolor=#77d3ee><div id=pic2>" + $(pics[5]).html() + "</div></td>";
  h+="<td align=center onmouseover='showpic(2)' onmouseout='closepic(2)'><a href='" + $(pics[4]).html() + "' target='_blank'><img border=0 src='" + $(pics[3]).html() + "' width=119  height=100/></a></td>";
  h+="<td align=center  onmouseover='showpic(3)' onmouseout='closepic(3)' width=120 height=100 bgcolor=#fbaa35><div id=pic3>" + $(pics[8]).html() + "</div></td>";
  h+="<td align=center  onmouseover='showpic(3)' onmouseout='closepic(3)'><a href='" + $(pics[7]).html() + "' target='_blank'><img border=0 src='" + $(pics[6]).html() + "' width=119  height=100/></a></td>";
  h+="</tr>";
  h+="<tr>";
  h+="<td align=center  onmouseover='showpic(4)' onmouseout='closepic(4)'><a href='" + $(pics[10]).html() + "' target='_blank'><img border=0 src='" + $(pics[9]).html() + "' width=120 height=100/></a></td>";
  h+="<td align=center  onmouseover='showpic(4)' onmouseout='closepic(4)' width=119 height=100 bgcolor=#fbaa35><div id=pic4>" + $(pics[11]).html() + "</div></td>";
  h+="<td align=center  onmouseover='showpic(5)' onmouseout='closepic(5)'><a href='" + $(pics[13]).html() + "' target='_blank'><img border=0 src='" + $(pics[12]).html() + "' width=120  height=100/></a></td>";
  h+="<td align=center  onmouseover='showpic(5)' onmouseout='closepic(5)' width=119 height=100 bgcolor=#00a1e1><div id=pic5>" + $(pics[14]).html() + "</div></td>";
  h+="<td align=center  onmouseover='showpic(6)' onmouseout='closepic(6)'><a href='" + $(pics[16]).html() + "' target='_blank'><img border=0 src='" + $(pics[15]).html() + "' width=120  height=100/></a></td>";
  h+="<td align=center  onmouseover='showpic(6)' onmouseout='closepic(6)' width=119 height=100 bgcolor=#77d3ee><div id=pic6>" + $(pics[17]).html() + "<div></td>";


  h+="</tr>";
  $('#tablist').html(h);
  $("div[id^=pic]").hide();
  
}
function showpic(id){
  $("#pic"+id).show();
}
function closepic(id){
  $("#pic"+id).hide();
}
