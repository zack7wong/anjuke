<script language="javascript">
function qihuan(a,b,c,d,e,f){
	$("#"+a).mouseover(function(){	
		$(this).removeClass(e);
		$(this).addClass(f);
		$($("#"+b).find("td:not(#"+a+",#no1,#no2,#no3,#no4,#no5)")).each(function(){
 			$(this).removeClass(f);
			$(this).addClass(e);
		});
		$($("#"+c).find("span")).each(function(){
			$(this).addClass("bjhid");
		});
		$($("#"+d).find("span")).each(function(){
			$(this).addClass("bjhid");
		});
		$("#"+a+"_content").removeClass("bjhid");
		$("#"+a+a+"_content").removeClass("bjhid");
	});
}
qihuan("alm1","alm_1","alm_1c","alm_1cc","qhuan2","qhuan1");
qihuan("alm2","alm_1","alm_1c","alm_1cc","qhuan2","qhuan1");

