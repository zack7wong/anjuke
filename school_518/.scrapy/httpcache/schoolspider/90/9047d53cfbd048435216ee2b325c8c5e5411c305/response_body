function qihuan2(a,b,c,e,f){
	$("#"+a).mouseover(function(){	
		$(this).removeClass(e);
		$(this).addClass(f);
		$($("#"+b).find("td:not(#"+a+",#no1,#no2,#no3,#no4,#no5)")).each(function(){
 			$(this).removeClass(f);
			$(this).addClass(e);
		});
		$($("#"+c).find("span")).each(function(){
			$(this).addClass("bjhid");
		});
		$("#"+a+"_content").removeClass("bjhid");
	});
}
qihuan2("alj1","alj_1","alj_1c","qhuan4","qhuan3");
