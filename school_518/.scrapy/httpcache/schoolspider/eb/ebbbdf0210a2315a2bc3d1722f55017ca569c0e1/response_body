            <script>
                var speed=10//速度数值越大速度越慢
                var colee1_left2=document.getElementById("colee1_left2");
                var colee1_left1=document.getElementById("colee1_left1");
                var colee1_left=document.getElementById("colee1_left");
                colee1_left2.innerHTML=colee1_left1.innerHTML
                function Marquee6(){
                    if(colee1_left2.offsetWidth-colee1_left.scrollLeft<=0)//offsetWidth 是对象的可见宽度
                        colee1_left.scrollLeft-=colee1_left1.offsetWidth//scrollWidth 是对象的实际内容的宽，不包边线宽度
                    else{
                        colee1_left.scrollLeft++
                    }
                }
                var MyMar6=setInterval(Marquee6,speed)
                colee1_left.onmouseover=function() {clearInterval(MyMar6)}
                colee1_left.onmouseout=function() {MyMar6=setInterval(Marquee6,speed)}
            </script>
