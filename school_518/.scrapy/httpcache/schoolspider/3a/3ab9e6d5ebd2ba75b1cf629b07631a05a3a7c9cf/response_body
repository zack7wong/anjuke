                  <div id="notice2"></div>
                  <script>
var dir_notice=1//每步移动像素，大＝快
var speed_notice=50//循环周期（毫秒）大＝慢
notice2.innerHTML=notice1.innerHTML
function Marquee_notice(){//正常移动
    //alert(notice2.offsetWidth+"\n"+notice.scrollTop)
    if (dir_notice>0 && (notice2.offsetWidth-notice.scrollTop)<=0) notice.scrollTop=0
    if (dir_notice<0 &&(notice.scrollTop<=0)) notice.scrollTop=notice2.offsetWidth
    notice.scrollTop+=dir_notice
    
    notice.onmouseover=function() {clearInterval(MyMar_notice)}//暂停移动
    notice.onmouseout=function() {MyMar_notice=setInterval(Marquee_notice,speed_notice)}//继续移动
}
function notice_Top(){if (dir_notice=-1)dir_notice=10}//换向左移
function notice_Top_b(){if (dir_notice=-1)dir_notice=1}//换向左移
function notice_Down(){if (dir_notice=1)dir_notice=-10}//换向右移
function notice_Down_b(){if (dir_notice=1)dir_notice=-1}//换向右移
var MyMar_notice=setInterval(Marquee_notice,speed_notice)
            </script>
            </div>
            
        
