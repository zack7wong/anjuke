    function setTabPage(typeId1,typeId2){
        document.getElementById(typeId1+"Div").style.display = "";
        document.getElementById(typeId2+"Div").style.display = "none";
        document.getElementById("tabpageDiv").innerHTML = '<a href="news_list.jsp?typeId='+typeId1+'"><img src="images/more.gif" width="36" height="17" border="0" /></a>';
        //news07Span
        document.getElementById(typeId1+"Span").style.color="#000000";
        document.getElementById(typeId2+"Span").style.color="";
