       //看不清，换图
       function chgImage(obj){
         if(obj.src.indexOf("2.jsp")!=-1)
           obj.src = "/inc/img.jsp";
         else
           obj.src = "/inc/img2.jsp";
       }
       //登录检测
       function checkLoginInfo(){
           if(checkForm(loginForm)==false)return;
           loginForm.submit();
       } 
       //投票
       function addVote(voteId){
    	var selectValue = "";
		var values = document.getElementsByName("selectValue");
		for(var i=0;i<values.length;i++){
			if(values[i].checked){
			    if(selectValue==""){
				  selectValue = values[i].value;
				}else{
				  selectValue += "|" + values[i].value;
				}
			}
		}  
		if(selectValue!=""){ 
			NewsDWR.addNewsVoteStatus(1,"","219.140.122.205","0201",voteId,"0",selectValue,
			    function(data){ 
		            if(data!=null){
		           	    alert(data);  
		            }        
	   		    });
	    }else{
	    	alert("您还没有选择，不能提交！");
	    }        
	}
    </script>
