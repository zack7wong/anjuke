		<script> 
		var x = 50,y = 60 
		var xin = true, yin = true 
		var step = 1 
		var delay = 20 
		var obj=document.getElementById("webAdWave") 
		function webAdWave() { 
			var L=T=0		 
			var R= document.documentElement.clientWidth-obj.offsetWidth 
			var B = document.documentElement.clientHeight-obj.offsetHeight
			
			var maxHeight = window.screen.height-250;
			if(maxHeight<=0)maxHeight = 550;
			
			if(B>maxHeight){B=maxHeight;}						
			 
			obj.style.left = x + document.documentElement.scrollLeft+"px"; 
			obj.style.top = y + document.documentElement.scrollTop+"px"; 
			x = x + step*(xin?1:-1) 
			if (x < L) { xin = true; x = L} 
			if (x > R){ xin = false; x = R} 
			y = y + step*(yin?1:-1) 
			if (y < T) { yin = true; y = T } 
			if (y > B) { yin = false; y = B } 
		} 
		var itl= setInterval("webAdWave()", delay) 
		obj.onmouseover=function(){clearInterval(itl)} 
		obj.onmouseout=function(){itl=setInterval("webAdWave()", delay)} 		
		</script>

