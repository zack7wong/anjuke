             var speed2=10//速度数值越大速度越慢
			  news0103_2.innerHTML=news0103_1.innerHTML
			  function Marquee2(){
				  if(news0103_2.offsetWidth-news0103.scrollLeft<=0){
				 	 news0103.scrollLeft-=news0103_1.offsetWidth
				  }else{
				 	 news0103.scrollLeft++
				  }
			  }
			  var MyMar2=setInterval(Marquee2,speed2)
			  news0103.onmouseover=function() {clearInterval(MyMar2)}
			  news0103.onmouseout=function() {MyMar2=setInterval(Marquee2,speed2)}
