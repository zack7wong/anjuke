    <td width="280" align="left" valign="top" background="/images/l1.jpg" class="R_BORDER"><table width="100%" border="0" cellspacing="0" cellpadding="0" class="TOP2">
      <tr>
        <td width="239" align="center" valign="top"><table width="100%" border="0" cellpadding="0" cellspacing="0" background="/images/an3.jpg">
          <tr>
            <td height="28" align="left" background="/images/an2.jpg" style="background-position:left; background-repeat:no-repeat; font-weight:bold; font-size:14px;"><img src="/images/S.gif" width="15" height="14" style="float:left; margin-right:10px;">学校荣誉 </td>
