function move() 
{ 
if(document.documentElement.scrollTop>170){
    document.getElementById("adv1").style.top=(document.documentElement.scrollTop+10) + "px";
    document.getElementById("adv2").style.top=(document.documentElement.scrollTop+10) + "px";
    document.getElementById("adv3").style.top=(document.documentElement.scrollTop+10) + "px";
}
else{
    document.getElementById("adv1").style.top="170px";
    document.getElementById("adv2").style.top="170px";
    document.getElementById("adv3").style.top="170px";
}
if(document.documentElement.scrollTop>280){
    document.getElementById("adv4").style.top=(document.documentElement.scrollTop+120) + "px";
    document.getElementById("adv5").style.top=(document.documentElement.scrollTop+120) + "px";
}
else{
    document.getElementById("adv4").style.top="280px";
    document.getElementById("adv5").style.top="280px";
}
}
 
function getViewportWidth() {
	if (window.innerWidth!=window.undefined) return window.innerWidth; 
	if (document.compatMode=='CSS1Compat') return document.documentElement.clientWidth; 
	if (document.body) return document.body.clientWidth; 
	return window.undefined; 
}
addEvent(window, "scroll", move);
var x,y,Img1,flag,days,Btn,Link;
        function CgHgt(){
            Img1=document.getElementById("Header_Img_1");
            days=document.getElementById("Days");
            Btn=document.getElementById("Btn");
	 Link=document.getElementById("Link");
            if(y==undefined || y==1){
                y=0;
                x=0;
            }
            else{
                y=1;
                x=-67;
                Img1.style.backgroundPositionY="-67px";
                Img1.style.backgroundImage="url(Images/school/80banana_B.png)";
                Btn.style.backgroundImage="url(Images/school/80bananaup.png)";
                days.style.display="none";
	      Link.style.height="110px";
            }
            days.innerHTML=GetDateDiff(Date(),'2018-04-29');
            clearInterval(flag);
            flag=setInterval(move,2);
        }

        function move(){

            if(y<=0){
                x=x-1;
                Btn.style.top=(x+65)+"px";
                Img1.style.backgroundPositionY=x+"px";
                if(x<=-67){
                    clearInterval(flag);
                    Img1.style.backgroundPositionY="0px";
                    Img1.style.backgroundImage="url(Images/school/80banana_L.png)";
                    Btn.style.backgroundImage="url(Images/school/80bananado.png)";
                    days.style.display="block";
	         Link.style.height="60px";
                }
            }
            else{
                x=x+1;
                Btn.style.top=(x+65)+"px";
                Img1.style.backgroundPositionY=x+"px";
                if(x>=0)
                {
                    clearInterval(flag);
                }
            }
        }
        function GetDateDiff(startDate,endDate)
        {
            var startTime = new Date(Date.parse(startDate.replace(/-/g,   "/"))).getTime();
            var endTime = new Date(Date.parse(endDate.replace(/-/g,   "/"))).getTime();
            var dates = Math.abs((startTime - endTime))/(1000*60*60*24);
            return Math.floor(dates);
        }


