document.getElementsByID = function(sIDValue,oBox) {
  //适用于获取某个HTML区块内部同属于某一特定type的所有HTML元素，如:input,script,link等等
  this.d= oBox || document;
  var children = this.d.getElementsByTagName('*') || document.all;
  var elements = new Array();
  for (var ii = 0; ii < children.length; ii++) {
    if (children[ii].id == sIDValue) {
      elements.push(children[ii]);
    }
  }
  return elements;
}
document.getElementsByClassName = function(className,oBox) {
//适用于获取某个HTML区块内部含有某一特定className的所有HTML元素
  this.d= oBox || document;
  var children = this.d.getElementsByTagName('*') || document.all;
  var elements = new Array();
  for (var ii = 0; ii < children.length; ii++) {
    var child = children[ii];
    var classNames = child.className.split(' ');
    for (var j = 0; j < classNames.length; j++) {
      if (classNames[j] == className) {
        elements.push(child);
        break;
      }
    }
  }
  return elements;
}
function addEvent(obj, evType, fn){
  if (obj.addEventListener){
    obj.addEventListener(evType, fn, false);
    return true;
  } else if (obj.attachEvent){
    var r = obj.attachEvent("on"+evType, fn);
    return r;
  } else {
    return false;
  }
}
  function addfavorite()
  {
  var url = "http://www.xxx.com";
  var title = "重庆清华中学　官方主页x";
    
  if (document.all){  
    try{  
      window.external.addFavorite(url,title);  
    }catch(e){  
      alert( "加入收藏失败，请使用Ctrl+D进行添加" );  
    }  
      
  }else if (window.sidebar){  
    window.sidebar.addPanel(title, url, "");  
   }else{  
    alert( "加入收藏失败，请使用Ctrl+D进行添加" );  
  }  
  }
  function ShowTab(id, a)
  {
    var bar = document.getElementById(id);
    var lis = bar.getElementsByTagName("li");
    for (var i=0; i<lis.length; i++) { 
      lis[i].childNodes[0].className = ""
    }
    lis[a-1].childNodes[0].className = "on";
    bar.id
    document.getElementsByID("b1tab1",bar)[0].style.display = "none";
    document.getElementsByID("b1tab2",bar)[0].style.display = "none";
    document.getElementsByID("b1tab3",bar)[0].style.display = "none";
    document.getElementsByID("b1tab4",bar)[0].style.display = "none";
    document.getElementsByID("b1tab"+a,bar)[0].style.display = "block";
  }
  var ddmenuitem	= 0;

  // open hidden layer
  function mopen(id)
  {	
    // cancel close timer
    mclose();
    // close old layer
    if(ddmenuitem) ddmenuitem.style.visibility = 'hidden';
    // get new layer and show it
    ddmenuitem = document.getElementById(id);
    ddmenuitem.style.visibility = 'visible';
  }
  // close showed layer
  function mclose()
  {
    if(ddmenuitem) ddmenuitem.style.visibility = 'hidden';
  }

  // close layer when click-out
  document.onclick = mclose;
  
  jQuery().ready(function(){
    // simple accordion
    jQuery('#container').accordion({
      header: '.title'
    });
  });
