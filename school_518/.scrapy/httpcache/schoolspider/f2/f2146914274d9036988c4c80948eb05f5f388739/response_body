        function CheckStateYJ() {
            var isSuccuss = $ajax('Chengrui.EDU.CXMS.index.CheckSignState');
            if (isSuccuss == "1") {
                alert("无法获取ipAddress地址,请检查网络设置！");
            }
            else if (isSuccuss == "2") {
                alert("抱歉，内网登陆才可访问阅卷系统！");
            }
            else {
                window.location.href = 'http://10.55.142.188/apms/';
            }
        }
        function CheckStateXJ() {
            var isSuccuss = $ajax('Chengrui.EDU.CXMS.index.CheckSignState');
            if (isSuccuss == "1") {
                alert("无法获取ipAddress地址,请检查网络设置！");
            }
            else if (isSuccuss == "2") {
                alert("抱歉，内网登陆才可访问校图书馆！");
            }
            else {
                window.location.href = 'http://10.55.142.244/';
            }
        }
        function CheckStateBG() {
            var isSuccuss = $ajax('Chengrui.EDU.CXMS.index.CheckSignState');
            if (isSuccuss == "1") {
                alert("无法获取ipAddress地址,请检查网络设置！");
            }
            else if (isSuccuss == "2") {
                alert("抱歉，校园内部网络才可访问行政办公！");
            }
            else {
                window.location.href = 'http://10.55.0.230/';
            }
        } 
