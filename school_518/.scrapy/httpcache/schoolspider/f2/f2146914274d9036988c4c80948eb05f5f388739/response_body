<script type="text/javascript">
    function Search() {
        var paramSearchTarget = document.getElementById("textfield").value == '请输入搜索关健字...' ? '' : document.getElementById("textfield").value;
        var searchTarget = paramSearchTarget.length > 0 ? escape(paramSearchTarget) : "";
        var IsSearch = "true";
        var url = 'http://www.cxms.net/Page/list.aspx?SearchTarget=' + searchTarget + '&IsSearch=' + IsSearch + '&ModuleID=1';
        window.open(url, "", "", "");
    }
</script>
