<script type="text/javascript" language="javascript">
    function getT(Obj) {
        var dtd = document.getElementById(Obj);
        for (var i = 0; i < dtd.length; i++) {
            if (dtd.options[i].selected == true) {
                LinkUrl = dtd.options[i].value;
                dtd.options[0].selected = true;
                break;
            }
        }
        if (LinkUrl.length > 0 && LinkUrl != "-1") {
            window.open(LinkUrl);
        }
    }
</script>
