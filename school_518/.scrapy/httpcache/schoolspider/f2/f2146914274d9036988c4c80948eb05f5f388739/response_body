<script type="text/javascript">
    function selectTag(id, divTagContent, showContent, selfObj) {
        // 操作标签
        var tag = document.getElementById(id).getElementsByTagName("li");
        var taglength = tag.length;
        for (i = 0; i < taglength; i++) {
            tag[i].className = "";
        }
        selfObj.parentNode.className = "selectTag";
        // 操作内容
        var Divs = document.getElementById(divTagContent).getElementsByTagName("div");
        var divLength = Divs.length;
        for (i = 0; i < divLength; i++) {
            var paramDiv = Divs.item(i);
            if (typeof (paramDiv != "undefined")) {
                if (paramDiv.id != '') {
                    paramDiv.style.display = "none";
                }
            }
            else {
                paramDiv = Divs.item[i];
                if (typeof (paramDiv != "undefined")) {
                    if (paramDiv.id != '') {
                        paramDiv.style.display = "none";
                    }
                }
            }
        }
        if (document.getElementById(showContent) != null) {
            document.getElementById(showContent).style.display = "block";
        }
    }
		
</script>
