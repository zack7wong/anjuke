<script type="text/javascript">
myFocus.set({
    id:'box',//焦点图盒子ID
    pattern:'mF_fscreen_tb',//风格应用的名称
    time:10,//切换时间间隔(秒)
    trigger:'click',//触发切换模式:'click'(点击)/'mouseover'(悬停)
    width:980,//设置图片区域宽度(像素)
    height:300,//设置图片区域高度(像素)
    txtHeight:'default'//文字层高度设置(像素),'default'为默认高度，0为隐藏
});
myFocus.set({
    id:'box2',//焦点图盒子ID
    pattern:'mF_slide3D',//风格应用的名称
    time:10,//切换时间间隔(秒)
    trigger:'click',//触发切换模式:'click'(点击)/'mouseover'(悬停)
    width:388,//设置图片区域宽度(像素)
    height:250,//设置图片区域高度(像素)
    txtHeight:'default'//文字层高度设置(像素),'default'为默认高度，0为隐藏
});
</script>
