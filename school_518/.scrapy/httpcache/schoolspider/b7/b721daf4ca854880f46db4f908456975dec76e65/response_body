    <script>
        $(function(){
            //加载页面时执行一次
            changeMargin();
            changeHeight();
            //监听浏览器宽度的改变
            window.onresize = function(){
                changeMargin();
                changeHeight();
                window.location.reload();
            };
            function changeMargin(){
                //获取网页可见区域宽度
                var docWidth = document.body.clientWidth;
                var list=document.getElementsByClassName("dropdown");
                var div=document.getElementById("navbarcontainer");
                //console.info("this"+div.offsetLeft);
                for(var i=0;i<list.length;i++){
                    list[i].onmousemove = function(){

                        var ul=this.getElementsByTagName("ul")[0];
                      
                        ul.style.left=0-this.offsetLeft+"px";
                        ul.style.paddingLeft=div.offsetLeft+"px";

                        var lis=this.getElementsByTagName("li")
                        var  lisLenth=10;

                        setTimeout(function () {
                            for (var i = 0; i < lis.length; i++) {
                                //console.info(i + "" + lis[i].offsetWidth);
                                lisLenth += lis[i].offsetWidth;
                            }
                            setTimeout(function () {
                               ul.style.width = docWidth + 'px';
                            }, 5);
                        }, 5)
                    }
                }
            }
            function changeHeight() {
            //获取内容的高度
            var bodyHeight =document.body.clientHeight;
            
            //获取底部导航的高度
            var bottomHeight = $(".bottom_con").height();
            //获取显示屏的高度
            var iHeight = document.documentElement.clientHeight || document.body.clientHeight;

            if (iHeight > bodyHeight) {
                //$("body").append('<div style="height: '+bottomHeight+'px"></div>');
                // $("body").attr("style", "height:" + (iHeight - bottomHeight) + "px;");
                var hei = iHeight-bodyHeight+bottomHeight-20+"px";
               
                $(".content_bar").css("padding-bottom",hei);
            }
        }
        });
        $(function(){
            var firstChild=$("#inner>:first");//用选择器的方式获取第一个子元素
          //console.info(firstChild.get(0))
            firstChild.addClass("active");
        });
      
    </script>
    <script type="text/javascript">
        $(document).ready(function () {
            var trigger = $('.hamburger'),
                overlay = $('.overlay'),
                isClosed = false;

            trigger.click(function () {
                hamburger_cross();
            });

            function hamburger_cross() {

                if (isClosed == true) {
                    overlay.hide();
                    trigger.removeClass('is-open');
                    trigger.addClass('is-closed');
                    isClosed = false;
                } else {
                    overlay.show();
                    trigger.removeClass('is-closed');
                    trigger.addClass('is-open');
                    isClosed = true;
                }
            }

            $('[data-toggle="offcanvas"]').click(function () {
                $('#wrapper').toggleClass('toggled');
            });
        });

        //$(function () {
          //  $('.sucaihuo').liMarquee();
        //});
        $(document).ready(function(){
            $(document).off('click.bs.dropdown.data-api');
        });
        $(document).ready(function(){
            dropdownOpen();//调用
        });
        $('.carousel').carousel({
            interval: 2000
        });
        /**
         * 鼠标划过就展开子菜单，免得需要点击才能展开
         */
        function dropdownOpen() {

            var $dropdownLi = $('li.dropdown');

            $dropdownLi.mouseover(function() {
                $(this).addClass('open');
            }).mouseout(function() {
                $(this).removeClass('open');
            });
        };
        $(document).ready(function () {
            $('ul.nav > li').hover(function (e) {
                e.preventDefault();
                $('ul.nav > li').removeClass('active');
                $(this).addClass('active');
            });
        });
    </script>
    <script>
        jQuery(function($){
            $('#home-slider').slideBox({
                width: '100%',
                height: '220',
                direction : 'left',//left,top#方向
                duration : 0.3,//滚动持续时间，单位：秒
                easing : 'linear',//swing,linear//滚动特效
                delay : 10,//滚动延迟时间，单位：秒
                hideClickBar:false
            });
        });
        var speed = 20;
        var wrap = document.getElementById("scroll");
        var ul = document.getElementById("scrollUl");
        function Marquee() {
            if ( (ul.offsetWidth - wrap.offsetWidth) >  wrap.scrollLeft ){
                wrap.scrollLeft++;
            }else{
                wrap.scrollLeft=0 ;
            }
        }
        var MyMar = setInterval(Marquee, speed);
        wrap.onmouseover = function () {
            clearInterval(MyMar)
        };
        wrap.onmouseout = function () {
            MyMar = setInterval(Marquee, speed)
        };
    </script>
