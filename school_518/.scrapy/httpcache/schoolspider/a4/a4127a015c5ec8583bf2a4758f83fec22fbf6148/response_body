        <script>
                        //名师巡礼动画
                        var techtag=0;
                        var techshowlength=$('#teachxl li').length;
                        $('#teachxl').width(techshowlength*195 );
                        setInterval(function()
                        {

                            techshow();
                        },3000);

                        function techshow(){
                            if(techtag >= (techshowlength-3))
                            {
                                techtag=0;
                                $("#teachxl").css({left: "0"});
                            }


                            $("#teachxl").animate({left: "-=165"}, 1000  );
                            techtag++;
                        }

                    </script>

                    <div id="teachfc">
                        <a href="http://www.dlhs24.com.cn/a/jiaoshifengcai/">
                            <img src="/uploads/heying.jpg" width="460" height="240"style="margin-top:10px;">
                        </a>
                    </div>

                </div>

                <script>
                    $('.index_title_txt.index_title:eq(0)').css('border-bottom','1px solid #ff9300');

                    $('.index_title_txt.index_title').mouseover(function(){

                        var tindex=$(this).index();

                        if( tindex == 0 )
                        {
                            $("#teachxl").show();
                            $("#teachfc").hide();
                        }
                        else{
                            $("#teachxl").hide();
                            $("#teachfc").show();
                        }

                        $('.index_title_txt.index_title').css('border-bottom','1px solid #d8d8d8');
                        $(this).css('border-bottom','1px solid #ff9300');

                    });
                </script>

            </div>
