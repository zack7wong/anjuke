            <script>
                var listImg = '';
                var listHref ='';
                var listImgArr = new Array();
                var listHrefArr =new Array();
                for(i =0;i<$('#flashbox ul li a').length;i++){
                    listImg = $('#flashbox ul li').eq(i).find('a img').attr('alt');
                    listImgArr.push(listImg);
                    listHref = $('#flashbox ul li').eq(i).find('a').attr('href');
                    listHrefArr.push(listHref);
                    $('.index_desc').append('<div class="index_desc_title"><a href='+listHrefArr[i]+' id=index'+i+' target="_blank">'+listImgArr[i]+'</a></div>');

                }

                $('.index_title_txt_a').mouseover(function(){
                    $('.index_title_txt_a:eq(0)').css('border-bottom','1px solid #ff9300');
                    $('.index_title_txt_a').css('border-bottom','1px solid #fff');
                    $(this).css('border-bottom','1px solid #ff9300');

                });

            </script>

            <script>
                //这里是广告播放器
                var adtag=0;
                $('.ad24').hide();
                $('.ad24:eq(0)').show();
                var adlength=$('.ad24').length;

                setInterval(function(){
                    adtag++;
                    adtag=adtag%adlength;
                    $('.ad24').hide();
                    $('.ad24:eq('+adtag+')').show();
                },4000);
            </script>

