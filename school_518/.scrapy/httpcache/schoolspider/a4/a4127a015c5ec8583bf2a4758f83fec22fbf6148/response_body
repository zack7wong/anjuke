            //这里是广告播放器
            var adtag=0;
            $('.ad24').hide();
            $('.ad24:eq(0)').show();
            var adlength=$('.ad24').length;

            setInterval(function(){
                adtag++;
                adtag=adtag%adlength;
                $('.ad24').hide();
                $('.ad24:eq('+adtag+')').show();
            },4000);
        </script>

