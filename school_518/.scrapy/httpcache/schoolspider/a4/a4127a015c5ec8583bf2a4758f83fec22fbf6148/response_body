            <script type="text/javascript">
                var i=0;
                var a_length =$('#homeUserContainer').find("a").length;
                var move_left=222;
                $(".homeUserBefore").click(function(){
                   var tmp = $('#homeUserContainer').css("left");
                   var tmp2 = parseInt(tmp.substr(0, tmp.length - 2));

                   var tmp3 = tmp2 + move_left;

                   if (tmp3 <= 0 && i>0) {
                     i--;
                     $('#homeUserContainer').css("left",tmp3 + "px");
                 }
             });
                $(".homeUserAfter").click(function(){
                   var tmp = $('#homeUserContainer').css("left");
                   var tmp2 = parseInt(tmp.substr(0, tmp.length - 2));
                   var tmp3 = tmp2 - move_left;
                   if(tmp3 <= -move_left && i< a_length-5){
                     i++;
                     $("#homeUserContainer").css("left",tmp3 + "px");
                 }
             });
         </script>

     </div>
