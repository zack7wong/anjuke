iDays=parseInt(Math.abs(oDate1-oDate2)/1000/60/60/24); //把相差的毫秒数转换为天数 
if(iDays <= 1 )
{
        var img='<font color="#ff0000">04-10</font>'
		document.write(img)
}
else
{
	 var img='04-10'
		document.write(img)
}
}
var date,date1,date2;
date=new Date();  //当前时间
date1="18-04-10"; //计算时间
date2=date.getFullYear()+"-"+(date.getMonth()+1)+"-"+date.getDate();

DateDiff(date1,date2)

</script></td>
