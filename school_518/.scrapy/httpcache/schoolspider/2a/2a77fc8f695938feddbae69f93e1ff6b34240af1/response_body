<script language="javascript" type="text/javascript">



function GetUserInfo(xml){
 
    var userInfo = " <table width=\"100%\" border=\"0\" cellspacing=\"0\" cellpadding=\"3\"><tr><td>" + xml.getElementsByTagName("loginname")[0].firstChild.data + "，您好！</td></tr><tr><td>";
    userInfo = userInfo + "登录次数：" + xml.getElementsByTagName("logins")[0].firstChild.data + " 次;";
	userInfo = userInfo + "待审内容"+xml.getElementsByTagName("allnocheck")[0].firstChild.data + " 条;";
    if(xml.getElementsByTagName("allsign")[0].firstChild.data=="0"){
//userInfo = userInfo + "待签收内容"+xml.getElementsByTagName("allsign")[0].firstChild.data + " 条";
}else{
userInfo = userInfo + "<a href=/cms/document_signmanage.aspx target=_blank><font>待签收内容"+xml.getElementsByTagName("allsign")[0].firstChild.data + " 条</font></a>";
}
    userInfo = userInfo + "</td></tr><tr><td style=\"text-align: center\"><a href=\"/oa/\" target=_blank linkOpenType=\"_top\"><font>办公平台</font></a>&nbsp;&nbsp;|&nbsp;&nbsp;";
    userInfo = userInfo + "<a href=\"#\"  onclick=\"loginout()\" linkOpenType=\"_top\"><font>退出登录</font></a></td>";
    userInfo = userInfo + "</tr></table>";
    return userInfo;
}
</script>




<!--  以下登录代码不要修改  -->
<!--  以下登录代码不要修改  -->
<script language="javascript" type="text/javascript">
refreshwindow();
function Loginin(LoginString){
    document.getElementById("LoginFrom").innerHTML = document.getElementById("LoginFrom").innerHTML + LoginString;
}

function CheckIsLogin(){
	
    //document.getElementById("PnlLogOnStatus").innerHTML = "<img src=\"/cms/Images/loading.gif\" alt=\"\"/>";
    var x = new AjaxRequest('XML', 'status');
    x.para = [];
	
    x.post('logincheck', '/cms/Ajax.aspx', function(s){
	
        var xml = x.createXmlDom(s);
        var status = xml.getElementsByTagName("status")[0].firstChild.data;
        
        if (status == 'ok') {
            //document.getElementById("PnlLogOnStatus").style.display = "";
            //document.getElementById("LoginFrom").style.display = "none";
            //document.getElementById("PnlLogOnStatus").innerHTML = GetUserInfo(xml);
            document.getElementById("tablelogin").innerHTML=GetUserInfo(xml);
        }        else {
            location.reload();
        }
    });
}
function refreshwindow(){
    //document.getElementById("PnlLogOnStatus").innerHTML = "<img src=\"/cms/Images/loading.gif\" alt=\"\"/>";
    var x = new AjaxRequest('XML', 'status');
    x.para = [];
    x.post('logincheck', '/cms/Ajax.aspx', function(s){
        var xml = x.createXmlDom(s);
        var status = xml.getElementsByTagName("status")[0].firstChild.data;
        
        if (status == 'ok') {
            //document.getElementById("PnlLogOnStatus").style.display = "";
            //document.getElementById("LoginFrom").style.display = "none";
            //document.getElementById("PnlLogOnStatus").innerHTML = GetUserInfo(xml);
            document.getElementById("tablelogin").innerHTML=GetUserInfo(xml);
        }

    });
}
function loginout(){
 
    var x = new AjaxRequest('XML','');
    x.post('userlogout', '/cms/Ajax.aspx', function(s) {
    var xml = x.createXmlDom(s);
        var status = xml.getElementsByTagName("status")[0].firstChild.data;
        if (status == "ok") {
            location.reload();
         }
    });
}
function CheckEnableValidCode(){
    var x = new AjaxRequest('XML', 'status');
    x.para = [];
    x.post('EnableValidCode', '/cms/Ajax.aspx', function(s){
        var xml = x.createXmlDom(s);
        var status = xml.getElementsByTagName("status")[0].firstChild.data;
        if (status == "yes") {
            document.getElementById("checkcode").style.display = '';
            document.getElementById("TxtValdisplayDateCode").value = '';
        }
    });
}
function Login(){
    if (document.getElementById("loginname").value == '') {
        alert("请填写账号！");
        document.getElementById("loginname").focus();
        return;
    }
	 
    if (document.getElementById("password").value == '') {
        alert("请填写密码！");
        document.getElementById("password").focus();
        return;
    }
	
    var x = new AjaxRequest('XML', 'status');
    x.para = ['loginname=' + document.getElementById("loginname").value, 'password=' + document.getElementById("password").value, 'ddlFlag=2'];
    x.post('userlogin', '/cms/Ajax.aspx', function(s){
        var xml = x.createXmlDom(s);
        var status = xml.getElementsByTagName("status")[0].firstChild.data;
        switch (status) {
            case "ok":
			
                CheckIsLogin();
				
                break;
            case "err":
                var errorMessage = xml.getElementsByTagName("body")[0].firstChild.data;
                alert(errorMessage);
                break;
        }
    });
}
</script></td>
              </tr>
