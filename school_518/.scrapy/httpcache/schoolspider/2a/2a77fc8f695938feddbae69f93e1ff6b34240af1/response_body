var aDate,oDate1,oDate2,iDays;
aDate=sDate1.split("-");
oDate1=new Date(aDate[1]+'-'+aDate[2]+'-'+aDate[0]);//转换为月-日-年格式 
aDate=sDate2.split("-");
oDate2=new Date(aDate[1] + '-'+aDate[2]+'-'+aDate[0]);
