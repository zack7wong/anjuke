var speed=30
demo20.innerHTML=demo10.innerHTML
function Marquee1(){
if(demo20.offsetWidth-demo0.scrollLeft<=0)
demo0.scrollLeft-=demo10.offsetWidth
else{
demo0.scrollLeft++
}
}
var MyMar1=setInterval(Marquee1,speed)
