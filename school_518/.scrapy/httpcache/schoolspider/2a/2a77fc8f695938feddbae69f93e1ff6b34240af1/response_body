<script language="javascript">
function qihuan(a,b,c,d,e,f){
	jQuery("#"+a).mouseover(function(){	
		jQuery(this).removeClass(e);
		jQuery(this).addClass(f);
		jQuery(jQuery("#"+b).find("td:not(#"+a+",#no1,#no2,#no3,#no4,#no5)")).each(function(){
 			jQuery(this).removeClass(f);
			jQuery(this).addClass(e);
		});
		jQuery(jQuery("#"+c).find("span")).each(function(){
			jQuery(this).addClass("bjhid");
		});
		jQuery(jQuery("#"+d).find("span")).each(function(){
			jQuery(this).addClass("bjhid");
		});
		jQuery("#"+a+"_content").removeClass("bjhid");
		jQuery("#"+a+a+"_content").removeClass("bjhid");
	});
}
qihuan("new1","new_1","new_1c","new_1cc","qhuan2","qhuan1");
qihuan("new2","new_1","new_1c","new_1cc","qhuan2","qhuan1");
qihuan("new3","new_1","new_1c","new_1cc","qhuan2","qhuan1");
qihuan("new4","new_1","new_1c","new_1cc","qhuan2","qhuan1");


qihuan("new5","new_2","new_2c","new_2cc","qhuan2","qhuan1");
qihuan("new6","new_2","new_2c","new_2cc","qhuan2","qhuan1");
qihuan("new7","new_2","new_2c","new_2cc","qhuan2","qhuan1");
qihuan("new8","new_2","new_2c","new_2cc","qhuan2","qhuan1");
qihuan("new9","new_2","new_2c","new_2cc","qhuan2","qhuan1");
</script>

