jQuery(".slider .bd li").first().before(jQuery(".slider .bd li").last());
    jQuery(".slider").hover(function() {
        jQuery(this).find(".arrow").stop(true, true).fadeIn(300)
		$(".txt").fadeIn(2000);
    }, function() {
        jQuery(this).find(".arrow").fadeOut(300)
    });
    jQuery(".slider").slide({
        //titCell: ".hd ul",
        mainCell: ".bd ul",
        effect: "leftLoop",
        autoPlay: true,
        vis: 3,
        autoPage: true,
		interTime:5000,
        trigger: "click",
	    startFun: function(i) {
	        jQuery(".slider .txt li").eq(i).animate({
	            "bottom": 0
	            // "display": ''
	        },2000).siblings().animate({
	            "bottom":-1336
	            // "display": 'none'
	        });
	    }
			
    });
jQuery(".focusBox").slide({
    titCell: ".num li",
    mainCell: ".pic",
    effect: "fold",
    autoPlay: true,
	interTime:5000,
    trigger: "click",
    startFun: function(i) {
        jQuery(".focusBox .txt li").eq(i).animate({
            "bottom": 0
        }).siblings().animate({
            "bottom": -36
        });
    }
});
