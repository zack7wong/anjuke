<script>
var speed21=60
var scroll_end11 = document.getElementById("scroll_end11");
var scroll_div21 = document.getElementById("scroll_div21");
scroll_end11.innerHTML=scroll_begin31.innerHTML
function Marquee21(){
if(scroll_end11.offsetWidth-scroll_div21.scrollLeft<=0)
scroll_div21.scrollLeft-=scroll_begin31.offsetWidth
else{
scroll_div21.scrollLeft++
}
}
var MyMar21=setInterval(Marquee21,speed21)
scroll_div21.onmouseover=function() {clearInterval(MyMar21)}
scroll_div21.onmouseout=function() {MyMar21=setInterval(Marquee21,speed21)}
</script>
